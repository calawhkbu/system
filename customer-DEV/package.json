{
  "name": "customer-DEV",
  "description": "This the custom config for DEV",
  "devDependencies": {
    "@nestjs/common": "^7.1.3",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "axios": "^0.19.2",
    "chalk": "2.4.2",
    "eslint": "^7.2.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsdoc": "^27.0.7",
    "eslint-plugin-prefer-arrow": "^1.2.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "node-jql": "^2.0.0-rc.86",
    "prettier": "^1.18.2",
    "sequelize": "^5.21.12",
    "typescript": "^3.7.5"
  },
  "scripts": {
    "lint": "eslint -c .eslintrc.js --ext .ts ./",
    "preprepare": "mkdir node_modules",
    "prepare": "yarn prepare:own;yarn prepare:thirdParty",
    "prepare:own": "yarn prepare:classes;yarn prepare:modules;yarn prepare:models;yarn prepare:utils",
    "prepare:classes": "ln -s ../../../swivel-backend-new/src/classes node_modules/classes",
    "prepare:modules": "ln -s ../../../swivel-backend-new/src/modules node_modules/modules",
    "prepare:models": "ln -s ../../../swivel-backend-new/src/models node_modules/models",
    "prepare:utils": "ln -s ../../../swivel-backend-new/src/utils node_modules/utils",
    "prepare:thirdParty": "yarn prepare:nest;yarn prepare:types;yarn prepare:axios;yarn prepare:chalk;yarn prepare:lodash;yarn prepare:moment;yarn prepare:node-jql;yarn prepare:sequelize;yarn prepare:typescript;yarn prepare:eslint;yarn prepare:eslint-plugin-import;yarn prepare:eslint-plugin-jsdoc;yarn prepare:eslint-plugin-prefer-arrow;",
    "prepare:nest": "ln -s ../../../swivel-backend-new/node_modules/@nestjs node_modules/@nestjs",
    "prepare:types": "ln -s ../../../swivel-backend-new/node_modules/@types node_modules/@types",
    "prepare:axios": "ln -s ../../../swivel-backend-new/node_modules/axios node_modules/axios",
    "prepare:chalk": "ln -s ../../../swivel-backend-new/node_modules/chalk node_modules/chalk",
    "prepare:lodash": "ln -s ../../../swivel-backend-new/node_modules/lodash node_modules/lodash",
    "prepare:moment": "ln -s ../../../swivel-backend-new/node_modules/moment node_modules/moment",
    "prepare:node-jql": "ln -s ../../../swivel-backend-new/node_modules/node-jql node_modules/node-jql",
    "prepare:sequelize": "ln -s ../../../swivel-backend-new/node_modules/sequelize node_modules/sequelize",
    "prepare:typescript": "ln -s ../../../swivel-backend-new/node_modules/typescript node_modules/typescript",
    "prepare:eslint": "ln -s ../../../swivel-backend-new/node_modules/eslint node_modules/eslint",
    "prepare:eslint-plugin-import": "ln -s ../../../swivel-backend-new/node_modules/eslint-plugin-import node_modules/eslint-plugin-import",
    "prepare:eslint-plugin-jsdoc": "ln -s ../../../swivel-backend-new/node_modules/typescript node_modules/eslint-plugin-jsdoc",
    "prepare:eslint-plugin-prefer-arrow": "ln -s ../../../swivel-backend-new/node_modules/typescript node_modules/eslint-plugin-prefer-arrow",
    "export": "node export"
  }
}