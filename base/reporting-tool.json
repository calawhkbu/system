  "map": {
    "url": "reports/uber",
    "configurations": {
      "map": {
        "id": "ClusterMapView",
        "props": {
          "center": [0, 0],
          "doubleClickZoom": false,
          "dragging": true,
          "minZoom": 2,
          "scrollWheelZoom": true,
          "zoom": 2,
          "zoomControl": false
        },
        "routeProps": {
          "startPointOptions": {
            "weight": 1.5,
            "radius": 13,
            "color": "#FFF3E0"
          },
          "startPointIconOptions": {
            "iconUrl": "/images/factory.png",
            "iconSize": [15, 15]
          },
          "middlePointOptions": {
            "weight": 1.5,
            "radius": 13,
            "color": "#FF9800"
          },
          "middlePointIconOptions": {
            "iconUrl": "/images/map/SEA.svg",
            "iconSize": [15, 15]
          },
          "endPointOptions": {
            "weight": 1.5,
            "radius": 13,
            "color": "#E65100"
          },
          "endPointIconOptions": {
            "iconUrl": "/images/destination.svg",
            "iconSize": [15, 15]
          },
          "travelledRouteStyle": {

          },
          "notTravelRouteStyle": {
            "dashArray": "5,5",
            "dashOffset": "0"
          },
          "options": {
            "weight": 1,
            "color": "blue"
          },
          "textOptions": {
            "offset": 10,
            "attributes": {
              "color": "blue",
              "font-weight": "bold",
              "font-size": 30
            }
          },
          "routeStyle": {
            "color": "blue",
            "vertices": 300,
            "offset": 10
          }
        },
        "layers": [
          {
            "id": 1,
            "component": "TileLayer",
            "url": "https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png"
          },
          {
            "id": 2,
            "component": "ZoomControl",
            "position": "bottomleft"
          },
          {
            "id": 3,
            "component": "ClusteringLayer",
            "url": "/reports/{entity}-details",
            "countUrl": "/reports/{entity}-details/count",
            "maxCount": 50000,
            "params": {
              "fields": ["id"],
              "filter": {}
            },
            "click": {
              "args": {
                "url": "{current}/details",
                "process": {
                  "_plugins": [
                    "from-string"
                  ],
                  "from-string": "{{\"{key}\":{{\"{key}\":[\"{value}\"]}}}}"
                },
                "cardProcess": {
                  "_plugins": [
                    "selection-by-row"
                  ],
                  "selection-by-row": {
                    "key": "entity",
                    "cards": {
                      "shipment": 54,
                      "booking": 56
                    }
                  }
                }
              }
            },
            "config": {
              "id": "masterNo",
              "searchPolNameKey": "portOfLoading",
              "searchPodNameKey": "portOfDischarge",
              "getPolNameKey": "pol",
              "getPodNameKey": "pod",
              "getPodCoordKey": "podCoord",
              "getPolCoordKey": "polCoord",
              "cluster": {
                "singleMarkerMode": true,
                "showCoverageOnHover": true,
                "maxClusterRadius": 80,
                "chunkedLoading": true,
                "removeOutsideVisibleBounds": true,
                "spiderfyDistanceMultiplier": 0.00000000001
              }
            }
          }
        ]
      }
    },
    "filters": {
      "estimatedDepartureDate": {
        "value": {
          "to": "2018-06-30",
          "from": "2018-06-01"
        },
        "summary": "2018/06/01 - 2018/06/30"
      }
    },
    "mapControl": {
      "pod": true
    }
  }
