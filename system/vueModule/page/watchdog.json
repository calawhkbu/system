        "configurations": {
          "i18nContext": "Alert",
          "colors": [
            "#005EB8",
            "#008675",
            "#E35205",
            "#AF272F",
            "#F1B434",
            "#c9cbcf"
          ]
        }
      }
    }
  }]
}