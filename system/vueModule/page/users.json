{
  "fillParent": true,
  "components": [{
    "is": "TableTool",
    "props": {
      "title": "Users",
      "back": "/admin",
      "urlFormat": "/admin/users",
      "reportingKey": "users",
      "reportingKeyKey": "reportIdParams",
      "jql": "users",
      "hasReports": false,
      "showFilterBar": true,
      "enableShare": false,
      "enableSetting": false,
      "noPartyGroupFilters": true,
      "nextPageComponent": "user",
      "noCache": true,
      "refreshWhenBack": true,
      "clickFunction": {
        "type": "router",
        "otherParams": {
          "func": "push",
          "path": "/detail/{{id}}",
          "base": true
        }
      },
      "newReport": {
        "filters": {
          "invitationStatus": {
            "value": ["sent", "accepted", "pending"]
          },
          "activeStatus": {
            "value": "active",
            "locked": true
          }
        },
        "configurations": {
          "i18nContext": "InvitationPage",
          "clickable": true,
          "sorting": {
            "direction": "DESC",
            "key": "updatedAt",
            "sql": {
              "classname": "ColumnExpression",
              "table": "person",
              "name": "updatedAt"
            }
          },
          "pagination": {
            "current": 0,
            "perPage": 20,
            "remote": true
          }
        }
      },
      "fieldOptions": [{
          "key": "id",
          "hidden": true
        },
        {
          "key": "updatedAt",
          "width": 150,
          "format": "YYYY-MM-DD"
        },
        {
          "label": "status",
          "key": "invitationStatus",
          "width": 150
        },
        {
          "label": "",
          "key": "canResend",
          "sortable": false,
          "align": "center",
          "clickFunction": {
            "type": "axios",
            "otherParams": {
              "axiosParams": {
                "url": "/api/invitation/resend/{{row.id}}",
                "method": "GET"
              }
            },
            "afterActions": [{
              "type": "notify",
              "otherParams": {
                "notification": "Notify.resendSuccess"
              }
            }]
          },
          "tdComponent": {
            "is": "icon",
            "hideIfFalse": true,
            "props": {
              "icon": "email"
            }
          },
          "width": 50
        },
        {
          "label": "",
          "key": "canDelete",
          "sortable": false,
          "align": "center",
          "clickFunction": {
            "type": "axios",
            "otherParams": {
              "axiosParams": {
                "url": "/api/invitation/delete/{{row.id}}",
                "method": "DELETE"
              }
            },
            "afterActions": [{
              "type": "notify",
              "otherParams": {
                "notification": "Notify.deleteSuccess"
              }
            }]
          },

          "tdComponent": {
            "is": "icon",
            "hideIfFalse": true,
            "props": {
              "icon": "delete"
            }
          },
          "width": 50
        },
        {
          "label": "",
          "key": "canRestore",
          "sortable": false,
          "align": "center",
          "clickFunction": {
            "type": "axios",
            "otherParams": {
              "axiosParams": {
                "url": "/api/invitation/restore/{{row.id}}",
                "method": "GET"
              }
            },
            "afterActions": [{
              "type": "notify",
              "otherParams": {
                "notification": "Notify.restoreSuccess"
              }
            }]
          },

          "tdComponent": {
            "is": "icon",
            "hideIfFalse": true,
            "props": {
              "icon": "refresh"
            }
          },
          "width": 50
        },
        {
          "key": "userName"
        },
        {
          "key": "firstName"
        },
        {
          "key": "lastName"
        },
        {
          "key": "displayName"
        }
      ],
      "filters": [

        {
          "name": "activeStatus",
          "type": "list",
          "props": {
            "items": [{
                "label": "active",
                "value": "active"
              },
              {
                "label": "deleted",
                "value": "deleted"
              }
            ]
          }
        }, {
          "display": "status",
          "name": "invitationStatus",
          "i18nContext": "InvitationPage",
          "props": {
            "multi": true,
            "items": [{
                "value": "sent",
                "label": "sent"
              },
              {
                "value": "accepted",
                "label": "accepted"
              },
              {
                "value": "pending",
                "label": "pending"
              },
              {
                "value": "disabled",
                "label": "disabled"
              }
            ]
          },
          "type": "list"
        },
        {

          "i18nContext": "InvitationPage",
          "display": "email",
          "name": "userNameLike",
          "type": "text"
        },
        {

          "i18nContext": "InvitationPage",
          "display": "name",
          "name": "nameLike",
          "type": "text"
        }, {
          "i18nContext": "InvitationPage",
          "name": "role",
          "type": "autocomplete",
          "props": {
              "api": {
                "query": {
                  "data": {
                    "limit": 5,
                    "fields": [
                      "id", "roleName"
                    ],
                    "subqueries": {
                      "q": {
                        "value": "{{ q }}"
                      }
                    }
                  },
                  "labelKey": "roleName",
                  "method": "POST",
                  "url": "api/role/query/role",
                  "valueKey": "id"
                },
                "resolve": {
                  "data": {
                    "fields": ["id", "roleName"],
                    "subqueries": {
                      "roleName": {
                        "value": "{{value}}"
                      }
                    }
                  },
                  "labelKey": "roleName",
                  "method": "POST",
                  "url": "api/code/query/code_master",
                  "valueKey": "id"
                }
              },
              "multi": true
          }
        }
      ]
    },
    "slots": [{
      "name": "middle-button",
      "is": "DynamicComponent",
      "props": {
        "class": "button",
        "components": [{
          "is": "v-btn",
          "props": {
            "small": true,
            "flat": true,
            "icon": true
          },
          "slots": [{
            "is": "Icon",
            "props": {
              "icon": "add"
            }
          }],
          "events": {
            "click.stop": [{
              "type": "router",
              "otherParams": {
                "func": "push",
                "path": "user/create",
                "base": true
              }
            }]
          }
        }]
      }
    }]
  }]
}
