{
  "layout": "v-card",
  "components": [{
    "is": "WidgetForm",
    "props": {
      "fields": [{
        "label": "ProductCategoryPage.productCategoryCode",
        "name": "productCategoryCode",
        "component": "v-text-field"
      }, {
        "label": "ProductCategoryPage.name",
        "name": "name",
        "component": "v-text-field"
      }, {
        "label": "ProductCategoryPage.description",
        "name": "description",
        "component": "v-text-field"
      }, {
        "label": "ProductCategoryPage.buyerParty",
        "name": "buyerParty",
        "component": "AsyncAutoSuggest",
        "class": "xs-12",
        "more": [
          {
            "name": "buyerPartyId",
            "selector": "id"
          }
        ],
        "props": {
          "showValue": false,
          "canCreate": true,
          "canCreateValue": {
            "id": null,
            "name": "{{autoSuggestModel}}"
          },
          "searchValueParams": {
            "url": "api/party/{{context.autoSuggestModel}}"
          },
          "searchTextParams": {
            "url": "api/party/query/party_auto_suggest",
            "method": "POST",
            "data": {
              "subqueries": {
                "activeStatus": {
                  "value": "active"
                },
                "q": {
                  "value": "{{context.search}}"
                },
                "partyTypeIn": {
                  "value": ["buyer"]
                }
              },
              "limit": 5
            }
          },
          "item-text": "name",
          "item-value": "id"
        }
      },
      {
        "component": "I18nText",
        "props": {
          "i18nContext": "ProductCategoryPage",
          "i18nKey": "fixedFieldConfig"
        }
      },
      {
        "component": "WidgetMoreForm",
        "class": "full-width",
        "props": { 
          "mainKey": "ColDefinition",
          "fixedKeys": [
            "itemKey",
            "skuCode",
            "skuName",
            "htsCode",
            "quantity",
            "quantityUnit",
            "bookedQuantity"
          ],
          "formProps": {
            "z": {
              "class": "xs-12 md-6 padding-leftright-4"
            }
          },
          "labelProps": {
            "z": {
              "isComponent": "v-label",
              "i18nContext": "PurchaseOrderPage",
              "class": "font-110-important"
            }
          },
          "canAdd": false,
          "fields": [{
            "label": "ProductCategoryPage.width",
            "name": "flexData.fixColDefinition.{{key}}Width",
            "component": "NumberField",
            "class": "xs-6",
            "props": {
              "type": "number",
              "min": 0
            }
          },{
            "label": "ProductCategoryPage.sequence",
            "name": "flexData.fixColDefinition.{{key}}Order",
            "component": "NumberField",
            "class": "xs-6",
            "props": {
              "type": "number",
              "min": 0
            }
          }]
        }
      },
      {
        "label": "ProductCategoryPage.definition",
        "name": "definition",
        "component": "TableField",
        "props": {
          "topLabel": true,
          "fields": [{
            "label": "ProductCategoryPage.fieldName",
            "name": "fieldName",
            "component": "v-text-field"
          },{
            "label": "ProductCategoryPage.type",
            "name": "type",
            "component": "v-select",
            "props": {
              "items": [{
                "text": "Text",
                "value": "string"
              },{
                "text": "Number",
                "value": "number"
              },{
                "text": "Date",
                "value": "date"
              }]
            }
          },{
            "label": "ProductCategoryPage.displayAsGroupOrCol",
            "name": "isGroup",
            "component": "v-select",
            "props": {
              "items":[{
                "text": "Group",
                "value": true
              },{
                "text": "Column",
                "value": false
              }]
            }
          },{
            "label": "ProductCategoryPage.toBooking",
            "name": "toBooking",
            "component": "BooleanField",
            "colGroupProps": {
              "style": {
                "width": "10%"
              }
            }
          },{
            "label": "ProductCategoryPage.width",
            "name": "width",
            "component": "NumberField",
            "colGroupProps": {
              "style": {
                "width": "10%"
              }
            },
            "props": {
              "type": "number",
              "min": 0
            }
          },{
            "label": "ProductCategoryPage.sequence",
            "name": "order",
            "component": "NumberField",
            "colGroupProps": {
              "style": {
                "width": "10%"
              }
            },
            "props": {
              "type": "number",
              "min": 0
            }
          }]
        }
      }]
    }
  }]
}
