{
  "layout": "v-card",
  "class": "min-height-235",
  "components": [{
    "is": "MouseHidden",
    "props": {
      "hiddenProps": {
        "style": {
          "position": "absolute",
          "right": "0px",
          "top": "0px"
        }
      }
    },
    "slots": [{
      "name": "hidden",
      "is": "DynamicComponent",
      "props": {
        "components": [{
          "is": "RightDrawer",
          "props": {
            "hide-overlay": true,
            "buttonComponentProps": {
              "icon": true,
              "small": true,
              "class": "zindex-3"
            },
            "closeFunction": [{
              "type": "dispatch",
              "otherParams": {
                "name": "widget/changeEdit",
                "data": {
                  "id": "shipmentDate",
                  "isEdit": false
                }
              }
            }]
          },
          "slots": [{
            "name": "button",
            "is": "Icon",
            "props": {
              "icon": "open_in_new"
            }
          }, {
            "name": "default",
            "is": "DynamicComponent",
            "props": {
              "class": "full-width padding-bottom-100",
              "components": [{
                "is": "WidgetMoreForm",
                "props": {
                  "formId": "shipmentDate",
                  "name": "shipmentDate",
                  "mainKey": "Date",
                  "formProps": {
                    "z": {
                      "class": "xs-12 padding-leftright-4"
                    }
                  },
                  "labelProps": {
                    "z": {
                      "isComponent": "v-label",
                      "i18nContext": "Widget",
                      "class": "font-110-important"
                    }
                  },
                  "fixedKeys": [
                    "oceanBill",
                    "cargoReady",
                    "scheduleAssigned",
                    "scheduleApproaved",
                    "spaceConfirmation",
                    "bookingSubmit",
                    "cyCutOff",
                    "documentCutOff",
                    "sentToShipper",
                    "pickup",
                    "shipperLoad",
                    "returnLoad",
                    "gateIn",
                    "cargoReceipt",
                    "shipperDocumentSubmit",
                    "shipperInstructionSubmit",
                    "houseBillDraftSubmit",
                    "houseBillConfirmation",
                    "masterBillReleased",
                    "preAlertSend",
                    "ediSend",
                    "cargoRolloverStatus",
                    "loadOnboard",
                    "departure",
                    "arrival",
                    "inboundTransfer",
                    "onRail",
                    "arrivalAtDepot",
                    "availableForPickup",
                    "pickupCargoBeforeDemurrage",
                    "finalCargo",
                    "cargoPickupWithDemurrage",
                    "sentToConsignee",
                    "finalDoorDelivery",
                    "returnEmptyContainer"
                  ],
                  "checkDisplayWhen": [
                    "if (!['oceanBill','cargoReady','scheduleAssigned','scheduleApproaved','spaceConfirmation','bookingSubmit','cyCutOff','documentCutOff','sentToShipper','pickup','shipperLoad','returnLoad','gateIn','cargoReceipt','shipperDocumentSubmit','shipperInstructionSubmit','houseBillDraftSubmit','houseBillConfirmation','masterBillReleased','preAlertSend','ediSend','cargoRolloverStatus','loadOnboard','departure','arrival','inboundTransfer','onRail','arrivalAtDepot','availableForPickup','pickupCargoBeforeDemurrage','finalCargo','cargoPickupWithDemurrage','sentToConsignee','finalDoorDelivery','returnEmptyContainer'].includes(key)) { return false }",
                    "const estimated = context._.get(form, `${key}DateEstimated`, null);",
                    "const actual = context._.get(form, `${key}DateActual`, null);",
                    "return estimated || actual"
                  ],
                  "canAdd": true,
                  "canAddProps": {
                    "class": "xs-12 md-6 padding-20 min-height-200 margin-auto"
                  },
                  "fields": [{
                      "label": "Widget.estimatedDate",
                      "name": "shipmentDate.{{flex}}{{key}}DateEstimated",
                      "component": "DateTimePicker",
                      "class": "xs-{% if flex === '' %}6{% else %}4{% endif %}",
                      "props": {
                        "right": true,
                        "type": "datetime",
                        "utc": true,
                        "defaultFormat": "ddd MMM DD YYYY HH:mm",
                        "useOwnFormat": true
                      }
                    },
                    {
                      "label": "Widget.actualDate",
                      "name": "shipmentDate.{{flex}}{{key}}DateActual",
                      "component": "DateTimePicker",
                      "class": "xs-{% if flex === '' %}6{% else %}4{% endif %}",
                      "props": {
                        "right": true,
                        "type": "datetime",
                        "utc": true,
                        "defaultFormat": "ddd MMM DD YYYY HH:mm",
                        "useOwnFormat": true
                      }
                    },
                    {
                      "label": "Widget.port",
                      "name": "shipmentDate.{{flex}}{{key}}DatePort",
                      "checkDisplayWhen": "return '{{flex}}' !== ''",
                      "component": "v-select",
                      "class": "xs-4",
                      "props": {
                        "right": true,
                        "class": "input-text-right",
                        "items": [
                          {
                            "key": "portOfLoading",
                            "value": "Port of Loading"
                          }, {
                            "key": "portOfDischarge",
                            "value": "Port of Discharge"
                          }
                        ],
                        "item-text": "value",
                        "item-value": "key"
                      }
                    },
                    {
                      "label": "Widget.remark",
                      "name": "shipmentDate.{{flex}}{{key}}DateRemark",
                      "component": "v-text-field",
                      "checkDisplayWhen": "return !(user.parties.findIndex(x => x.erpCode === 'L1052') >= 0 && ['departure'].includes('{{key}}')) ",
                      "class": "xs-12 md-12"
                    }
                  ]
                }
              }]
            }
          }]
        }]
      }
    }, {
      "is": "DynamicComponent",
      "props": {
        "components": [{
          "is": "WidgetMoreForm",
          "props": {
            "small": true,
            "name": "shipmentDate",
            "mainKey": "Date",
            "formProps": {
              "z": {
                "class": "xs-12 padding-leftright-2",
                "formContainerProps": {
                  "style": {
                    "margin-top": "-2px"
                  }
                }
              }
            },
            "labelProps": {
              "z": {
                "isComponent": "v-label",
                "i18nContext": "Widget",
                "class": "font-110-important"
              }
            },
            "fixedKeys": [
              "oceanBill",
              "cargoReady",
              "scheduleAssigned",
              "scheduleApproaved",
              "spaceConfirmation",
              "bookingSubmit",
              "cyCutOff",
              "documentCutOff",
              "sentToShipper",
              "pickup",
              "shipperLoad",
              "returnLoad",
              "gateIn",
              "cargoReceipt",
              "shipperDocumentSubmit",
              "shipperInstructionSubmit",
              "houseBillDraftSubmit",
              "houseBillConfirmation",
              "masterBillReleased",
              "preAlertSend",
              "ediSend",
              "cargoRolloverStatus",
              "loadOnboard",
              "departure",
              "arrival",
              "inboundTransfer",
              "onRail",
              "arrivalAtDepot",
              "availableForPickup",
              "pickupCargoBeforeDemurrage",
              "finalCargo",
              "cargoPickupWithDemurrage",
              "sentToConsignee",
              "finalDoorDelivery",
              "returnEmptyContainer"
            ],
            "checkDisplayWhen": [
              "if (!['departure', 'arrival', 'oceanBill'].includes(key)) { return false }",
              "const estimated = context._.get(form, `${key}DateEstimated`, null);",
              "const actual = context._.get(form, `${key}DateActual`, null);",
              "return estimated || actual"
            ],
            "canAdd": false,
            "fields": [{
                "label": "Widget.estimatedDate",
                "name": "shipmentDate.{{flex}}{{key}}DateEstimated",
                "component": "DateTimePicker",
                "class": "xs-6",
                "props": {
                  "right": true,
                  "type": "datetime",
                  "utc": true,
                  "defaultFormat": "ddd MMM DD YYYY HH:mm",
                  "useOwnFormat": true
                }
              },
              {
                "label": "Widget.actualDate",
                "name": "shipmentDate.{{flex}}{{key}}DateActual",
                "component": "DateTimePicker",
                "class": "xs-6",
                "props": {
                  "right": true,
                  "type": "datetime",
                  "utc": true,
                  "defaultFormat": "ddd MMM DD YYYY HH:mm",
                  "useOwnFormat": true
                }
              }
            ]
          }
        }]
      }
    }]
  }]
}
