{
  "components": [{
    "is": "AsyncComponent",
    "props": {
      "class": "xs-12 margin-topbottom-2",
      "layoutName": "plugins/document",
      "loadWhenDataLoaded": true,
      "axiosBaseParams": [
        "const data = [];",
        "const entityData = context.$store.state.widget.data;",
        "data.push({ tableName: 'shipment', primaryKey: entityData.id });",
        "for (const shipmentBooking of entityData.shipmentBooking || []) {",
        "if (shipmentBooking.booking) {",
        "data.push({ tableName: 'booking', primaryKey: shipmentBooking.booking.id });",
        "}",
        "}",
        "return {",
        "method: 'POST',",
        "url: 'api/document/documents',",
        "data,",
        "};"
      ],
      "uploadAxiosParams": [
        "const entityData = context.$store.state.widget.data;",
        "return {",
        "method: 'POST',",
        "url: 'api/document/document/upload',",
        "data: {",
        "tableName: swig.tableName || 'shipment',",
        "primaryKey: swig.primaryKey || entityData.id,",
        "fileName: swig.fileName,",
        "fileData: { base64String: swig.base64String, extension: swig.extension }",
        "}",
        "};"
      ],
      "downloadAxiosParams": [
        "const selectedExtra = context.$store.state.widget.selectedExtra;",
        "const tableName = context.selectedExtra ? 'booking' : 'shipment';",
        "const entityData = selectedExtra || context.$store.state.widget.data;",
        "const primaryKey = entityData.id;",
        "return {",
        "responseType: 'blob',",
        "method: 'POST',",
        "url: 'api/document/document/download',",
        "data: {",
        "tableName: swig.tableName,",
        "primaryKey: swig.primaryKey,",
        "fileName: swig.fileName,",
        "}",
        "}"
      ],
      "deleteAxiosParams": [
        "const selectedExtra = context.$store.state.widget.selectedExtra;",
        "const tableName = selectedExtra ? 'booking' : 'shipment';",
        "const entityData = selectedExtra || context.$store.state.widget.data;",
        "const primaryKey = entityData.id;",
        "return {",
        "method: 'POST',",
        "url: 'api/document/document/delete',",
        "data: {",
        "tableName: swig.tableName,",
        "primaryKey: swig.primaryKey,",
        "fileName: swig.fileName,",
        "}",
        "}"
      ],
      "imageLoadAxiosParams": [
        "const selectedExtra = context.$store.state.widget.selectedExtra;",
        "const tableName = selectedExtra ? 'booking' : 'shipment';",
        "const entityData = selectedExtra || context.$store.state.widget.data;",
        "const primaryKey = entityData.id;",
        "return {",
        "method: 'POST',",
        "url: 'api/document/document/preview',",
        "data: {",
        "tableName: swig.tableName,",
        "primaryKey: swig.primaryKey,",
        "fileName: swig.fileName,",
        "}",
        "}"
      ]
    }
  }]
}
