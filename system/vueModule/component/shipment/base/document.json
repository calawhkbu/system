{
  "components": [{
    "is": "AsyncComponent",
    "props": {
      "class": "xs-12 margin-topbottom-2",
      "layoutName": "plugins/document",
      "loadWhenDataLoaded": true,
      "axiosBaseParams": [
        "const selectedExtra = context.$store.state.widget.selectedExtra;",
        "const tableName = selectedExtra ? 'booking' : 'shipment';",
        "const entityData = selectedExtra || context.$store.state.widget.data;",
        "const primaryKey = entityData.id;",
        "return {",
        "url: 'api/document/document/{{tableName}}/{{primaryKey}}'.format({ primaryKey, tableName }),",
        "};"
      ],
      "uploadAxiosParams": [
        "const selectedExtra = context.$store.state.widget.selectedExtra;",
        "const tableName = selectedExtra ? 'booking' : 'shipment';",
        "const entityData = selectedExtra || context.$store.state.widget.data;",
        "const primaryKey = entityData.id;",
        "return {",
        "method: 'POST',",
        "url: 'api/document/document/upload',",
        "data: {",
        "tableName, primaryKey,",
        "fileName: swig.fileName,",
        "fileData: { base64String: swig.base64String, extension: swig.extension }",
        "}",
        "};"
      ],
      "downloadAxiosParams": [
        "const selectedExtra = context.$store.state.widget.selectedExtra;",
        "const tableName = context.selectedExtra ? 'booking' : 'shipment';",
        "const entityData = selectedExtra || context.$store.state.widget.data;",
        "const primaryKey = entityData.id;",
        "return {",
        "responseType: 'blob',",
        "method: 'POST',",
        "url: 'api/document/document/download',",
        "data: {tableName, primaryKey, fileName: swig.fileName}",
        "}"
      ],
      "deleteAxiosParams": [
        "const selectedExtra = context.$store.state.widget.selectedExtra;",
        "const tableName = selectedExtra ? 'booking' : 'shipment';",
        "const entityData = selectedExtra || context.$store.state.widget.data;",
        "const primaryKey = entityData.id;",
        "return {",
        "method: 'POST',",
        "url: 'api/document/document/delete',",
        "data: {tableName, primaryKey, fileName: swig.fileName}",
        "}"
      ],
      "imageLoadAxiosParams": [
        "const selectedExtra = context.$store.state.widget.selectedExtra;",
        "const tableName = selectedExtra ? 'booking' : 'shipment';",
        "const entityData = selectedExtra || context.$store.state.widget.data;",
        "const primaryKey = entityData.id;",
        "return {",
        "method: 'POST',",
        "url: 'api/document/document/preview',",
        "data: {",
        "tableName,",
        "primaryKey,",
        "fileName: swig.fileName",
        "}",
        "}"
      ]
    }
  }]
}
