                  "more": [
                    {
                      "name": "productId",
                      "selector": "id"
                    }
                  ],
                  "props": {
                    "searchValueParams": {
                      "method": "POST",
                      "url": "api/product/query/product",
                      "data": {
                        "subqueries": {
                          "idLike": {
                            "value": "{{context.autoSuggestModel}}"
                          }
                        }
                      }
                    },
                    "searchTextParams": {
                      "method": "POST",
                      "url": "api/product/query/product",
                      "data": {
                        "subqueries": {
                          "productCodeLike": {
                            "value": "{{context.search}}"
                          },
                          "nameLike": {
                            "value": "{{context.search}}"
                          }
                        },
                        "limit": 5
                      }
                    },
                    "item-text": "name",
