{
  "components": [{
    "is": "AsyncComponent",
    "props": {
      "class": "xs-12 margin-topbottom-2",
      "layoutName": "plugins/document",
      "loadWhenDataLoaded": true,
      "axiosBaseParams": [
        "const data = context.$store.state.widget.data;",
        "const primaryKey = data.id;",
        "return {",
        "url: 'api/document/document/purchase_order/{{primaryKey}}'.format({ primaryKey }),",
        "};"
      ],
      "uploadAxiosParams": [
        "const data = context.$store.state.widget.data;",
        "const primaryKey = data.id;",
        "return {",
        "method: 'POST',",
        "url: 'api/document/document/upload',",
        "data: {",
        "tableName: 'purchase_order', primaryKey,",
        "fileName: swig.fileName,",
        "fileData: { base64String: swig.base64String, extension: swig.extension }",
        "}",
        "};"
      ],
      "downloadAxiosParams": [
        "const data = context.$store.state.widget.data;",
        "const primaryKey = data.id;",
        "return {",
        "responseType: 'blob',",
        "method: 'POST',",
        "url: 'api/document/document/download',",
        "data: {tableName: 'purchase_order', primaryKey, fileName: swig.fileName}",
        "}"
      ],
      "deleteAxiosParams": [
        "const data = context.$store.state.widget.data;",
        "const primaryKey = data.id;",
        "return {",
        "method: 'POST',",
        "url: 'api/document/document/delete',",
        "data: {tableName: 'purchase_order', primaryKey, fileName: swig.fileName}",
        "}"
      ],
      "imageLoadAxiosParams": [
        "const data = context.$store.state.widget.data;",
        "const primaryKey = data.id;",
        "return {",
        "method: 'POST',",
        "url: 'api/document/document/preview',",
        "data: {",
        "tableName: 'purchase_order',",
        "primaryKey,",
        "fileName: swig.fileName",
        "}",
        "}"
      ]
    }
  }]
}
