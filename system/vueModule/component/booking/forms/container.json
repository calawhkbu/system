          }],
          "props": {
            "searchTextParams": {
              "url": "api/code/query/code_master",
              "method": "POST",
              "data": {
                "subqueries": {
                  "codeType": {
                    "value": "CURRENCY"
                  },
                  "q": {
                    "value": "{{context.search}}"
                  }
                }
              }
            },
            "onInit": true,
            "item-text": "code",
            "item-value": "code",
            "showValue": false
