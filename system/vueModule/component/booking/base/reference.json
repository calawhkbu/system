{
  "layout": "v-card",
  "components": [{
    "is": "MouseHidden",
    "props": {
      "hiddenProps": {
        "style": {
          "position": "absolute",
          "right": "0px",
          "top": "0px"
        }
      }
    },
    "slots": [{
      "name": "hidden",
      "is": "DynamicComponent",
      "props": {
        "components": [{
          "is": "v-btn",
          "if": [
            "if (context.isSwivelAdmin) { return true; }",
            "const user = context.$store.state.auth.userProfile;",
            "if (!user) { return false; }",
            "const partyGroup = user.selectedPartyGroup",
            "const partyGroupInstalled = (user.selectedPartyGroup.configuration || {}).installedService || [];",
            "const userRoles = user.roles || []",
            "if (partyGroupInstalled.find(install => install === 'BOOKING_CONTROL')) {",
              "return !userRoles.find(role => role.name === 'BLOCK_BOOKING_EDIT')",
            "}",
            "return true"
          ],
          "props": {
            "icon": true,
            "small": true,
            "class": "zindex-3 margin-0"
          },
          "slots": [{
            "is": "Icon",
            "props": {
              "icon": "edit"
            }
          }],
          "events": {
            "click.stop": [{
              "type": "dispatch",
              "otherParams": {
                "name": "widget/changeMode",
                "mode": "edit"
              },
              "afterActions": [{
                "type": "dispatch",
                "otherParams": {
                  "name": "widget/updateComponentSetting",
                  "value": {
                    "stepper": 4
                  }
                }
              }]
            }]
          }
        }, {
          "is": "RightDrawer",
          "props": {
            "hide-overlay": true,
            "buttonComponentProps": {
              "icon": true,
              "small": true,
              "class": "zindex-3 margin-0"
            }
          },
          "slots": [{
            "name": "button",
            "is": "Icon",
            "props": {
              "icon": "open_in_new"
            }
          }, {
            "name": "default",
            "is": "DynamicComponent",
            "props": {
              "class": "full-width full-height",
              "components": [{
                "is": "WidgetForm",
                "props": {
                  "fields": [{
                      "label": "BookingPage.references",
                      "name": "bookingReference",
                      "component": "TableField",
                      "class": "xs-12",
                      "props": {
                        "topLabel": true,
                        "fields": [{
                            "label": "BookingPage.refName",
                            "name": "refName",
                            "class": "xs-12 md-5",
                            "component": "LocalAutoSuggest",
                            "colGroupProps": {
                              "style": {
                                "width": "25%"
                              }
                            },
                            "props": {
                              "canCreate": true,
                              "funcItems": ["MBL", "MAWB", "HBL", "HAWB"]
                            }
                          },
                          {
                            "label": "BookingPage.refDescription",
                            "name": "refDescription",
                            "class": "xs-12 md-5",
                            "component": "v-text-field",
                            "colGroupProps": {
                              "style": {
                                "width": "50%"
                              }
                            }
                          },
                          {
                            "label": "BookingPage.docDate",
                            "name": "docDate",
                            "component": "DateTimePicker",
                            "class": "xs-12 md-2",
                            "colGroupProps": {
                              "style": {
                                "width": "20%"
                              }
                            },
                            "props": {
                              "type": "date",
                              "utc": true,
                              "defaultFormat": "ddd MMM DD YYYY",
                              "useOwnFormat": true
                            }
                          }
                        ]
                      }
                    },
                    {
                      "label": "BookingPage.shipMarks",
                      "name": "shipMarks",
                      "component": "v-textarea",
                      "class": "xs-12 md-6"
                    },
                    {
                      "label": "BookingPage.goodsDescription",
                      "name": "goodsDescription",
                      "component": "v-textarea",
                      "class": "xs-12 md-6"
                    },
                    {
                      "label": "BookingPage.remark",
                      "name": "remark",
                      "component": "v-textarea",
                      "class": "xs-12 md-6"
                    },
                    {
                      "label": "BookingPage.specialInstruction",
                      "name": "flexData.specialInstruction",
                      "component": "v-textarea",
                      "class": "xs-12 md-6"
                    }
                  ]
                }
              }]
            }
          }]
        }]
      }
    }, {
      "is": "DynamicComponent",
      "props": {
        "components": [{
          "is": "v-card-title",
          "props": {
            "class": "bold uppercase padding-0 min-height-28 padding-leftright-4"
          },
          "slots": [{
            "is": "I18nText",
            "props": {
              "i18nContext": "BookingPage",
              "i18nKey": "referenceForm"
            }
          }]
        }, {
          "is": "v-divider"
        }, {
          "is": "WidgetForm",
          "props": {
            "class": "padding-top-10",
            "fields": [{
                "label": "BookingPage.references",
                "name": "bookingReference",
                "component": "TableField",
                "class": "xs-12",
                "checkDisplayWhen": "return form && form.bookingReference && form.bookingReference.length",
                "props": {
                  "topLabel": true,
                  "fields": [{
                      "label": "BookingPage.refName",
                      "name": "refName",
                      "class": "xs-12 md-5",
                      "component": "LocalAutoSuggest",
                      "colGroupProps": {
                        "style": {
                          "width": "25%"
                        }
                      },
                      "props": {
                        "canCreate": true,
                        "funcItems": ["MBL", "MAWB", "HBL", "HAWB"]
                      }
                    },
                    {
                      "label": "BookingPage.refDescription",
                      "name": "refDescription",
                      "class": "xs-12 md-5",
                      "component": "v-text-field",
                      "colGroupProps": {
                        "style": {
                          "width": "50%"
                        }
                      }
                    },
                    {
                      "label": "BookingPage.docDate",
                      "name": "docDate",
                      "component": "DateTimePicker",
                      "class": "xs-12 md-2",
                      "colGroupProps": {
                        "style": {
                          "width": "20%"
                        }
                      },
                      "props": {
                        "type": "date",
                        "utc": true,
                        "defaultFormat": "ddd MMM DD YYYY",
                        "useOwnFormat": true
                      }
                    }
                  ]
                }
              },
              {
                "label": "BookingPage.shipMarks",
                "name": "shipMarks",
                "component": "v-textarea",
                "checkDisplayWhen": "return form && form.shipMarks",
                "class": "xs-6"
              },
              {
                "label": "BookingPage.goodsDescription",
                "name": "goodsDescription",
                "component": "v-textarea",
                "checkDisplayWhen": "return form && form.goodsDescription",
                "class": "xs-6"
              },
              {
                "label": "BookingPage.remark",
                "name": "remark",
                "component": "v-textarea",
                "checkDisplayWhen": "return form && form.remark",
                "class": "xs-6"
              },
              {
                "label": "BookingPage.specialInstruction",
                "name": "flexData.specialInstruction",
                "component": "v-textarea",
                "checkDisplayWhen": "return form && form.flexData && form.flexData.specialInstruction",
                "class": "xs-6"
              }
            ]
          }
        }]
      }
    }]
  }]
}
