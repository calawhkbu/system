{
  "fillParent": true,
  "components": [{
    "is": "TableTool",
    "props": {
      "title": "Shipment",
      "reportingKey": "shipments",
      "jql": "shipments",
      "hasReports": true,
      "showFilterBar": true,
      "enableShare": true,
      "enableSetting": true,
      "noPartyGroupFilters": false,
      "nextPageComponent": "shipment",
      "clickFunction": {
        "changeRow": [
          "return {",
          "...row,",
          "shipmentParty: {",
          "officePartyName: row.officePartyName,",
          "shipperPartyName: row.shipperPartyName,",
          "consigneePartyName: row.consigneePartyName,",
          "roAgentPartyName: row.roAgentPartyName,",
          "linerAgentPartyName: row.linerAgentPartyName,",
          "agentPartyName: row.agentPartyName,",
          "controllingCustomerPartyName: row.controllingCustomerPartyName,",
          "notifyPartyPartyName: row.notifyPartyPartyName,",
          "},",
          "shipmentDate: {",
          "departureDateEstimated: row.departureDateEstimated,",
          "departureDateActual: row.departureDateActual,",
          "arrivalDateEstimated: row.arrivalDateEstimated,",
          "arrivalDateActual: row.arrivalDateActual,",
          "},",
          "shipmentContainers: row.containerNos ? row.containerNos.split(',').map(no => ({ containerNo: no })) : [],",
          "shipmentPo: row.poNos ? row.poNos.split(',').map(no => ({ poNo: no})) : [],",
          "...(row.shipId ? {shipmentReference: [{refName: 'Shipment Reference ID', refDescription: row.shipId}]} : {})",
          "};"
        ],
        "type": "dispatch",
        "otherParams": {
          "name": "widget/initShortData"
        },
        "afterActions": [{
          "type": "router",
          "otherParams": {
            "base": true,
            "func": "push",
            "path": "detail-edit/{{id}}"
          }
        }]
      },
      "newReport": {
        "filters": {
          "isActive": {
            "value": "active"
          },
          "date": {
            "func": {
              "args": {
                "unit": "month"
              },
              "key": "current"
            }
          }
        },
        "configurations": {
          "i18nContext": "ShipmentPage",
          "clickable": true,
          "draggable": true,
          "pagination": {
            "current": 0,
            "perPage": 20,
            "remote": true
          },

          "fields": [
            "moduleTypeCode",
            "houseNo",
            "masterNo",
            "poNos",
            "carrierName",
            "jobDate",
            "departureDateEstimated",
            "departureDateActual",
            "arrivalDateEstimated",
            "arrivalDateActual",
            "portOfLoadingName",
            "portOfDischargeName",
            "controllingCustomerPartyName",
            "shipperPartyName",
            "consigneePartyName",
            "linerAgentPartyName",
            "roAgentPartyName",
            "agentPartyName",
            "haveCurrentTrackingNo",
            "lastStatusCodeOrDescription"
          ]
        }
      },
      "filters": [{
        "name": "isActive",
        "type": "list",
        "props": {
          "items": [{
              "label": "active",
              "value": "active"
            },
            {
              "label": "deleted",
              "value": "deleted"
            }
          ]
        }
      }],
      "fieldOptions": [{
          "key": "id",
          "hidden": true
        },
        {
          "key": "poNos",
          "label": "poNo",
          "tdComponent": {
            "props": {
              "html": "{{ 'args[0].poNos ? args[0].poNos.split(`,`).join(`<br>`) : ``' | calculate(row) | safe }}"
            }
          }
        },

        {
          "key": "containerNos",
          "label": "containerNo",
          "tdComponent": {
            "props": {
              "html": "{{ 'args[0].containerNos ? args[0].containerNos.split(`,`).join(`<br>`) : ``' | calculate(row) | safe }}"
            }
          }
        },

        {

          "api": {
            "arrayMapper": {
              "keyKey": "code",
              "valueKey": "name"
            },
            "axiosParams": {
              "data": {
                "subqueries": {
                  "codeType": {
                    "value": "YD_STATUS"
                  }
                }
              },
              "method": "POST",
              "url": "api/code/query/code_master"
            }
          },
          "key": "lastStatusCodeOrDescription",
          "label": "lastStatus",
          "i18nContext" : "Reporting"
        },

        {
          "key": "contractNos",
          "label": "contractNo"
        },
        {
          "key": "haveCurrentTrackingNo",
          "label": ""
        },
        {
          "format": "YYYY-MM-DD",
          "key": "jobDate"
        },
        {
          "boolean": true,
          "key": "isDirect"
        },
        {
          "boolean": true,
          "key": "isCoload"
        },
        {
          "key": "nominatedTypeCode",
          "label": "nominatedType"
        },
        {
          "key": "carrierName",
          "label": "carrier"
        },
        {
          "key": "masterNo",
          "width": 140
        },
        {
          "api": {
            "arrayMapper": {
              "keyKey": "code",
              "valueKey": "name"
            },
            "axiosParams": {
              "data": {
                "subqueries": {
                  "codeType": {
