                    "value": "MODULE"
                  }
                }
              },
              "method": "POST",
              "url": "api/code/query/code_master"
            }
          },
          "key": "moduleTypeCode",
          "label": "moduleType"
        },
        {
          "api": {
            "arrayMapper": {
              "keyKey": "code",
              "valueKey": "name"
            },
            "axiosParams": {
              "data": {
                "subqueries": {
                  "codeType": {
                    "value": "BOUND"
                  }
                }
              },
              "method": "POST",
              "url": "api/code/query/code_master"
            }
          },
          "key": "boundTypeCode",
          "label": "boundType"
        },
        {
          "api": {
            "arrayMapper": {
              "keyKey": "code",
              "valueKey": "name"
            },
            "axiosParams": {
              "data": {
                "subqueries": {
                  "codeType": {
                    "value": "INCOTERMS"
                  }
                }
              },
              "method": "POST",
              "url": "api/code/query/code_master"
            }
          },
          "key": "incoTermsCode",
          "label": "incoTerms"
        },
        {
          "api": {
            "arrayMapper": {
