              "valueKey": "name"
            },
            "axiosParams": {
              "data": {
                "subqueries": {
                  "codeType": {
                    "value": "PAYTERMS"
                  }
                }
              },
              "method": "POST",
              "url": "api/code/query/code_master"
            }
          },
          "key": "otherTermsCode",
          "label": "otherTerms"
        },
        {
          "api": {
            "arrayMapper": {
              "keyKey": "code",
              "valueKey": "name"
            },
            "axiosParams": {
              "data": {
                "subqueries": {
                  "codeType": {
                    "value": "PAYTERMS"
                  }
                }
              },
              "method": "POST",
              "url": "api/code/query/code_master"
            }
          },
          "key": "freightTermsCode",
          "label": "freightTerms"
        },
        {
          "format": "YYYY-MM-DD",
          "key": "departureDateEstimated"
        },
        {
          "format": "YYYY-MM-DD",
          "key": "arrivalDateEstimated"
        },
        {
          "format": "YYYY-MM-DD",
          "key": "departureDateActual"
        },
        {
          "format": "YYYY-MM-DD",
          "key": "arrivalDateActual"
        },
        {
          "key": "shipperPartyName",
          "label": "shipper"
        },
        {
          "key": "forwarderPartyName",
          "label": "forwarder"
        },
        {
          "key": "consigneePartyName",
          "label": "consignee"
        },
        {
          "key": "linerAgentPartyName",
          "label": "linerAgent"
        },
        {
          "key": "roAgentPartyName",
          "label": "roAgent"
        },
        {
          "key": "agentPartyName",
          "label": "agent"
        },
        {
          "key": "officePartyName",
          "label": "office"
        },
        {
          "key": "controllingCustomerPartyName",
          "label": "controllingCustomer"
        },
        {
          "key": "salesmanCode",
          "label": "salesman"
        },
        {
          "key": "quantity"
        },
        {
          "api": {
            "arrayMapper": {
              "keyKey": "code",
              "valueKey": "name"
            },
            "axiosParams": {
              "data": {
                "subqueries": {
                  "codeType": {
                    "value": "SHIPTYPE"
                  }
                }
              },
              "method": "POST",
              "url": "api/code/query/code_master"
            }
          },
          "key": "shipmentTypeCode",
          "label": "shipmentType"
        },
        {
          "key": "placeOfReceiptName",
          "label": "placeOfReceipt"
        },
        {
          "key": "portOfLoadingName",
          "label": "portOfLoading"
        },
        {
          "key": "portOfDischargeName",
          "label": "portOfDischarge"
        },
        {
          "key": "placeOfDeliveryName",
          "label": "placeOfDelivery"
