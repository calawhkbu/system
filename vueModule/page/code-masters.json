{
  "fillParent": true,
  "components": [{
    "is": "TableTool",
    "props": {
      "title": "Code",
      "back": "/admin",
      "reportingKey": "code-masters",
      "urlFormat": "/admin/code-masters",
      "jql": "code-masters",
      "showFilterBar": true,
      "enableShare": false,
      "enableSetting": false,
      "noPartyGroupFilters": true,
      "nextPageComponent": "code-master",
      "newReport": {
        "filters": {
          "isActive": {
            "value": "active",
            "locked": true
          }
        },
        "configurations": {
          "i18nContext": "CodeMasterPage",
          "clickable": true,
          "pagination": {
            "current": 0,
            "perPage": 20,
            "remote": true
          }
        }
      },
      "fieldOptions": [{
          "key": "id",
          "hidden": true
        },
        {
          "key": "partyGroupCode",
          "hidden": true
        },
        {
          "label": "",
          "key": "canResetDefault",
          "sortable": false,
          "align": "center",
          "clickFunction": {
            "type": "axios",
            "otherParams": {
              "axiosParams": {
                "url": "/api/code/resetDefault/{{row.id}}",
                "method": "POST"
              }
            },
            "afterActions": [{
              "type": "notify",
              "otherParams": {
                "notification": "Notify.resetDefaultSuccess"
              }
            }]
          },
          "tdComponent": {
            "is": "icon",
            "hideIfFalse": true,
            "props": {
              "icon": "refresh"
            }
          },
          "width": 50
        }
      ],
      "filters": [{

          "name": "isActive",
          "type": "list",
          "props": {
            "items": [{
                "label": "active",
                "value": "active"
              },
              {
                "label": "deleted",
                "value": "deleted"
              }
            ]
          }
        },
        {
          "name": "codeType",
          "i18nContext": "CodeMasterPage",
          "props": {
            "api": {
              "query": {
                "data": {
                  "limit": 5,
                  "fields": [
                    "codeType"
                  ],
                  "subqueries": {
                    "codeTypeLike": {
                      "value": "{{ q }}"
                    }
                  }
                },
                "labelKey": "codeType",
                "method": "POST",
                "url": "api/code/query/code_master",
                "valueKey": "codeType"
              },
              "resolve": {
                "data": {
                  "fields": ["codeType"],
                  "subqueries": {
                    "codeType": {
                      "value": "{{value}}"
                    }
                  }
                },
                "labelKey": "codeType",
                "method": "POST",
                "url": "api/code/query/code_master",
                "valueKey": "codeType"
              }
            },
            "multi": false
          },
          "type": "autocomplete"
        },
        {
          "name": "code",
          "type": "text"
        }
      ],
      "clickFunction": {
        "type": "router",
        "otherParams": {
          "func": "push",
          "path": "/detail/{{id}}",
          "base": true
        }
      }
    },
    "slots": [{
      "name": "middle-button",
      "is": "DynamicComponent",
      "props": {
        "class": "button",
        "components": [{
          "is": "v-btn",
          "props": {
            "small": true,
            "flat": true,
            "icon": true
          },
          "slots": [{
            "is": "Icon",
            "props": {
              "icon": "add"
            }
          }],
          "events": {
            "click.stop": [{
              "type": "router",
              "otherParams": {
                "func": "push",
                "path": "code-master/create",
                "base": true
              }
            }]
          }
        }]
      }
    }]
  }]
}