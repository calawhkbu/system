                "flat": true,
                "icon": true
              },
              "events": {
                "click.stop": [
                  {
                    "type": "router",
                    "otherParams": {
                      "func": "go",
                      "go": -1
                    }
                  }
                ]
              },
              "slots": [
                {
                  "is": "Icon",
                  "props": {
                    "icon": "arrow_back"
                  }
                }
              ]
            },
            {
              "is": "I18nText",
              "props": {
                "class": "headline",
                "i18nKey": "UserPage.title",
                "swig": true
              }
            }
          ],
          "components": {
            "create": [
              {
                "is": "WidgetPanel",
                "props": {
                  "initUrl": "api/person/{{id}}",
                  "saveUrl": "api/person",
                  "primaryKey": "id"
                },
                "slots": [
                  {
                    "name": "default",
                    "is": "DynamicComponent",
                    "props": {
                      "components": [
                        {
                          "is": "AsyncComponent",
