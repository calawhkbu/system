{
  "fillParent": true,
  "components": [{
    "is": "TableTool",
    "props": {
      "title": "Templates",
      "back": "/admin",
      "urlFormat": "/admin/templates",
      "reportingKey": "templates",
      "jql": "templates",
      "noPartyGroupFilters": true,
      "hasReports": false,
      "showFilterBar": true,
      "enableShare": false,
      "enableSetting": false,
      "noCache": true,
      "refreshWhenBack": true,
      "nextPageComponent": "template",
      "clickFunction": {
        "type": "router",
        "otherParams": {
          "func": "push",
          "path": "/detail/{{id}}",
          "base": true
        }
      },
      "newReport": {
        "filters": {
          "isActive": {
            "value": "active",
            "locked": true
          },
          "extension": {
            "value": "xlsx"
          }
        },
        "configurations": {
          "i18nContext": "TemplatePage",
          "clickable": true,
          "sorting": {},
          "pagination": {
            "current": 0,
            "perPage": 20
          }
        }
      },
      "fieldOptions": [{
          "key": "id",
          "hidden": true
        },
        {
          "key": "isActive",
          "boolean": true
        },
        {
          "label": "",
          "key": "canDelete",
          "sortable": false,
          "align": "center",
          "clickFunction": {
            "type": "axios",
            "otherParams": {
              "axiosParams": {
                "url": "/api/template/delete",
                "method": "POST",
                "data": {
                  "templateName": "{{row.templateName}}"
                }
              }
            },
            "afterActions": [{
              "type": "notify",
              "otherParams": {
                "notification": "Notify.deleteSuccess"
              }
            }]
          },
          "tdComponent": {
            "is": "icon",
            "hideIfFalse": true,
            "props": {
              "icon": "delete"
            }
          },
          "width": 50
        },

        {
          "label": "",
          "key": "canRestore",
          "sortable": false,
          "align": "center",
          "clickFunction": {
            "type": "axios",
            "otherParams": {
              "axiosParams": {
                "url": "/api/template/restore",
                "method": "POST",
                "data": {
                  "templateName": "{{row.templateName}}"
                }
              }
            },
            "afterActions": [{
              "type": "notify",
              "otherParams": {
                "notification": "Notify.restoreSuccess"
              }
            }]
          },
          "tdComponent": {
            "hideIfFalse": true,
            "is": "icon",
            "props": {
              "icon": "refresh"
            }
          },
          "width": 50
        }
      ],
      "filters": [{
          "name": "isActive",
          "type": "list",
          "props": {
            "items": [{
                "label": "active",
                "value": "active"
              },
              {
                "label": "deleted",
                "value": "deleted"
              }
            ]
          }
        }, {
          "name": "templateName",
          "i18nContext": "TemplatePage",
          "type": "text"
        },
        {
          "name": "extension",
          "i18nContext": "TemplatePage",
          "type": "list",
          "props": [{
            "label": "Excel",
            "value": "xlsx"
          }, {
            "label": "PDF",
            "value": "pdf"
          }]
        }
      ]
    },
    "slots": [{
      "name": "middle-button",
      "is": "DynamicComponent",
      "props": {
        "class": "button",
        "components": [{
          "is": "v-btn",
          "props": {
            "small": true,
            "flat": true,
            "icon": true
          },
          "slots": [{
            "is": "Icon",
            "props": {
              "icon": "add"
            }
          }],
          "events": {
            "click.stop": [{
              "type": "router",
              "otherParams": {
                "func": "push",
                "path": "/template/create",
                "base": true
              }
            }]
          }
        }]
      }
    }]
  }]
}