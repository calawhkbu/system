{
  
  "components": [
    {
      "is": "TableTool",
      "props": {
        "reportingKey": "invitations",
        "jql": "invitations",
        "urlFormat": "/invitations{{ id|if('/', '') }}{{ id }}",
        "hasReports": true,
        "showFilterBar": true,
        "enableShare": true,
        "enableSetting": true,
        "newReport": {
          "filters": {
            "isActive": true
          },
          "configurations": {
            "i18nContext": "InvitationPage",
            "clickable": true,
            "draggable": true,
            "sorting": {
              "direction": "DESC",
              "key": "updatedAt"
            },
            "pagination": {
              "current": 0,
              "perPage": 20
            }
          }
        },
        "fieldOptions": [
          {
            "key": "id",
            "sql": "invitation.id",
            "hidden": true
          },
          {
            "key": "createdAt",
            "sql": "invitation.createdAt",
            "width": 150,
            "format": "YYYY-MM-DD"
          },
          {
            "key": "updatedAt",
            "sql": "invitation.updatedAt",
            "width": 150,
            "format": "YYYY-MM-DD"
          },
          {
            "key": "invitationNo",
            "width": 150
          }
        ],
        "filters": [],
        "clickFunction": {
          "type": "router",
          "otherParams": {
            "func": "push",
            "path": "/invitation/{{id}}"
          }
        },
        "icons": [
          {
            "icon": "add",
            "click": [
              {
                "type": "router",
                "otherParams": {
                  "func": "push",
                  "path": "invitation/create"
                }
              }
            ]
          }
        ]
      }
    }
  ]
}
