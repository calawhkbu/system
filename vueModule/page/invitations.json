{
  "layout": "MainLayout",
  "components": [{
    "is": "TableTool",
    "props": {
      "reportingKey": "invitations",
      "jql": "invitations",
      "showFilterBar": true,
      "enableShare": false,
      "enableSetting": false,
      "newReport": {
        "filters": {
          "isActive": true
        },
        "configurations": {
          "i18nContext": "Reporting",
          "clickable": true,
          "draggable": true,
          "sorting": {},
          "pagination": {
            "current": 0,
            "perPage": 25
          }
        }
      },
      "fieldOptions": [
        {
          "isComponent": {
            "is": "ClickableCell",
            "props": {
              "clickFunction": {

              }
            },
            "slots": [
              {
                "is": "Icon",
                "props": {
                  "icon": "close"
                }
              }
            ]
          }
        },
        {
          "key": "id",
          "sql": "i.id",
          "hidden": true
        },
        {
          "key": "userName",
          "sql": "p.userName"
        },
        {
          "key": "firstName",
          "sql": "p.firstName"
        },
        {
          "key": "lastName",
          "sql": "p.lastName"
        },
        {
          "key": "displayName",
          "sql": "p.displayName"
        },
        {
          "key": "status",
          "sql": "i.status"
        },
        {
          "key": "createdAt",
          "sql": "i.createdAt",
          "hidden": true
        },
        {
          "key": "updatedAt",
          "sql": "i.updatedAt",
          "hidden": true
        }
      ],
      "filters": [
        {
          "name": "userName",
          "type": "text"
        },
        {
          "name": "firstName",
          "type": "text"
        },
        {
          "name": "lastName",
          "type": "text"
        },
        {
          "name": "displayName",
          "type": "text"
        }
      ],
      "clickFunction": {
        "type": "router",
        "otherParams": {
          "func": "push",
          "path": "/invitation/{{id}}"
        }
      },
      "back": "/admin",
      "icons": [
        {
          "icon": "add",
          "click": [
            {
              "type": "router",
              "otherParams": {
                "func": "push",
                "path": "invitation/create"
              }
            }
          ]
        }
      ]
    }
  }]
}
