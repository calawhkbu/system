{
  "fillParent": true,
  "components": [{
    "is": "TableTool",
    "props": {
      "back": "/admin",
      "urlFormat": "/admin/trackings",
      "title": "Tracking",
      "reportingKey": "tracking",
      "jql": "tracking",
      "hasReports": false,
      "showFilterBar": true,
      "enableShare": false,
      "enableSetting": false,
      "noPartyGroupFilters": true,
      "nextPageComponent": "tracking",
      "noCache": true,
      "newReport": {
        "filters": {
          "isActive": {
            "value": "active"
          },
          "manual": {
            "value": true
          }
        },
        "configurations": {
          "i18nContext": "TrackingPage",
          "sorting": {
            "expression": {
              "classname": "ColumnExpression",
              "name": "updatedAt"
            },
            "order": "DESC"
          },
          "pagination": {
            "remote": true,
            "current": 0,
            "perPage": 30
          }
        }
      },
      "clickFunction": {
        "type": "router",
        "otherParams": {
          "func": "push",
          "path": "/detail/{{id}}",
          "base": true
        }
      },
      "filters": [{
        "i18nContext": "TrackingPage",
        "name": "trackingNo",
        "type": "text"
      }, {
        "name": "isActive",
        "type": "list",
        "props": {
          "items": [

            {
              "label": "active",
              "value": "active"
            },
            {
              "label": "deleted",
              "value": "deleted"
            }
          ]


        }
      }, {
        "name": "manual",
        "type": "list",
        "props": {
          "items": [{
            "label": "yes",
            "value": true
          }, {
            "label": "no",
            "value": false
          }]
        }
      }],
      "fieldOptions": [{
        "key": "id",
        "hidden": true
      }, {
        "key": "updatedAt",
        "width": 150,
        "format": "YYYY-MM-DD HH:mm:ss"
      }, {
        "key": "lastStatusDate",
        "width": 150,
        "format": "YYYY-MM-DD HH:mm:ss"
      }]
    },
    "slots": [{
      "name": "middle-button",
      "is": "DynamicComponent",
      "props": {
        "class": "button",
        "components": [{
          "is": "v-btn",
          "props": {
            "small": true,
            "flat": true,
            "icon": true
          },
          "slots": [{
            "is": "Icon",
            "props": {
              "icon": "add"
            }
          }],
          "events": {
            "click.stop": [{
              "type": "router",
              "otherParams": {
                "func": "push",
                "path": "tracking/create",
                "base": true
              }
            }]
          }
        }]
      }
    }]
  }]
}