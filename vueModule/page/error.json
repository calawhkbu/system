{
  "layout": "CardLayout",
  "components": [{
      "is": "Icon",
      "props": {
        "icon": "error_outline",
        "size": 150,
        "style": {
          "flex": 0.6
        }
      }
    },
    {
      "is": "p",
      "props": {
        "class": "display-1 font-weight-bold text-capitalize center",
        "style": {
          "flex": 0.2
        }
      },
      "slots": [{
        "is": "I18nText",
        "props": {
          "i18nKey": "",
          "dispatchKey": "$route.params.params",
          "i18nContext": "ErrorLayout"
        }
      }]
    },
    {
      "is": "div",
      "props": {
        "class": "center",
        "style": {
          "flex": 0.2,
          "display": "flex",
          "flex-direction": "column"
        }
      },
      "slots": [{
        "is": "DynamicComponent",
        "props": {
          "components": [{
              "is": "v-btn",
              "props": {
                "flat": true,
                "block": true
              },
              "slots": [{
                "is": "I18nText",
                "name": "default",
                "props": {
                  "i18nKey": "ErrorLayout.Back"
                }
              }],
              "events": {
                "click.stop": [{
                  "type": "router",
                  "otherParams": {
                    "func": "go",
                    "go": -1
                  }
                }]
              }
            },
            {
              "is": "v-btn",
              "props": {
                "block": true
              },
              "slots": [{
                "is": "I18nText",
                "name": "default",
                "props": {
                  "i18nKey": "ErrorLayout.Report"
                }
              }],
              "events": {
                "click": ["console.log('TODO ERROR REPORT API')"]
              }
            }
          ]
        }
      }]
    }
  ]
}