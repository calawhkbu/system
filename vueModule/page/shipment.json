{
  "layout": "MainLayout",
  "components": [
    {
      "is": "Widget",
      "props": {
        "initUrl": "api/partyGroup/{{id}}",
        "saveUrl": "api/partyGroup",
        "primaryKey": "id",
        "onlyRead": true,
        "bookmark": true,
        "topSlots": [
          {
            "is": "v-btn",
            "props": {
              "flat": true,
              "icon": true
            },
            "events": {
              "click.stop": [
                {
                  "type": "router",
                  "otherParams": {
                    "func": "go",
                    "go": -1
                  }
                }
              ]
            },
            "slots": [
              {
                "is": "Icon",
                "props": {
                  "icon": "arrow_back"
                }
              }
            ]
          },
          {
            "is": "I18nText",
            "props": {
              "class": "headline",
              "i18nKey": "ShipmentPage.title"
            }
          }
        ],
        "components": {
          "assignment": [
          ],
          "map": [
            {
              "id": "Map",
              "layout": "AsyncLocalComponent",
              "layoutName": "shipment/fullmap"
            }
          ]
        }
      }
    }
  ]
}
