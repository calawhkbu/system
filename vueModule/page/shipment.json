{
  "fillParent": true,
  "components": [{
    "is": "Widget",
    "props": {
      "settingProp": {
        "overrideType": "shipment",
        "initAxiosParams": {
          "url": "api/shipment/{{context.$route.params.params | urlEncode }}"
        },
        "primaryKey": "houseNo",
        "modeFunction": [
          "return 'detail';"
        ],
        "haveTermOnCreate": false,
        "haveTermOnEdit": false,
        "widgetComponentSetting": {
          "detail": {
            "readonly": true
          }
        }
      }
    }
  }]
}