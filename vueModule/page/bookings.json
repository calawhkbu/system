{
  "layout": "MainLayout",
  "components": [{
    "is": "TableTool",
    "props": {
      "reportingKey": "bookings",
      "jql": "bookings",
      "urlFormat": "/bookings{{ id|if('/', '') }}{{ id }}",
      "hasReports": true,
      "showFilterBar": true,
      "enableShare": true,
      "enableSetting": true,
      "newReport": {
        "filters": {
          "isActive": true
        },
        "configurations": {
          "i18nContext": "Reporting",
          "clickable": true,
          "draggable": true,
          "sorting": {},
          "pagination": {
            "current": 0,
            "perPage": 25
          }
        }
      },
      "fieldOptions": [{
          "key": "id",
          "sql": "booking.id",
          "hidden": true
        },
        {
          "key": "bookingNo",
          "width": 150
        },
        {
          "key": "departureDateEstimated",
          "datetime": "YYYY-MM-DD"
        },
        {
          "key": "departureDateActual",
          "datetime": "YYYY-MM-DD"
        },
        {
          "key": "arrivalDateEstimated",
          "datetime": "YYYY-MM-DD"
        },
        {
          "key": "cargoReadyDateEstimated",
          "datetime": "YYYY-MM-DD"
        },
        {
          "key": "cargoReadyDateActual",
          "datetime": "YYYY-MM-DD"
        },
        {
          "key": "cYCutOffDate",
          "datetime": "YYYY-MM-DD"
        }
      ],
      "filters": [],
      "clickFunction": {
        "type": "router",
        "otherParams": {
          "func": "push",
          "path": "/booking/{{id}}"
        }
      },
      "icons": [{
        "icon": "add",
        "click": [{
          "type": "router",
          "otherParams": {
            "func": "push",
            "path": "booking/create"
          }
        }]
      }, {
        "icon": "fas fa-file-import",
        "click": [{
          "type": "router",
          "otherParams": {
            "func": "push",
            "path": "booking/import"
          }
        }]
      }]
    }
  }]
}