{
  "components": [
    {
      "is": "TableTool",
      "props": {
        "reportingKey": "users",
        "jql": "users",
        "back": "/admin",
        "title": "Users",
        "showFilterBar": true,
        "enableShare": false,
        "enableSetting": false,
        "newReport": {
          "filters": {
            "invitationStatus": {
              "value": "used"
            },
            "isActive": true
          },
          "configurations": {
            "i18nContext": "Reporting",
            "clickable": true,
            "draggable": true,
            "sorting": {},
            "pagination": {
              "current": 0,
              "perPage": 20
            }
          }
        },
        "clickFunction": {
          "type": "router",
          "otherParams": {
            "func": "push",
            "path": "/user/{{personId}}"
          }
        },
        "fieldOptions": [
          {
            "key": "personId",
            "expression": "person.id",
            "hidden": true
          }
        ],
        "filters": [
          {
            "name": "userName",
            "type": "text"
          },
          {
            "name": "firstName",
            "type": "text"
          },
          {
            "name": "lastName",
            "type": "text"
          },
          {
            "name": "displayName",
            "type": "text"
          }
        ]
      }
    }
  ]
}
