{
  "layout": "MainLayout",
  "components": [{
    "is": "TableTool",
    "props": {
      "reportingKey": "users",
      "jql": "users",
      "back": "/admin",
      "title": "Users",
      "showFilterBar": true,
      "enableShare": false,
      "enableSetting": false,
      "newReport": {
        "filters": {
          "isActive": true
        },
        "configurations": {
          "i18nContext": "Reporting",
          "clickable": true,
          "draggable": true,
          "sorting": {},
          "pagination": {
            "remote": {},
            "current": 0,
            "perPage": 25
          }
        }
      },
      "clickFunction": {
        "type": "router",
        "otherParams": {
          "func": "push",
          "path": "/user/{{personId}}"
        }
      },
      "fieldOptions": [
        {
          "key": "personId",
          "expression": "personId",
          "hidden": true
        }
      ],
      "filters": [
        {
          "name": "userName",
          "type": "text"
        },
        {
          "name": "firstName",
          "type": "text"
        },
        {
          "name": "lastName",
          "type": "text"
        },
        {
          "name": "displayName",
          "type": "text"
        }
      ]
    }
  }]
}
