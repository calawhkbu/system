{

  "fillParent": true,
  "components": [{
    "is": "TableTool",
    "props": {
      "reportingKey": "users",
      "jql": "invitations",
      "urlFormat": "/users{{ id|if('/', '') }}{{ id }}",
      "hasReports": false,
      "showFilterBar": true,
      "enableShare": false,
      "enableSetting": true,
      "newReport": {
        "filters": {
          "isActive": true,
          "date": {
            "func": {
              "args": {
                "unit": "year"
              },
              "key": "current"
            }
          }
        },
        "configurations": {
          "i18nContext": "InvitationPage",
          "clickable": true,
          "draggable": true,
          "sorting": {
            "direction": "DESC",
            "key": "updatedAt"
          },
          "pagination": {
            "current": 0,
            "perPage": 20
          }
        }
      },
      "fieldOptions": [{
          "key": "id",
          "sql": "invitation.id",
          "hidden": true
        },
        {
          "key": "createdAt",
          "sql": "invitation.createdAt",
          "width": 150,
          "format": "YYYY-MM-DD"
        },
        {
          "key": "updatedAt",
          "sql": "invitation.updatedAt",
          "width": 150,
          "format": "YYYY-MM-DD"
        },
        {
          "key": "invitationNo",
          "width": 150
        }
      ],
      "clickFunction": {
        "type": "router",
        "otherParams": {
          "func": "push",
          "path": "/user/{{id}}"
        }
      },
      "icons": [{
        "icon": "add",
        "click": [{
          "type": "router",
          "otherParams": {
            "func": "push",
            "path": "user/create"
          }
        }]
      }]
    }
  }]
}