{
  "fillParent": true,
  "components": [{
    "is": "TableTool",
    "props": {
      "title": "Users",
      "back": "/admin",
      "urlFormat": "/admin/users",
      "reportingKey": "users",
      "jql": "users",
      "hasReports": false,
      "showFilterBar": true,
      "enableShare": false,
      "enableSetting": false,
      "noPartyGroupFilters": true,
      "nextPageComponent": "user",
      "clickFunction": {
        "type": "router",
        "otherParams": {
          "func": "push",
          "path": "/detail/{{id}}",
          "base": true
        }
      },
      "newReport": {
        "filters": {
          "invitationStatus": {
            "value": ["sent", "accepted"]
          }
        },
        "configurations": {
          "i18nContext": "InvitationPage",
          "clickable": true,
          "sorting": {
            "direction": "DESC",
            "key": "updatedAt",
            "sql": {
              "classname": "ColumnExpression",
              "table": "person",
              "name": "updatedAt"
            }
          },
          "pagination": {
            "current": 0,
            "perPage": 20
          }
        }
      },
      "fieldOptions": [{
          "key": "id",
          "sql": {
            "classname": "ColumnExpression",
            "table": "invitation",
            "name": "id"
          },
          "hidden": true
        },
        {
          "key": "updatedAt",
          "sql": {
            "classname": "ColumnExpression",
            "table": "person",
            "name": "updatedAt"
          },
          "width": 150,
          "format": "YYYY-MM-DD"
        },
        {
          "label": "status",
          "key": "invitationStatus",
          "width": 150
        },
        {
          "label": "",
          "key": "canResend",
          "sortable": false,
          "align": "center",
          "clickFunction": {
            "type": "axios",
            "otherParams": {
              "axiosParams": {
                "url": "/api/invitation/resend/{{row.id}}",
                "method": "GET"
              }
            },
            "afterActions": [{
              "type": "notify",
              "otherParams": {
                "notification": "Notify.resendSuccess"
              }
            }]
          },
          "tdComponent": {
            "is": "icon",
            "hideIfFalse": true,
            "props": {
              "icon": "email"
            }
          },
          "width": 50
        },
        {
          "label": "",
          "key": "canDelete",
          "sortable": false,
          "align": "center",
          "clickFunction": {
            "type": "axios",
            "otherParams": {
              "axiosParams": {
                "url": "/api/invitation/delete/{{row.id}}",
                "method": "DELETE"
              }
            },
            "afterActions": [{
              "type": "notify",
              "otherParams": {
                "notification": "Notify.deleteSuccess"
              }
            }]
          },

          "tdComponent": {
            "is": "icon",
            "hideIfFalse": true,
            "props": {
              "icon": "delete"
            }
          },
          "width": 50
        },
        {
          "label": "",
          "key": "canRestore",
          "sortable": false,
          "align": "center",
          "clickFunction": {
            "type": "axios",
            "otherParams": {
              "axiosParams": {
                "url": "/api/invitation/restore/{{row.id}}",
                "method": "GET"
              }
            },
            "afterActions": [{
              "type": "notify",
              "otherParams": {
                "notification": "Notify.restoreSuccess"
              }
            }]
          },

          "tdComponent": {
            "is": "icon",
            "hideIfFalse": true,
            "props": {
              "icon": "refresh"
            }
          },
          "width": 50
        },
        {
          "key": "userName",
          "sql": {
            "classname": "ColumnExpression",
            "table": "person",
            "name": "userName"
          }
        },
        {
          "key": "firstName",
          "sql": {
            "classname": "ColumnExpression",
            "table": "person",
            "name": "firstName"
          }
        },
        {
          "key": "lastName",
          "sql": {
            "classname": "ColumnExpression",
            "table": "person",
            "name": "lastName"
          }
        },
        {
          "key": "displayName",
          "sql": {
            "classname": "ColumnExpression",
            "table": "person",
            "name": "displayName"
          }
        }
      ],
      "filters": [{
          "display": "status",
          "name": "invitationStatus",
          "i18nContext": "InvitationPage",
          "props": {
            "multi": true,
            "items": [{
                "value": "sent",
                "label": "sent"
              },
              {
                "value": "accepted",
                "label": "accepted"
              },
              {
                "value": "disabled",
                "label": "disabled"
              }
            ]
          },
          "type": "list"
        },
        {

          "i18nContext": "InvitationPage",
          "display": "email",
          "name": "userName",
          "type": "text"
        },
        {

          "i18nContext": "InvitationPage",
          "display": "name",
          "name": "nameLike",
          "type": "text"
        }
      ]
    },
    "slots": [{
      "name": "middle-button",
      "is": "DynamicComponent",
      "props": {
        "class": "button",
        "components": [{
          "is": "v-btn",
          "props": {
            "small": true,
            "flat": true,
            "icon": true
          },
          "slots": [{
            "is": "Icon",
            "props": {
              "icon": "add"
            }
          }],
          "events": {
            "click.stop": [{
              "type": "router",
              "otherParams": {
                "func": "push",
                "path": "user/create",
                "base": true
              }
            }]
          }
        }]
      }
    }]
  }]
}