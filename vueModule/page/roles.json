{
  "layout": "MainLayout",
  "components": [{
    "is": "TableTool",
    "props": {
      "reportingKey": "roles",
      "jql": "roles",
      "back": "/admin",
      "icons": [
        {
          "icon": "add",
          "click": [
            {
              "type": "router",
              "otherParams": {
                "func": "push",
                "path": "role/create"
              }
            }
          ]
        }
      ],
      "clickFunction": {
        "type": "router",
        "otherParams": {
          "func": "push",
          "path": "/role/{{id}}"
        }
      },
      "showFilterBar": true,
      "enableShare": false,
      "enableSetting": false,
      "newReport": {
        "configurations": {
          "i18nContext": "Reporting",
          "clickable": true,
          "draggable": true,
          "sorting": {},
          "pagination": {
            "remote": {},
            "current": 0,
            "perPage": 25
          }
        }
      },
      "fieldOptions": [
        {
          "key": "id",
          "hidden": true
        },
        {
          "key": "roleName"
        },
        {
          "key":" roleGroup"
        }
      ]
    }
  }]
}
