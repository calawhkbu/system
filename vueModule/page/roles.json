{
  "fillParent": true,
  "components": [
    {
      "is": "TableTool",
      "props": {
        "reportingKey": "roles",
        "jql": "roles",
        "back": "/admin",
        "clickFunction": {
          "type": "router",
          "otherParams": {
            "func": "push",
            "path": "/role/{{id}}"
          }
        },
        "showFilterBar": true,
        "enableShare": false,
        "enableSetting": false,
        "newReport": {
          "configurations": {
            "i18nContext": "Reporting",
            "clickable": true,
            "draggable": true,
            "sorting": {},
            "pagination": {
              "current": 0,
              "perPage": 20
            }
          }
        },

        "icons": [
          {
            "icon": "add",
            "click": [
              {
                "type": "router",
                "otherParams": {
                  "func": "push",
                  "path": "/role/create"
                }
              }
            ]
          }
        ],


        "filters": [
          {
            "name": "roleName",
            "type": "text"
          }
        ],
        "fieldOptions": [
          {
            "key": "id",
            "sql":{
              "classname":"ColumnExpression","table":"role","name":"id"
            },
            "hidden": true
          },
          {
            "key": "roleName"
          },
          {
            "key": "roleGroup"
          }
        ]
      }
    }
  ]
}
