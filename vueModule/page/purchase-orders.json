{
  "fillParent": true,
  "components": [{
    "is": "TableTool",
    "props": {
      "reportingKey": "purchase-orders",
      "jql": "purchase-orders",
      "hasReports": true,
      "showFilterBar": true,
      "enableShare": true,
      "enableSetting": true,
      "noPartyGroupFilters": false,
      "nextPageComponent": "purchase-order",
      "clickFunction": {
        "type": "router",
        "otherParams": {
          "func": "push",
          "path": "detail/{{id}}",
          "base": true
        }
      },
      "newReport": {
        "filters": {
          "isActive": true,
          "date": {
            "func": {
              "args": {
                "unit": "year"
              },
              "key": "current"
            }
          }
        },
        "configurations": {
          "i18nContext": "PurchaseOrderPage",
          "clickable": true,
          "draggable": true,
          "sorting": {
            "direction": "DESC",
            "key": "updatedAt",
            "sql": {
              "classname": "ColumnExpression",
              "table": "purchase_order",
              "name": "updatedAt"
            }
          },
          "pagination": {
            "current": 0,
            "perPage": 20
          }
        }
      },
      "filters": [],
      "fieldOptions": [{
          "key": "id",
          "sql": {
            "classname": "ColumnExpression",
            "table": "purchase_order",
            "name": "id"
          },
          "hidden": true
        },
        {
          "key": "createdAt",
          "sql": {
            "classname": "ColumnExpression",
            "table": "purchase_order",
            "name": "createdAt"
          },
          "width": 150,
          "format": "YYYY-MM-DD"
        },
        {
          "key": "updatedAt",
          "sql": {
            "classname": "ColumnExpression",
            "table": "purchase_order",
            "name": "updatedAt"
          },
          "width": 150,
          "format": "YYYY-MM-DD"
        }
      ]
    },
    "slots": [{
      "name": "middle-button",
      "is": "DynamicComponent",
      "props": {
        "class": "button",
        "components": [{
            "is": "v-btn",
            "props": {
              "small": true,
              "flat": true,
              "icon": true
            },
            "slots": [{
              "is": "Icon",
              "props": {
                "icon": "add"
              }
            }],
            "events": {
              "click.stop": [{
                "type": "router",
                "otherParams": {
                  "func": "push",
                  "path": "purchase-order/create"
                }
              }]
            }
          },
          {
            "is": "v-btn",
            "props": {
              "small": true,
              "flat": true,
              "icon": true
            },
            "slots": [{
              "is": "Icon",
              "props": {
                "icon": "fas fa-file-import"
              }
            }],
            "events": {
              "click.stop": [{
                "type": "router",
                "otherParams": {
                  "func": "push",
                  "path": "purchase-order/create-import"
                }
              }]
            }
          }
        ]
      }
    }]
  }]
}