{
  "fillParent": true,
  "components": [{
    "is": "SimpleTableTool",
    "props": {
      "reportingKey": "related-parties",
      "jql": "related-parties",
      "reportUrl": "relationship/report/parties/{{ context.detailPageParams }}",
      "back": "/parties",
      "noPartyGroupFilters": true,
      "showFilterBar": true,
      "nextPageKey": "routeParams",
      "clickFunction": {
        "type": "router",
        "otherParams": {
          "func": "push",
          "path": "/{{ partyBId }}",
          "base": true
        }
      },
      "newReport": {
        "filters": {
          "isActive": true,
          "id": 1
        },
        "configurations": {
          "i18nContext": "Reporting",
          "sorting": {},
          "pagination": {
            "current": 0,
            "perPage": 20
          }
        }
      },
      "filters": [
        {
          "name": "shortName",
          "type": "text"
        }
      ],
      "fieldOptions": [
        {
          "key": "partyAId",
          "sql": {
            "classname": "ColumnExpression",
            "table": "related_party",
            "name": "partyAId"
          },
          "hidden": true
        },
        {
          "key": "partyBId",
          "sql": {
            "classname": "ColumnExpression",
            "table": "related_party",
            "name": "partyBId"
          },
          "hidden": true
        },
        {
          "key": "partyAName",
          "sql": {
            "classname": "ColumnExpression",
            "table": "party",
            "name": "name"
          },
          "hidden": true
        },
        {
          "key": "partyBName",
          "sql": {
            "classname": "ColumnExpression",
            "table": "partyB",
            "name": "name"
          }
        },
        {
          "key": "shortName",
          "sql": {
            "classname": "ColumnExpression",
            "table": "partyB",
            "name": "shortName"
          },
          "width": 120
        },
        {
          "key": "groupName",
          "sql": {
            "classname": "ColumnExpression",
            "table": "partyB",
            "name": "groupName"
          },
          "width": 160
        },
        {
          "key": "partyTypes",
          "sql": {
            "classname": "ColumnExpression",
            "table": "related_party",
            "name": "partyTypes"
          }
        },
        {
          "key": "email",
          "sql": {
            "classname": "ColumnExpression",
            "table": "partyB",
            "name": "email"
          }
        },
        {
          "key": "phone",
          "sql": {
            "classname": "ColumnExpression",
            "table": "partyB",
            "name": "phone"
          },
          "width": 160
        },
        {
          "key": "showDelete",
          "label": "",
          "align": "center",
          "clickFunction": {
            "type": "axios",
            "otherParams": {
              "axiosParams": {
                "url": "/relationship/party/{{row.partyAId}}/{{row.partyBId}}",
                "method": "DELETE"
              }
            },
            "afterActions": [
              {
                "type": "emit",
                "otherParams": {
                  "name": "refresh",
                  "params": [true]
                }
              }
            ]
          },
          "tdComponent": {
            "is": "icon",
            "props": {
              "icon": "block"
            }
          },
          "width": 60
        }
      ]
    },
    "slots": [{
      "name": "middle-button",
      "is": "DynamicComponent",
      "props": {
        "class": "button",
        "components": [{
          "is": "v-btn",
          "props": {
            "small": true,
            "flat": true,
            "icon": true
          },
          "slots": [{
            "is": "Icon",
            "props": {
              "icon": "add"
            }
          }],
          "events": {
            "click.stop": [{
              "type": "router",
              "otherParams": {
                "func": "push",
                "path": "/create",
                "base": true
              }
            }]
          }
        }]
      }
    }]
  }]
}