{
  "fillParent": true,
  "components": [{
    "is": "TableTool",
    "props": {
      "title": "API",
      "back": "/admin",
      "urlFormat": "/admin/apis",
      "reportingKey": "apis",
      "reportingKeyKey": "reportIdParams",
      "jql": "apis",
      "hasReports": false,
      "showFilterBar": true,
      "enableShare": false,
      "enableSetting": false,
      "noPartyGroupFilters": true,
      "nextPageComponent": "api",
      "clickFunction": {
        "type": "router",
        "otherParams": {
          "func": "push",
          "path": "/detail/{{id}}",
          "base": true
        }
      },
      "newReport": {
        "filters": {
          "isActive": {
            "value": "active"
          }
        },
        "configurations": {
          "i18nContext": "ApiPage",
          "clickable": true,
          "sorting": {
            "direction": "DESC",
            "key": "updatedAt"
          },
          "pagination": {
            "current": 0,
            "perPage": 20
          }
        }
      },
      "fieldOptions": [{
          "key": "id",
          "hidden": true
        },
        {
          "key": "updatedAt",
          "width": 150,
          "format": "YYYY-MM-DD"
        }
      ],
      "filters": [{
          "name": "isActive",
          "type": "list",
          "props": {
            "items": [{
                "label": "active",
                "value": "active"
              },
              {
                "label": "deleted",
                "value": "deleted"
              }
            ]
          }
        },
        {
          "i18nContext": "APIPage",
          "name": "name",
          "type": "text"
        }
      ]
    },
    "slots": [{
      "name": "middle-button",
      "is": "DynamicComponent",
      "props": {
        "class": "button",
        "components": [{
          "is": "v-btn",
          "props": {
            "small": true,
            "flat": true,
            "icon": true
          },
          "slots": [{
            "is": "Icon",
            "props": {
              "icon": "add"
            }
          }],
          "events": {
            "click.stop": [{
              "type": "router",
              "otherParams": {
                "func": "push",
                "path": "api/create",
                "base": true
              }
            }]
          }
        }]
      }
    }]
  }]
}