{
  "fillParent": true,
  "components": [
    {
      "is": "TableTool",
      "props": {
        "back": "/admin",
        "reportingKey": "locations",
        "jql": "locations",
        "showFilterBar": true,
        "enableShare": false,
        "enableSetting": false,
        "newReport": {
          "filters": {
          },
          "configurations": {
            "i18nContext": "Reporting",
            "clickable": true,
            "draggable": true,
            "sorting": {},
            "expanded" : {},
            "pagination": {
              "current": 0,
              "perPage": 20,
              "remote" : true
            }
          }
        },

        "icons": [
          {
            "icon": "add",
            "click": [
              {
                "type": "router",
                "otherParams": {
                  "func": "push",
                  "path": "/location/create"
                }
              }
            ]
          }
        ],

        "fieldOptions": [
          {
            "key": "id",
            "hidden": true
          }
        ],
        "filters": [
          {
            "name": "countryCode",
            "type": "text"
          }
        ],
        "clickFunction": {
          "type": "router",
          "otherParams": {
            "func": "push",
            "path": "/location/{{id}}"
          }
        }
      }
    }
  ]
}
