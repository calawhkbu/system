{
  "layout": "MainLayout",
  "components": [{
    "is": "TableTool",
    "props": {
      "clickFunction": {
        "type": "router",
        "otherParams": {
          "func": "push",
          "path": "/{{tableName}}/{{primaryKey}}"
        }
      },
      "showFilterBar": true,
      "enableShare": false,
      "enableSetting": false,
      "reportingKey": "alerts",
      "urlFormat": "/alerts{{ id|if('/', '') }}{{ id }}",
      "newReport": {
        "configurations": {
          "i18nContext": "Reporting",
          "clickable": true,
          "draggable": true,
          "sorting": {},
          "pagination": {
            "remote": {},
            "current": 0,
            "perPage": 25
          }
        }
      }
    }
  }]
}
