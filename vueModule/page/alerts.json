{
  "layout": "MainLayout",
  "components": [
    {
      "is": "TableTool",
      "props": {
        "showFilterBar": true,
        "enableShare": false,
        "enableSetting": false,
        "reportingKey": "alerts",
        "jql": "alerts",
        "urlFormat": "/alerts{{ id|if('/', '') }}{{ id }}",
        "newReport": {
          "configurations": {
            "i18nContext": "Reporting",
            "clickable": true,
            "draggable": true,
            "sorting": {},
            "pagination": {
              "current": 0,
              "perPage": 20
            }
          }
        },
        "fieldOptions": [
          {
            "key": "tableName",
            "sql": "alert.tableName",
            "hidden": true
          },
          {
            "key": "primaryKey",
            "sql": "alert.primaryKey",
            "hidden": true
          }
        ],
        "clickFunction": {
          "type": "router",
          "otherParams": {
            "func": "push",
            "path": "/{{tableName}}/{{primaryKey}}"
          }
        }
      }
    }
  ]
}
