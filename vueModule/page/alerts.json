{
  "layout": "MainLayout",
  "components": [{
    "is": "TableTool",
    "props": {
      "reportingKey": "alerts",
      "jql": "alerts",
      "urlFormat": "/alerts{{ id|if('/', '') }}{{ id }}",
      "hasReports": true,
      "showFilterBar": true,
      "enableShare": true,
      "enableSetting": true,
      "newReport": {
        "filters": {
          "isActive": true
        },
        "configurations": {
          "i18nContext": "Alert",
          "clickable": true,
          "draggable": true,
          "sorting": {
            "direction": "DESC",
            "key": "alert.updatedAt"
          },
          "pagination": {
            "current": 0,
            "perPage": 20
          }
        }
      },
      "fieldOptions": [{
          "key": "tableName",
          "sql": "alert.tableName",
          "hidden": true
        },
        {
          "key": "primaryKey",
          "sql": "alert.primaryKey",
          "hidden": true
        },
        {
          "key": "alertCategory",
          "i18nContext": "Alert"
        },
        {
          "key": "alertType",
          "i18nContext": "Alert"
        }
      ],
      "filters": [],
      "clickFunction": {
        "type": "router",
        "otherParams": {
          "func": "push",
          "path": "/booking/{{id}}"
        }
      },
      "icons": []
    }
  }]
}