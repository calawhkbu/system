{
  "fillParent": true,
  "components": [{
    "is": "TableTool",
    "props": {
      "reportingKey": "alerts",
      "jql": "alerts",
      "showDefault": true,
      "hasReports": false,
      "showFilterBar": true,
      "enableShare": true,
      "enableSetting": false,
      "noPartyGroupFilters": false,
      "newReport": {
        "filters": {
          "isActive": {
            "value": "active"
          },
          "entityType": {
            "value": "shipment"
          }
        },
        "configurations": {
          "i18nContext": "AlertPage",
          "clickable": true,
          "draggable": true,
          "sorting": {
            "direction": "DESC",
            "key": "updatedAt",
            "sql": {
              "classname": "ColumnExpression",
              "table": "alert",
              "name": "updatedAt"
            }
          },
          "pagination": {
            "current": 0,
            "perPage": 20,
            "remote": true
          }
        }
      },
      "fieldOptions": [{
          "key": "tableName",
          "hidden": true
        },
        {
          "key": "primaryKey",
          "hidden": true
        },

        {
          "key": "alertTitle",
          "i18nOptions": "return (row && row.alertContent && row.alertContent.entity)  ? ({ entity : row.alertContent.entity }) : ({})"
        },

        {
          "key": "alertMessage",
          "i18nOptions": "return (row && row.alertContent && row.alertContent.entity)  ? ({ entity : row.alertContent.entity }) : ({})"
        },

        {
          "key": "alertCategory"
        },
        {
          "key": "alertType"
        },
        {
          "key": "alertContent",
          "hidden": true
        },

        {
          "key": "alertUpdatedAt",
          "hidden": true
        },
        {
          "key": "alertCreatedAt",
          "hidden": true
        }
      ],
      "filters": [{
          "display": "entityType",
          "name": "entityType",
          "props": {
            "items": [{
                "label": "booking",
                "value": "booking"
              },
              {
                "label": "shipment",
                "value": "shipment"
              }
            ],
            "required": true
          },
          "type": "list"
        },


        {
          "name": "alertUpdatedAt",
          "type": "date"
        },
        {
          "name": "alertCreatedAt",
          "type": "date"
        },
        {
          "display": "date",
          "name": "date",
          "type": "date",
          "props": {
            "required": false
          }

        }

      ],
      "clickFunction": {
        "type": "router",
        "otherParams": {
          "func": "push",
          "path": "alerts/default/{{tableName}}/{{primaryKey}}"
        }
      },
      "icons": []
    }
  }]
}