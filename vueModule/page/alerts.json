{
  "fillParent": true,
  "components": [{
    "is": "TableTool",
    "props": {
      "reportingKey": "alerts",
      "jql": "alerts",
      "urlFormat": "/alerts{{ id|if('/', '') }}{{ id }}",
      "hasReports": false,
      "showFilterBar": true,
      "noPartyGroupFilters": true,
      "enableShare": true,
      "enableSetting": true,
      "newReport": {
        "filters": {
          "isActive": true
        },
        "configurations": {
          "i18nContext": "Alert",
          "clickable": true,
          "draggable": true,
          "sorting": {
            "direction": "DESC",
            "key": "alert.updatedAt"
          },
          "pagination": {
            "current": 0,
            "perPage": 20
          }
        }
      },
      "fieldOptions": [{
          "key": "tableName",
          "sql": {
            "classname": "ColumnExpression",
            "table": "alert",
            "name": "tableName"
          },
          "hidden": true
        },
        {
          "key": "primaryKey",
          "sql": {
            "classname": "ColumnExpression",
            "table": "alert",
            "name": "primaryKey"
          },
          "hidden": true
        },
        {
          "key": "alertCategory",
          "i18nContext": "Alert"
        },
        {
          "key": "alertType",
          "i18nContext": "Alert",
          "sql": {
            "classname": "FunctionExpression",
            "name": "CONCAT",
            "parameters": [{
                "classname": "ColumnExpression",
                "table": "alert",
                "name": "alertType"
              },
              "Title"
            ]
          }
        },
        {
          "key": "alertContent",
          "i18nContext": "Alert",
          "sql": {
            "classname": "FunctionExpression",
            "name": "IFNULL",
            "parameters": [{
                "classname": "FunctionExpression",
                "name": "JSON_UNQUOTE",
                "parameters": [{
                  "classname": "FunctionExpression",
                  "name": "JSON_EXTRACT",
                  "parameters": [{
                      "classname": "ColumnExpression",
                      "table": "flex_data",
                      "name": "data"
                    },
                    "$.customMessage"
                  ]
                }]
              },
              {
                "classname": "FunctionExpression",
                "name": "CONCAT",
                "parameters": [{
                    "classname": "ColumnExpression",
                    "table": "alert",
                    "name": "alertType"
                  },
                  "Message"
                ]
              }
            ]
          }
        }
      ],
      "filters": [],
      "clickFunction": {
        "type": "router",
        "otherParams": {
          "func": "push",
          "path": "/booking/{{id}}"
        }
      },
      "icons": []
    }
  }]
}