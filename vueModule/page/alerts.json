{
  "fillParent": true,
  "components": [
    {
      "is": "TableTool",
      "props": {
        "reportingKey": "alerts",
        "jql": "alerts",
        "hasReports": true,
        "showFilterBar": true,
        "enableShare": true,
        "enableSetting": true,
        "noPartyGroupFilters": false,
        "newReport": {
          "filters": {
            "activeStatus": {
              "value": "active"
            },
            "entityType": {
              "value": "shipment"
            },
            "alertCreatedAt": {
              "func": {
                "args": {
                  "unit": "week"
                },
                "key": "current"
              }
            }
          },
          "configurations": {
            "checkbox": {},
            "i18nContext": "AlertPage",
            "clickable": true,
            "draggable": true,
            "sorting": {
              "direction": "DESC",
              "key": "updatedAt",
              "sql": {
                "classname": "ColumnExpression",
                "table": "alert",
                "name": "updatedAt"
              }
            },
            "pagination": {
              "current": 0,
              "perPage": 20,
              "remote": true
            }
          }
        },
        "fieldOptions": [
          {
            "key": "alertId",
            "hidden": true
          },
          {
            "key": "checkbox",
            "checkbox": true,
            "showCheckAll": true
          },
          {
            "key": "alertTableName",
            "hidden": true
          },
          {
            "key": "alertPrimaryKey",
            "hidden": true
          },
          {
            "key": "alertTitle",
            "i18nOptions": "return (row && row.alertContent && row.alertContent.entity)  ? ({ entity : row.alertContent.entity }) : ({})"
          },
          {
            "key": "alertMessage",
            "i18nOptions": "return (row && row.alertContent && row.alertContent.entity)  ? ({ entity : row.alertContent.entity }) : ({})"
          },
          {
            "key": "alertCategory"
          },
          {
            "key": "alertType"
          },
          {
            "key": "alertContent",
            "hidden": true
          },
          {
            "key": "alertUpdatedAt",
            "hidden": true
          },
          {
            "key": "alertCreatedAt",
            "hidden": true
          }
        ],
        "filters": [
          {
            "name": "alertType",
            "type": "autocomplete",
            "props": {
              "api": {
                "query": {
                  "url": "api/alert/alertType/get",
                  "data": {
                    "limit": 5,
                    "subqueries": {
                      "q": {
                        "value": "{{ q }}"
                      }
                    }
                  },
                  "method": "POST",
                  "labelKey": "alertTitle",
                  "valueKey": "alertType",
                  "subLabelKey": "alertType"
                },
                "resolve": {
                  "url": "api/alert/alertType/get",
                  "data": {
                    "subqueries": {
                      "alertType": {
                        "value": "{{value}}"
                      }
                    }
                  },
                  "method": "POST",
                  "labelKey": "alertTitle",
                  "valueKey": "alertType",
                  "subLabelKey": "alertType"
                }
              },
              "threshold": 0,
              "showAllIfEmpty" : true,
              "multi": true
            },
            "display": "alertType"
          },
          {
            "display": "alertStatus",
            "name": "alertStatus",
            "props": {
              "items": [
                {
                  "label": "open",
                  "value": "open"
                },
                {
                  "label": "closed",
                  "value": "closed"
                }
              ]
            },
            "type": "list"
          },
          {
            "display": "entityType",
            "name": "entityType",
            "props": {
              "items": [
                {
                  "label": "booking",
                  "value": "booking"
                },
                {
                  "label": "shipment",
                  "value": "shipment"
                }
              ],
              "required": true
            },
            "type": "list"
          },
          {
            "name": "alertUpdatedAt",
            "type": "date"
          },
          {
            "name": "alertCreatedAt",
            "type": "date",
            "props": {
              "required": true
            }
          },
          {
            "display": "date",
            "name": "date",
            "type": "date",
            "props": {
              "required": false
            }
          }
        ],
        "clickFunction": {
          "type": "router",
          "otherParams": {
            "base": true,
            "func": "push",
            "path": "{{alertTableName}}/{{alertPrimaryKey}}"
          }
        },
        "icons": []
      },
      "slots": [
        {
          "name": "middle-button",
          "is": "ReportHandleAlertDialogButton",
          "props": {}
        }
      ]
    }
  ]
}