{
  "layout": "MainLayout",
  "components": [{
    "is": "TableTool",
    "props": {
      "showFilterBar": true,
      "enableShare": false,
      "enableSetting": false,
      "reportingKey": "alerts",
      "jql": "alerts",
      "urlFormat": "/alerts{{ id|if('/', '') }}{{ id }}",
      "newReport": {
        "configurations": {
          "i18nContext": "Reporting",
          "clickable": true,
          "draggable": true,
          "sorting": {},
          "pagination": {
            "current": 0,
            "perPage": 25
          }
        }
      },
      "fieldOptions": [
        {
          "key": "id",
          "hidden": true
        },
        {
          "key": "tableName",
          "hidden": true
        },
        {
          "key": "primaryKey",
          "hidden": true
        },
        {
          "key": "createdAt",
          "datetime": "YYYY-MM-DD HH:mm:ss"
        },
        {
          "key": "updatedAt",
          "datetime": "YYYY-MM-DD HH:mm:ss"
        }
      ],
      "clickFunction": {
        "type": "router",
        "otherParams": {
          "func": "push",
          "path": "/{{tableName}}/{{primaryKey}}"
        }
      }
    }
  }]
}
