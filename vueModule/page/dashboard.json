                      "key": "last",
                      "args": {
                        "unit": "year"
                      }
                    }
                  },
                  {
                    "label": "last 3 month",
                    "func": {
                      "key": "last",
                      "args": {
                        "value": 3,
                        "unit": "month"
                      }
                    }
                  }
                ]
              },
              {
                "label": "coming",
                "shortcuts": [{
                  "label": "coming 7 days",
                  "func": {
                    "key": "coming",
                    "args": {
                      "value": 7,
                      "unit": "day",
                      "startFromNow": true
                    }
                  }
                }]
              },
              {
                "label": "past",
                "shortcuts": [{
                  "label": "past 7 days",
                  "func": {
                    "key": "past",
                    "args": {
                      "value": 7,
                      "unit": "day"
                    }
                  }
