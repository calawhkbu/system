{
  "layout": "MainLayout",
  "components": [{
    "is": "TableTool",
    "props": {
      "reportingKey": "templates",
      "jql": "templates",
      "showFilterBar": true,
      "enableShare": false,
      "enableSetting": false,
      "newReport": {
        "filters": {
          "fileType": {
            "value": "excel"
          },
          "isActive": true
        },
        "configurations": {
          "i18nContext": "Reporting",
          "clickable": true,
          "draggable": true,
          "sorting": {},
          "pagination": {
            "remote": {},
            "current": 0,
            "perPage": 25
          }
        }
      },
      "fieldOptions": [
        {
          "key": "id",
          "hidden": true
        }
      ],
      "filters": [
        {
          "name": "isActive"
        },
        {
          "name": "templateName",
          "type": "text"
        }
      ],
      "clickFunction": {
        "type": "router",
        "otherParams": {
          "func": "push",
          "path": "/template/{{id}}"
        }
      }
    }
  }]
}
