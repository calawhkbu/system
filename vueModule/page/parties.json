{
  "fillParent": true,
  "components": [{
    "is": "TableTool",
    "props": {
      "title": "Relationship",
      "reportingKey": "parties",
      "jql": "parties",
      "hasReports": false,
      "showFilterBar": true,
      "enableShare": false,
      "enableSetting": false,
      "noPartyGroupFilters": true,
      "nextPageKey": "reportIdParams",
      "newReport": {
        "filters": {
          "isActive": true,
          "parties": true,
          "contacts": true,
          "showInfo": true
        },
        "configurations": {
          "i18nContext": "RelationshipPage",
          "sorting": {
            "direction": "DESC",
            "key": "updatedAt",
            "sql": {
              "classname": "ColumnExpression",
              "table": "party",
              "name": "name"
            }
          },
          "pagination": {
            "remote": true,
            "current": 0,
            "perPage": 20
          }
        }
      },
      "filters": [{
        "name": "shortName",
        "type": "text"
      }, {
        "name": "name",
        "type": "text"
      }, {
        "name": "groupName",
        "type": "text"
      }],
      "fieldOptions": [{
          "key": "id",
          "sql": {
            "classname": "ColumnExpression",
            "table": "party",
            "name": "id"
          },
          "hidden": true
        },
        {
          "key": "name",
          "width": 300
        },
        {
          "key": "shortName",
          "width": 150
        },
        {
          "key": "groupName",
          "width": 150
        },
        {
          "key": "partyTypes",
          "width": 200
        },
        {
          "key": "parties",
          "clickFunction": {
            "type": "router",
            "otherParams": {
              "func": "push",
              "path": "relatedParties/{{ row.id }}",
              "base": true
            }
          },
          "width": 80
        },
        {
          "key": "contacts",
          "clickFunction": {
            "type": "router",
            "otherParams": {
              "func": "push",
              "path": "relatedPersons/{{ row.id }}",
              "base": true
            }
          },
          "width": 80
        },
        {
          "key": "showInfo",
          "label": "",
          "tdComponent": {
            "is": "icon",
            "props": {
              "icon": "edit"
            }
          },
          "clickFunction": {
            "type": "router",
            "otherParams": {
              "func": "push",
              "path": "party/detail/{{ row.id }}",
              "base": true
            }
          },
          "width": 60
        }
      ]
    },
    "slots": [{
      "name": "middle-button",
      "is": "DynamicComponent",
      "props": {
        "class": "button",
        "components": [{
          "is": "v-btn",
          "props": {
            "small": true,
            "flat": true,
            "icon": true
          },
          "slots": [{
            "is": "Icon",
            "props": {
              "icon": "add"
            }
          }],
          "events": {
            "click.stop": [{
              "type": "router",
              "otherParams": {
                "func": "push",
                "path": "party/create/create",
                "base": true
              }
            }]
          }
        }]
      }
    }]
  }]
}