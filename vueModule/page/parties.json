{
  "components": [
    {
      "is": "TableTool",
      "props": {
        "reportingKey": "connections",
        "jql": "parties",
        "showFilterBar": true,
        "enableShare": false,
        "enableSetting": false,
        "newReport": {
          "filter": {
            "isActive": true
          },
          "configurations": {
            "i18nContext": "Reporting",
            "clickable": true,
            "draggable": true,
            "sorting": {},
            "pagination": {
              "current": 0,
              "perPage": 20
            }
          }
        },
        "filters": [
          {
            "name": "name",
            "type": "text"
          }
        ],
        "fieldOptions": [
          {
            "key": "id",
            "sql": "party.id"
          }
        ],
        "_clickFunction": {
          "type": "router",
          "otherParams": {
            "func": "push",
            "path": "/party/{{id}}"
          }
        }
      }
    }
  ]
}
