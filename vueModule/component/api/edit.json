{
  "class": "grid",
  "components": [{
    "is": "DynamicComponent",
    "props": {
      "class": "xs-12 grid min-height-28 align-item-center full-width",
      "components": [{
        "is": "v-btn",
        "props": {
          "small": true,
          "flat": true,
          "icon": true
        },
        "slots": [{
          "is": "Icon",
          "props": {
            "icon": "arrow_back"
          }
        }],
        "events": {
          "click.stop": [{
            "type": "router",
            "otherParams": {
              "func": "push",
              "path": "/apis"
            }
          }]
        }
      }, {
        "is": "I18nText",
        "props": {
          "i18nContext": "ApiPage",
          "i18nKey": "title",
          "i18nOptions": {
            "id": "{{context.$store.state.widget.data.name}}"
          }
        }
      }, {
        "is": "v-spacer"
      }, {
        "is": "v-btn",
        "props": {
          "small": true,
          "icon": true,
          "flat": true,
          "class": "primary white-text"
        },
        "slots": [{
          "is": "Icon",
          "props": {
            "small": true,
            "icon": "edit"
          }
        }],
        "events": {
          "click.stop": [{
            "type": "dispatch",
            "otherParams": {
              "name": "widget/changeMode",
              "mode": "edit"
            }
          }]
        }
      }]
    }
  }, {
    "is": "DynamicComponent",
    "props": {
      "layout": "v-card",
      "class": "xs-12 margin-bottom-8",
      "components": [{
        "is": "WidgetForm",
        "props": {
          "class": "xs-12",
          "fields": [{
              "label": "ApiPage.name",
              "name": "name",
              "component": "v-text-field",
              "class": "xs-12"
            },
            {
              "label": "ApiPage.password",
              "name": "password",
              "component": "v-text-field",
              "class": "xs-12"
            }
          ]
        }
      }]
    }
  }]
}