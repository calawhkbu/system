{
  "class": "grid",
  "components": [{
    "is": "DynamicComponent",
    "props": {
      "class": "xs-12 grid min-height-28 align-item-center full-width padding-8",
      "components": [{
        "is": "v-btn",
        "props": {
          "small": true,
          "flat": true,
          "icon": true
        },
        "slots": [{
          "is": "Icon",
          "props": {
            "icon": "arrow_back"
          }
        }],
        "events": {
          "click.stop": [{
            "type": "router",
            "otherParams": {
              "func": "push",
              "path": "/admin/apis"
            }
          }]
        }
      }, {
        "is": "I18nText",
        "props": {
          "i18nContext": "ApiPage",
          "i18nKey": "title",
          "i18nOptions": {
            "id": "{{context.$store.state.widget.data.name}}"
          }
        }
      }, {
        "is": "v-spacer"
      }, {
        "is": "v-btn",
        "props": {
          "small": true,
          "icon": true,
          "flat": true,
          "class": "primary white-text"
        },
        "slots": [{
          "is": "Icon",
          "props": {
            "small": true,
            "icon": "edit"
          }
        }],
        "events": {
          "click.stop": [{
            "type": "dispatch",
            "otherParams": {
              "name": "widget/changeMode",
              "mode": "edit"
            }
          }]
        }
      }]
    }
  }, {
    "is": "DynamicComponent",
    "props": {
      "layout": "v-card",
      "class": "xs-12 margin-bottom-2",
      "components": [{
        "is": "WidgetForm",
        "props": {
          "class": "xs-12",
          "fields": [{
            "label": "ApiPage.name",
            "name": "name",
            "component": "v-text-field",
            "class": "xs-12"
          }]
        }
      }]
    }
  }, {
    "is": "DynamicComponent",
    "props": {
      "layout": "v-card",
      "class": "margin-bottom-4 full-width",
      "components": [{
        "is": "FrontendWidgetHelper",
        "props": {
          "refreshToken": "$store.state.widget.data.refreshToken",
          "html": [
            "<html>",
            "  <head>",
            "    <meta charset=\"utf-8\">",
            "    <script src=\"https://unpkg.com/vue\"></script>",
            "    <script src=\"{{frontendUrl}}/widget/swivel-widget.js\"></script>",
            "  </head>",
            "  <body>",
            "    <swivel-widget",
            "      apiToken=\"{{value}}\"",
            "      name=\"{{name}}\"",
            "      {% if data %}data='{{data|safe}}'{% endif %}",
            "    />",
            "  </body>",
            "</html>"
          ],
          "widgetItems": [{
            "text": "Search Tracking Widget",
            "name": "SearchBillWidget"
          }, {
            "text": "Login Widget",
            "name": "LoginWidget"
          }, {
            "text": "Simple Tracking Widget",
            "name": "BillTrackingWidget",
            "data": "{ \"masterNo\": \"\", \"containerNo\": \"\" }"
          }]
        }
      }]
    }
  }, {
    "is": "DynamicComponent",
    "props": {
      "layout": "v-card",
      "class": "margin-bottom-2 full-width",
      "components": [{
        "is": "BackendApiHelper",
        "props": {
          "refreshToken": "$store.state.widget.data.refreshToken"
        }
      }]
    }
  }]
}