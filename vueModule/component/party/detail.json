{
  "class": "grid",
  "components": [{
    "is": "DynamicComponent",
    "props": {
      "class": "xs-12 grid min-height-28 align-item-center full-width",
      "components": [{
        "is": "v-btn",
        "props": {
          "small": true,
          "flat": true,
          "icon": true
        },
        "slots": [{
          "is": "Icon",
          "props": {
            "icon": "arrow_back"
          }
        }],
        "events": {
          "click.stop": [{
            "type": "router",
            "otherParams": {
              "func": "push",
              "path": "/parties"
            }
          }]
        }
      }, {
        "is": "I18nText",
        "props": {
          "i18nContext": "PartyPage",
          "i18nKey": "title",
          "i18nOptions": {
            "id": "{{context.$store.state.widget.data.name}}"
          }
        }
      }, {
        "is": "v-spacer"
      }, {
        "is": "v-btn",
        "props": {
          "small": true,
          "icon": true,
          "flat": true,
          "class": "primary white-text"
        },
        "slots": [{
          "is": "Icon",
          "props": {
            "small": true,
            "icon": "edit"
          }
        }],
        "events": {
          "click.stop": [{
            "type": "dispatch",
            "otherParams": {
              "name": "widget/changeMode",
              "mode": "edit"
            }
          }]
        }
      }]
    }
  }, {
    "is": "DynamicComponent",
    "props": {
      "layout": "v-card",
      "class": "xs-12 margin-bottom-8",
      "components": [{
        "is": "WidgetForm",
        "props": {
          "class": "xs-12",
          "fields": [{
              "label": "PartyPage.partyName",
              "name": "name",
              "component": "v-text-field",
              "class": "xs-12"
            }, {
              "label": "PartyPage.partyShortName",
              "name": "shortName",
              "component": "v-text-field",
              "class": "xs-12"
            }, {
              "label": "PartyPage.groupName",
              "name": "groupName",
              "component": "v-text-field",
              "class": "xs-12"
            },
            {
              "label": "PartyPage.email",
              "name": "email",
              "component": "v-text-field",
              "props": {
                "type": "email"
              },
              "class": "xs-3"
            },
            {
              "label": "PartyPage.phone",
              "name": "phone",
              "component": "v-text-field",
              "class": "xs-3"
            },
            {
              "label": "PartyPage.fax",
              "name": "fax",
              "component": "v-text-field",
              "class": "xs-3"
            },
            {
              "label": "PartyPage.identity",
              "name": "identity",
              "component": "v-text-field",
              "class": "xs-3"
            },
            {
              "label": "PartyPage.address",
              "name": "address",
              "component": "v-textarea",
              "props": {
                "auto-grow": true,
                "rows": 5
              },
              "class": "xs-12"
            },
            {
              "label": "PartyPage.city",
              "name": "cityCode",
              "component": "AsyncAutoSuggest",
              "relation": "cityCode",
              "relationMore": [{
                "name": "cityCode",
                "selector": "locationCode"
              }],
              "props": {
                "canCreate": true,
                "canCreateValue": {
                  "id": null,
                  "code": "{{autoSuggestModel}}"
                },
                "searchTextParams": {
                  "url": "api/location/query/location",
                  "method": "POST",
                  "data": {
                    "subqueries": {
                      "q": {
                        "value": "{{context.search}}"
                      }
                    },
                    "limit": 5
                  }
                },
                "item-text": "name",
                "item-value": "locationCode"
              },
              "class": "xs-3"
            },
            {
              "label": "PartyPage.state",
              "name": "stateCode",
              "component": "AsyncAutoSuggest",
              "relation": "stateCode",
              "relationMore": [{
                "name": "stateCode",
                "selector": "code"
              }],
              "props": {
                "canCreate": true,
                "canCreateValue": {
                  "id": null,
                  "code": "{{autoSuggestModel}}"
                },
                "searchTextParams": {
                  "url": "api/code/query/code_master",
                  "method": "POST",
                  "data": {
                    "subqueries": {
                      "codeType": {
                        "value": "STATE-PROVINCE"
                      },
                      "q": {
                        "value": "{{context.search}}"
                      }
                    },
                    "limit": 5
                  }
                },
                "item-text": "name",
                "item-value": "code"
              },
              "class": "xs-3"
            },
            {
              "label": "PartyPage.country",
              "name": "countryCode",
              "component": "AsyncAutoSuggest",
              "relation": "countryCode",
              "relationMore": [{
                "name": "countryCode",
                "selector": "code"
              }],
              "props": {
                "canCreate": true,
                "canCreateValue": {
                  "id": null,
                  "code": "{{autoSuggestModel}}"
                },
                "searchTextParams": {
                  "url": "api/code/query/code_master",
                  "method": "POST",
                  "data": {
                    "subqueries": {
                      "codeType": {
                        "value": "COUNTRY"
                      },
                      "q": {
                        "value": "{{context.search}}"
                      }
                    },
                    "limit": 5
                  }
                },
                "item-text": "name",
                "item-value": "code"
              },
              "class": "xs-3"
            },
            {
              "label": "PartyPage.zip",
              "name": "zip",
              "component": "v-text-field",
              "class": "xs-3"
            }
          ]
        }
      }]
    }
  }, {
    "is": "DynamicComponent",
    "props": {
      "layout": "v-card",
      "class": "xs-12 margin-topbottom-8",
      "components": [{
        "is": "WidgetForm",
        "props": {
          "class": "xs-12",
          "fields": [{
            "label": "PartyPage.thirdPartyCode",
            "name": "thirdPartyCode",
            "component": "JsonField",
            "class": "xs-12 min-height-270",
            "hint": "PartyPage.thirdPartyCodeHint",
            "props": {
              "editorJson": {
                "height": "200px",
                "options": {
                  "mode": "code"
                }
              }
            }
          }]
        }
      }]
    }
  }, {
    "is": "DynamicComponent",
    "props": {
      "layout": "v-card",
      "class": "xs-12 margin-topbottom-8",
      "components": [{
        "is": "WidgetForm",
        "props": {
          "class": "xs-12",
          "fields": [{
            "label": "PartyPage.types",
            "name": "types",
            "component": "TableField",
            "class": "xs-12",
            "props": {
              "fields": [{
                "label": "PartyPage.type",
                "name": "type",
                "component": "v-text-field",
                "class": "xs-12"
              }]
            }
          }]
        }
      }]
    }
  }]
}