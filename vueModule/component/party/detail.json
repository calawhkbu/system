{
  "class": "grid",
  "components": [{
    "is": "DynamicComponent",
    "props": {
      "class": "xs-12 grid min-height-28 align-item-center full-width padding-2",
      "components": [{
        "is": "v-btn",
        "props": {
          "class": "margin-0",
          "small": true,
          "flat": true,
          "icon": true
        },
        "slots": [{
          "is": "Icon",
          "props": {
            "icon": "arrow_back"
          }
        }],
        "events": {
          "click.stop": [{
            "type": "router",
            "otherParams": {
              "func": "push",
              "path": "/{{ p[0] }}/{{ p[1] }}"
            }
          }]
        }
      }, {
        "is": "I18nText",
        "props": {
          "i18nContext": "PartyPage",
          "i18nKey": "title",
          "i18nOptions": {
            "id": "{{context.$store.state.widget.data.name}}"
          }
        }
      }, {
        "is": "v-spacer"
      }, {

      }, {
        "is": "ToolTip",
        "props": {
          "bottom": true,
          "activatorComponents": {
            "layout": "span",
            "components": [{
              "is": "v-btn",
              "props": {
                "small": true,
                "flat": true,
                "icon": true,
                "class": "primary white-text"
              },
              "slots": [{
                "is": "Icon",
                "props": {
                  "icon": "edit"
                }
              }],
              "events": {
                "click.stop": [{
                  "type": "dispatch",
                  "otherParams": {
                    "name": "widget/changeMode",
                    "mode": "edit"
                  }
                }]
              }
            }]
          }
        },
        "mainComponents": {
          "layout": "span",
          "components": [{
            "is": "I18nText",
            "props": {
              "i18nKey": "edit"
            }
          }]
        }
      }]
    }
  }, {
    "is": "DynamicComponent",
    "props": {
      "class": "xs-12 grid full-width",
      "components": [{
        "is": "AsyncComponent",
        "props": {
          "layoutName": "party/base/general"
        }
      }, {
        "is": "AsyncComponent",
        "props": {
          "layoutName": "party/base/contact"
        }
      }, {
        "is": "AsyncComponent",
        "props": {
          "layoutName": "party/base/address"
        }
      }, {
        "is": "AsyncComponent",
        "props": {
          "layoutName": "party/base/external"
        }
      }, {
        "is": "AsyncComponent",
        "props": {
          "layoutName": "party/base/roles"
        }
      }]
    }
  }]
}