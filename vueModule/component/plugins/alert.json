{
  "layout": "Plugin",
  "name": "Alert",
  "transition": "slide-y-reverse-transition",
  "lazy": true,
  "hide-overlay": true,
  "remote": true,
  "shareProps": {
    "i18nContext": "Alert",
    "iconType": "",
    "icon": "alertCategory",
    "iconMap": {
      "Exception": "notification_important",
      "Message": "message"
    },
    "titleKey": "alertTitle",
    "titleI18nOptionsKeys": ["flexData"],
    
    "subTitleKeys": ["alertMessage"],
    "subTitleI18nOptionsKeys": ["flexData"],
    "noIcon": "notifications_none"
  },
  "baseSummaryProps": {
    "size": -3
  },
  "baseDetailProps": {
    "avaliableAlertTab": {
      "Open": "return data.filter(x => x.status === 'open')",
      "StayAlert": "return data.filter(x => x.status === 'open' && !!x.stayAlert)",
      "Closed": "return data.filter(x => x.status === 'closed')",
      "All": "return data"
    }
  }
}