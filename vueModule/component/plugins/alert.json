{
  "layout": "Plugin",
  "name": "Alert",
  "transition": "slide-y-reverse-transition",
  "loadWhenDataLoaded": true,
  "lazy": true,
  "hide-overlay": true,
  "remote": true,
  "shareProps": {
    "i18nContext": "Alert",
    "iconType": "",
    "icon": "alertCategory",
    "iconMap": {
      "Exception": "notification_important",
      "Message": "message"
    },
    "titleKey": "alertType",
    "subTitleKeys": ["alertContent"],
    "noIcon": "notifications_none"
  },
  "baseSummaryProps": {
    "size": -3
  },
  "baseDetailProps": {
    "changeMessageStatusAxiosParams": {
      "method": "POST",
      "url": "api/alert/updateStatus/{{id}}"
    },
    "selects": {
      "All": "return data",
      "Exception": "return data.filter(m => m.alertCategory === 'Exception')",
      "Message": "return data.filter(m => m.alertCategory === 'Message' || m.alertCategory === 'Notification')"
    }
  }
}
