{
  "layout": "Plugin",
  "lazy": true,
  "transition": "slide-y-reverse-transition",
  "name": "Alert",
  "remote": true,
  "loadWhenDataLoaded": true,
  "axiosBaseParams": [
    "const tableName = encodeURIComponent(context.$route.params.page);",
    "const primaryKey = encodeURIComponent(context.$route.params.params);",
    "return {",
    "url: 'api/alert/list/history',",
    "method: 'POST',",
    "data: { tableName, primaryKey }",
    "};"
  ],
  "loopGetData": 60000,
  "shareProps": {
    "i18nContext": "Alert",
    "iconType": "",
    "icon": "alertCategory",
    "iconMap": {
      "Exception": "notification_important",
      "Message": "message"
    },
    "titleKey": "alertType",
    "subTitleKeys": ["alertContent"],
    "noIcon": "notifications_none"
  },
  "baseSummaryProps": {
    "size": -3
  },
  "baseDetailProps": {}
}