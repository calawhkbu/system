{
  "layout": "Plugin",
  "lazy": true,
  "width": "50%",
  "name": "Tracking",
  "remote": true,
  "loadWhenDataLoaded": true,
  "axiosBaseParams": [
    "const data = context.$store.state.widget.data;",
    "return {",
    "url: `api/tracking/getold`,",
    "method: 'post',",
    "data: {",
    "masterNo: data.masterNo,",
    "carrierBookingNo: data.billContainers.map(c => c.carrierBookingNo),",
    "containerNo: data.billContainers.map(c => c.containerNo)",
    "}",
    "}"
  ],
  "shareProps": {
    "icon": "statusCode",
    "titleKey": {
      "key": "key",
      "track": true
    },
    "subTitleKeys": [
      "statusDescription",
      "statusPlace",
      {
        "key": "statusDate",
        "is": "Datetime",
        "format": "YYYY-MM-DD HH:mm:ss"
      }
    ],
    "iconMap": {
      "BKCF": "tracking/bkcf",
      "STSP": "tracking/stsp",
      "EPRL": "tracking/eprl",
      "GITM": "tracking/cy-cutoff",
      "LOBD": "tracking/lobd",
      "DLPT": "tracking/dlpt",
      "TSLB": "tracking/lobd",
      "TSBA": "tracking/cy-cutoff",
      "BDAR": "tracking/dlpt",
      "DSCH": "tracking/lobd",
      "DECL": "tracking/sumit-so-date",
      "PASS": "tracking/cus",
      "TMPS": "tracking/sale-invoice-date",
      "STCS": "tracking/stcs",
      "RCVE": "tracking/eprl",
      "BKD": "tracking/bkcf",
      "RCS": "tracking/rcs",
      "MNF": "tracking/eprl",
      "DEP": "tracking/airplane-takeoff",
      "TAR": "tracking/airplane-landing",
      "TDE": "tracking/airplane-takeoff",
      "ARR": "tracking/airplane-landing",
      "RWB": "tracking/sale-invoice-date",
      "RCF": "tracking/rcf",
      "CUS": "tracking/cus",
      "NFD": "tracking/nfd",
      "MAN": "tracking/man",
      "DLV": "tracking/stsp",
      "CANF": "tracking/canf",
      "ERR": "tracking/err",
      "bookingReceivedByForwarder": "tracking/paperwork",
      "oceanBill": "tracking/sumit-so-date",
      "SEAdeparture": "tracking/ship-departure",
      "SEAarrival": "tracking/ship-arrival",
      "AIRdeparture": "tracking/air-departure",
      "AIRarrival": "tracking/air-arrival",
      "cargoReady": "tracking/cargo-ready",
      "cyCutoff": "tracking/cy-cutoff",
      "pickup": "tracking/pickup",
      "delivery": "tracking/final-door-delivery",
      "finalDoorDelivery": "tracking/final-door-delivery"
    }
  },
  "baseSummaryProps": {
    "getModule": "moduleType",
    "size": -1
  },
  "baseDetailProps": {
    "getModule": "moduleType"
  }
}