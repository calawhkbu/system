                  }
                }
              },
              "searchTextParams": {
                "method": "POST",
                "url": "api/location/query/location",
                "data": {
                  "subqueries": {
                    "q": {
                      "value": "{{context.search}}"
                    }
                  },
                  "limit": 5
                }
              },
              "item-text": "name",
