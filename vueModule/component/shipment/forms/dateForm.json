{
  "components": [{
    "is": "WidgetMoreForm",
    "props": {
      "name": "shipmentDate",
      "mainKey": "Date",
      "formProps": {
        "z": {
          "class": "xs-12 padding-4"
        }
      },
      "labelProps": {
        "z": {
          "isComponent": "v-label",
          "i18nContext": "Widget",
          "class": "font-110"
        },
        "departure": {
          "class": "font-110 brown--text"
        },
        "arrival": {
          "class": "font-110 indigo--text"
        }
      },
      "fixedKeys": ["oceanBill", "cargoReady", "cyCutoff", "pickup", "cargoReceipt", "departure", "arrival", "finalDoorDelivery"],
      "alwaysShowKeys": ["oceanBill", "departure", "arrival"],
      "canAdd": true,
      "canAddProps": {
        "class": "xs-12 padding-20 min-height-200 margin-auto"
      },
      "fields": [{
          "label": "Widget.estimatedDate",
          "name": "shipmentDate.{{flex}}{{key}}DateEstimated",
          "component": "DateTimePicker",

          "class": "xs-6",
          "props": {
            "type": "datetime",
            "utc": true,
            "defaultFormat": "ddd MMM DD YYYY HH:mm:ss",
            "useOwnFormat": true
          }
        },
        {
          "label": "Widget.actualDate",
          "name": "shipmentDate.{{flex}}{{key}}DateActual",
          "component": "DateTimePicker",
          "class": "xs-6",
          "props": {
            "type": "datetime",
            "utc": true,
            "defaultFormat": "ddd MMM DD YYYY HH:mm:ss",
            "useOwnFormat": true
          }
        }
      ],
      "hiddenFieldsProps": {
        "style": {
          "width": "500px",
          "max-width": "100%",
          "z-index": 1
        }
      },
      "hiddenFields": [{
          "label": "Widget.estimatedDate",
          "name": "shipmentDate.{{flex}}{{key}}DateEstimated",
          "component": "DateTimePicker",

          "class": "xs-6",
          "props": {
            "type": "datetime",
            "utc": true,
            "defaultFormat": "ddd MMM DD YYYY HH:mm:ss",
            "useOwnFormat": true
          }
        },
        {
          "label": "Widget.actualDate",
          "name": "shipmentDate.{{flex}}{{key}}DateActual",
          "component": "DateTimePicker",
          "class": "xs-6",
          "props": {
            "type": "datetime",
            "utc": true,
            "defaultFormat": "ddd MMM DD YYYY HH:mm:ss",
            "useOwnFormat": true
          }
        },
        {
          "label": "Widget.remark",
          "name": "shipmentDate.{{flex}}{{key}}Remark",
          "component": "v-text-field",
          "class": "xs-12 md-12"
        }
      ]
    }
  }]
}