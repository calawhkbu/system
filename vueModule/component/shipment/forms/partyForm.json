{
  "components": [{
    "is": "WidgetMoreForm",
    "props": {
      "name": "shipmentParty",
      "mainKey": "Party",
      "formProps": {
        "z": {
          "class": "xs-12 md-6 padding-leftright-4"
        }
      },
      "labelProps": {
        "z": {
          "isComponent": "v-label",
          "i18nContext": "Widget",
          "class": "font-110"
        },
        "shipper": {
          "class": "font-110 teal--text required form-label"
        },
        "consignee": {
          "class": "font-110 green--text required form-label"
        },
        "office": {
          "class": "font-110 blue--text"
        },
        "agent": {
          "class": "font-110 purple--text"
        },
        "linerAgent": {
          "class": "font-110 amber--text"
        },
        "roAgent": {
          "class": "font-110 brown--text"
        },
        "controllingCustomer": {
          "class": "font-110 orange--text"
        },
        "notifyParty": {
          "class": "font-110 pink--text"
        }
      },
      "fieldProps": {
        "z": {
          "thirdPartyCode.erp": "{{thirdPartyCode.erp}}",
          "id": "{{id}}",
          "name": "{{name}}",
          "address": "{{address}}",
          "cityCode": "{{cityCode}}",
          "countryCode": "{{countryCode}}",
          "zip": "{{zip}}",
          "shortName": "{{shortName}}",
          "fixValue": "{{fixValue}}"
        }
      },
      "fixedKeys": [
        "shipper",
        "consignee",
        "agent",
        "office",
        "roAgent",
        "linerAgent",
        "controllingCustomer",
        "notifyParty"
      ],
      "alwaysShowKeys": [
        "shipper",
        "consignee",
        "agent",
        "office",
        "roAgent",
        "linerAgent",
        "controllingCustomer",
        "notifyParty"
      ],
      "canAdd": true,
      "canAddProps": {
        "class": "xs-12 md-6 padding-20 min-height-200 margin-auto"
      },
      "addItemsOrFunc": ["Trucker", "Broker", "Factory", "Warehouse"],
      "fields": [{
        "name": "shipmentParty.{{flex}}{{key}}PartyName",
        "component": "v-text-field",
        "class": "xs-12 md-12"
      }],
      "hiddenFieldsProps": {
        "style": {
          "width": "500px",
          "max-width": "100%",
          "z-index": 1
        }
      },

      "checkDisplayWhen" : "return !(user.parties.findIndex(x => x.erpCode === 'L1052') >= 0 && ['shipper','office','notifyParty'].includes(key))",
      "hiddenFields": [
        {
          "name": "shipmentParty.{{flex}}{{key}}Party",
          "component": "AsyncAutoSuggest",

          "checkDisplayWhen" : "return !(user.parties.findIndex(x => x.erpCode === 'L1052') >= 0 && ['agent','consignee','controllingCustomer'].includes('{{key}}'))",
          "class": "xs-12 md-8",
          "relation": "shipmentParty.{{flex}}{{key}}Party",
          "relationMore": [
            {
              "name": "shipmentParty.{{flex}}{{key}}PartyId",
              "selector": "id"
            },
            {
              "name": "shipmentParty.{{flex}}{{key}}PartyName",
              "selector": "name"
            },
            {
              "name": "shipmentParty.{{flex}}{{key}}PartyPhone",
              "selector": "partyPhone"
            },
            {
              "name": "shipmentParty.{{flex}}{{key}}PartyFax",
              "selector": "partyFax"
            },
            {
              "name": "shipmentParty.{{flex}}{{key}}PartyEmail",
              "selector": "partyEmail"
            },
            {
              "name": "shipmentParty.{{flex}}{{key}}PartyAddress",
              "selector": "partyAddress"
            },
            {
              "name": "shipmentParty.{{flex}}{{key}}PartyCityCode",
              "selector": "partyCityCode"
            },
            {
              "name": "shipmentParty.{{flex}}{{key}}PartyStateCode",
              "selector": "partyStateCode"
            },
            {
              "name": "shipmentParty.{{flex}}{{key}}PartyCountryCode",
              "selector": "partyCountryCode"
            },
            {
              "name": "shipmentParty.{{flex}}{{key}}PartyZip",
              "selector": "partyZip"
            },
            {
              "name": "shipmentParty.{{flex}}{{key}}PartyContactPersonId",
              "selector": "contactPersonId"
            },
            {
              "name": "shipmentParty.{{flex}}{{key}}PartyContactPersonEmail",
              "selector": "contactPersonEmail"
            },
            {
              "name": "shipmentParty.{{flex}}{{key}}PartyContactPersonName",
              "selector": "contactPersonName"
            },
            {
              "name": "shipmentParty.{{flex}}{{key}}PartyContactPersonPhone",
              "selector": "contactPersonPhone"
            }
          ],
          "props": {
            "createFormPrefix": "{{key}}",
            "createDialog": {
              "beforeItem": {
                "layout": "Dummy",
                "components": [{
                  "is": "Icon",
                  "props": {
                    "icon": "add"
                  }
                }, {
                  "is": "I18nText",
                  "props": {
                    "i18nContext": "Widget",
                    "i18nKey": "Click here to create a new Party"
                  }
                }, {
                  "is": "I18nText",
                  "props": {
                    "i18nKey": "("
                  }
                }]
              },
              "afterItem": {
                "layout": "Dummy",
                "components": [{
                  "is": "I18nText",
                  "props": {
                    "i18nKey": ")"
                  }
                }]
              },
              "beforeDialog": {
                "layout": "v-card-title",
                "components": [{
                  "is": "I18nText",
                  "props": {
                    "i18nContext": "Widget",
                    "i18nKey": "Create a new party"
                  }
                }]
              },
              "nameKey": "name",
              "api": {
                "url": "api/party",
                "method": "POST",
                "item-text": "name",
                "item-value": "thirdPartyCode.erp"
              },
              "defaultValue": {
                "types": [{
                  "type": "{{key}}"
                }]
              },
              "parseReturnValue": {
                "contactPersonEmail": null,
                "contactPersonId": null,
                "contactPersonName": null,
                "erpCode": "{{thirdPartyCode.erp}}",
                "id": "{{id}}",
                "name": "{{name}}",
                "old360Id": null,
                "partyAddress": "{{address}}",
                "partyCityCode": "{{cityCode}}",
                "partyCountryCode": "{{countryCode}}",
                "partyEmail": null,
                "partyFax": null,
                "partyId": "{{id}}",
                "partyName": "{{name}}",
                "partyPhone": null,
                "partyStateCode": null,
                "partyZip": "{{zip}}",
                "shortName": "{{shortName}}"
              },
              "fields": [{
                  "label": "Widget.partyName",
                  "name": "name",
                  "component": "v-text-field",
                  "class": "xs-12 md-8",
                  "readonlyClass": "hidden"
                },
                {
                  "label": "Widget.erpCode",
                  "name": "thirdPartyCode.erp",
                  "component": "v-text-field",
                  "class": "xs-12 md-4",
                  "readonlyClass": "hidden"
                },
                {
                  "label": "Widget.address",
                  "name": "address",
                  "component": "v-textarea",
                  "props": {
                    "auto-grow": true,
                    "rows": 5
                  },
                  "class": "xs-12",
                  "readonlyClass": "hidden"
                },
                {
                  "label": "Widget.city",
                  "name": "cityCode",
                  "component": "AsyncAutoSuggest",
                  "class": "xs-12 md-4",
                  "readonlyClass": "xs-12 md-4",
                  "relation": "cityCode",
                  "relationMore": [{
                    "name": "cityCode",
                    "selector": "locationCode"
                  }],
                  "props": {
                    "canCreate": true,
                    "canCreateValue": {
                      "id": null,
                      "code": "{{autoSuggestModel}}"
                    },
                    "searchTextParams": {
                      "url": "api/location/query/location",
                      "method": "POST",
                      "data": {
                        "subqueries": {
                          "q": {
                            "value": "{{context.search}}"
                          }
                        },
                        "limit": 5
                      }
                    },
                    "item-text": "name",
                    "item-value": "locationCode"
                  }
                },
                {
                  "label": "Widget.state",
                  "name": "stateCode",
                  "component": "AsyncAutoSuggest",
                  "class": "xs-12 md-4",
                  "readonlyClass": "xs-12 md-4",
                  "relation": "stateCode",
                  "relationMore": [{
                    "name": "stateCode",
                    "selector": "code"
                  }],
                  "props": {
                    "canCreate": true,
                    "canCreateValue": {
                      "id": null,
                      "code": "{{autoSuggestModel}}"
                    },
                    "searchTextParams": {
                      "url": "api/code/query/code_master",
                      "method": "POST",
                      "data": {
                        "subqueries": {
                          "codeType": {
                            "value": "STATE-PROVINCE"
                          },
                          "q": {
                            "value": "{{context.search}}"
                          }
                        },
                        "limit": 5
                      }
                    },
                    "item-text": "name",
                    "item-value": "code"
                  }
                },
                {
                  "label": "Widget.country",
                  "name": "countryCode",
                  "component": "AsyncAutoSuggest",
                  "class": "xs-12 md-4",
                  "readonlyClass": "xs-12 md-4",
                  "relation": "countryCode",
                  "relationMore": [{
                    "name": "countryCode",
                    "selector": "code"
                  }],
                  "props": {
                    "canCreate": true,
                    "canCreateValue": {
                      "id": null,
                      "code": "{{autoSuggestModel}}"
                    },
                    "searchTextParams": {
                      "url": "api/code/query/code_master",
                      "method": "POST",
                      "data": {
                        "subqueries": {
                          "codeType": {
                            "value": "COUNTRY"
                          },
                          "q": {
                            "value": "{{context.search}}"
                          }
                        },
                        "limit": 5
                      }
                    },
                    "item-text": "name",
                    "item-value": "code"
                  }
                },
                {
                  "label": "Widget.zip",
                  "name": "zip",
                  "component": "v-text-field",
                  "class": "xs-12 md-6",
                  "readonlyClass": "hidden"
                }
              ]
            },
            "searchTextParams": {
              "url": "api/party/query/party_auto_suggest",
              "method": "POST",
              "data": {
                "subqueries": {
                  "q": {
                    "value": "{{context.search}}"
                  },
                  "partyTypes": {
                    "value": ["{{key}}"]
                  }
                },
                "limit": 5
              }
            },
            "ellipsis": false,
            "item-text": "name",
            "item-value": "erpCode"
          }
        },
        {
          "label": "Widget.shortName",
          "name": "shipmentParty.{{flex}}{{key}}Party.shortName",

          "component": "v-text-field",
          "class": "xs-12 md-4",
          "props": {
            "readonly": "readonly"
          }
        },
        {
          "label": "Widget.contactName",
          "name": "shipmentParty.{{flex}}{{key}}PartyContactName",
          "checkDisplayWhen" : "return !(user.parties.findIndex(x => x.erpCode === 'L1052') >= 0 && ['agent','consignee','controllingCustomer'].includes('{{key}}'))",
          "component": "v-text-field",
          "class": "xs-12 md-12"
        },
        {
          "label": "Widget.contactEmail",
          "name": "shipmentParty.{{flex}}{{key}}PartyContactEmail",
          "component": "v-text-field",

          "checkDisplayWhen" : "return !(user.parties.findIndex(x => x.erpCode === 'L1052') >= 0 && ['agent','consignee','controllingCustomer'].includes('{{key}}'))",
          "class": "xs-12 md-12",
          "readonlyClass": "xs-10 md-10"
        },
        {
          "label": "Widget.inviteEmail",
          "name": "shipmentParty.{{flex}}{{key}}PartyContactEmail",
          "checkDisplayWhen": "return form && form.shipmentParty && form.shipmentParty.{{flex}}{{key}}PartyContactEmail",
          "component": "button-field",
          "readonlyClass": "xs-2",
          "slots": [{
            "is": "Icon",
            "props": {
              "icon": "email"
            }
          }],
          "props": {
            "flat": true,
            "icon": true,
            "events": {
              "click.stop": [{
                "type": "axios",
                "otherParams": {
                  "axiosParams": {
                    "url": "/api/invitation/invite/invite",
                    "method": "POST",
                    "data": {
                      "entityId": "{{context.$store.state.widget.id}}",
                      "tableName": "shipment",
                      "partyId": ["{{ '`${args[0]}${args[1]}PartyId`'  | calculate(flex,key) | getFrom(context.$store.state.widget.data)}}"],
                      "partyType": ["{{key}}"],
                      "person": {
                        "userName": "{{ context.fixValue }}"
                      }
                    }
                  }
                },
                "afterActions": [{
                  "type": "notify",
                  "otherParams": {
                    "notification": "invite success"
                  }
                }]
              }]
            }
          }
        },
        {
          "label": "Widget.contactPhone",
          "name": "shipmentParty.{{flex}}{{key}}PartyContactPhone",

          "checkDisplayWhen" : "return !(user.parties.findIndex(x => x.erpCode === 'L1052') >= 0 && ['agent','consignee','controllingCustomer'].includes('{{key}}'))",
          "component": "v-text-field",
          "class": "xs-12 md-12"
        },
        {
          "name": "shipmentParty.{{flex}}{{key}}PartyContacts",
          "component": "GridField",

          "checkDisplayWhen" : "return !(user.parties.findIndex(x => x.erpCode === 'L1052') >= 0 && ['agent','consignee'].includes('{{key}}'))",
          "class": "xs-12",
          "readonlyClass": "xs-12",
          "props": {
            "overrideRowIndex": 1,
            "fields": [{
                "label": "Widget.contactXName",
                "name": "Name",
                "component": "v-text-field",
                "class": "xs-12 md-12",
                "readonlyClass": "xs-12 md-12"
              },
              {
                "label": "Widget.contactXEmail",
                "name": "Email",
                "component": "v-text-field",
                "class": "xs-12 md-12",
                "readonlyClass": "xs-10 md-10"
              },
              {
                "label": "Widget.inviteEmail",
                "checkDisplayWhen": "return form.Email",
                "name": "Email",
                "component": "button-field",
                "readonlyClass": "xs-2 md-2",
                "slots": [{
                  "is": "Icon",
                  "props": {
                    "icon": "email"
                  }
                }],
                "props": {
                  "flat": true,
                  "icon": true,
                  "events": {
                    "click.stop": [{
                      "type": "axios",
                      "otherParams": {
                        "axiosParams": {
                          "url": "/api/invitation/invite/invite",
                          "method": "POST",
                          "data": {
                            "entityId": "{{context.$store.state.widget.id}}",
                            "tableName": "shipment",
                            "partyId": ["{{ '`${args[0]}${args[1]}PartyId`'  | calculate(flex,key) | getFrom(context.$store.state.widget.data)}}"],
                            "partyType": ["{{key}}"],
                            "person": {
                              "userName": "{{ context.fixValue }}"
                            }
                          }
                        }
                      },
                      "afterActions": [{
                        "type": "notify",
                        "otherParams": {
                          "notification": "invite success"
                        }
                      }]
                    }]
                  }
                }
              },
              {
                "label": "Widget.contactXPhone",
                "name": "Phone",
                "component": "v-text-field",
                "class": "xs-12 md-12",
                "_readonlyClass": "xs-12 md-12"
              }
            ]
          }
        },
        {
          "label": "Widget.address",
          "name": ["shipmentParty.{{flex}}{{key}}PartyAddress", "shipmentParty.{{flex}}{{key}}Party.address"],
          "checkDisplayWhen" : "return !(user.parties.findIndex(x => x.erpCode === 'L1052') >= 0 && ['agent','consignee','controllingCustomer'].includes('{{key}}'))",
          "component": "v-textarea",
          "props": {
            "auto-grow": true,
            "rows": 5
          },
          "class": "xs-12 md-12"
        },
        {
          "label": "Widget.city",
          "name": ["shipmentParty.{{flex}}{{key}}PartyCityCode", "shipmentParty.{{flex}}{{key}}Party.cityCode"],
          "checkDisplayWhen" : "return !(user.parties.findIndex(x => x.erpCode === 'L1052') >= 0 && ['agent','consignee','controllingCustomer'].includes('{{key}}'))",
          "component": "v-text-field",
          "class": "xs-12 md-3",
          "readonlyClass": "xs-12 md-3"
        },
        {
          "label": "Widget.state",
          "name": ["shipmentParty.{{flex}}{{key}}PartyStateCode", "shipmentParty.{{flex}}{{key}}Party.stateCode"],
          "checkDisplayWhen" : "return !(user.parties.findIndex(x => x.erpCode === 'L1052') >= 0 && ['agent','consignee','controllingCustomer'].includes('{{key}}'))",
          "component": "v-text-field",
          "class": "xs-12 md-4",
          "readonlyClass": "xs-12 md-3"
        },
        {
          "label": "Widget.country",
          "name": ["shipmentParty.{{flex}}{{key}}PartyCountryCode", "shipmentParty.{{flex}}{{key}}Party.countryCode"],
          "checkDisplayWhen" : "return !(user.parties.findIndex(x => x.erpCode === 'L1052') >= 0 && ['agent','consignee','controllingCustomer'].includes('{{key}}'))",
          "component": "v-text-field",
          "class": "xs-12 md-4",
          "readonlyClass": "xs-12 md-3"
        },
        {
          "label": "Widget.zip",
          "name": ["shipmentParty.{{flex}}{{key}}PartyZip", "shipmentParty.{{flex}}{{key}}Party.zip"],
          "checkDisplayWhen" : "return !(user.parties.findIndex(x => x.erpCode === 'L1052') >= 0 && ['agent','consignee','controllingCustomer'].includes('{{key}}'))",
          "component": "v-text-field",
          "class": "xs-12",
          "readonlyClass": "xs-12 md-3"
        }
      ]
    }
  }]
}