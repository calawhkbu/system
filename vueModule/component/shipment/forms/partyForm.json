{
  "components": [
    {
      "is": "WidgetMoreForm",
      "props": {
        "mainKey": "Party",
        "formProps": {
          "z": {
            "class": "xs-12 md-6 padding-4"
          }
        },
        "labelProps": {
          "z": {
            "isComponent": "v-label",
            "i18nContext": "Widget",
            "class": "font-110"
          },
          "shipper": {
            "class": "font-110 teal--text required form-label"
          },
          "consignee": {
            "class": "font-110 green--text required form-label"
          },
          "agent": {
            "class": "font-110 blue--text"
          },
          "controllingCustomer": {
            "class": "font-110 blue--text"
          },
          "linerAgent": {
            "class": "font-110 brown--text"
          },
          "office": {
            "class": "font-110 pink--text"
          }
        },
        "fixedKeys": [
          "shipper",
          "consignee",
          "agent",
          "controllingCustomer",
          "linerAgent",
          "office"
        ],
        "alwaysShowKeys": [
          "shipper",
          "consignee",
          "agent",
          "controllingCustomer",
          "linerAgent",
          "office"
        ],
        "canAdd": true,
        "canAddProps": {
          "class": "xs-12 md-6 padding-20 min-height-200 margin-auto"
        },
        "addItemsOrFunc": ["Trucker", "Broker", "Factory", "Warehouse"],

        "fields": [
          {
            "name": "{{flex}}{{key}}PartyName",
            "component": "v-text-field",
            "class": "xs-12 md-12"
          }
        ],
        "hiddenFieldsProps": {
          "style": {
            "width": "500px",
            "max-width": "100%",
            "z-index": 1
          }
        },
        "hiddenFields": [
          {
            "name": "{{flex}}{{key}}PartyName",
            "component": "v-text-field",
            "class": "xs-12 md-12"
          },

          {
            "label": "Widget.countryCode",
            "name": "{{flex}}{{key}}PartyCountryCode",
            "component": "ReadField",
            "class": "xs-12 md-12",
            "checkDisplayWhen": "return form && form.{{flex}}{{key}}PartyCountryCode",
            "props": {
              "resultProp": "name",

              "axiosParams": {
                "url": "api/code/query/code_master",
                "method": "POST",
                "data": {
                  "subqueries": {
                    "codeType": {
                      "value": "COUNTRY"
                    },
                    "code": {
                      "value": "{{context.fixValue}}"
                    }
                  }
                }
              }
            }
          },

          {
            "label": "Widget.stateCode",
            "name": "{{flex}}{{key}}PartyStateCode",
            "component": "ReadField",
            "class": "xs-12 md-12",
            "checkDisplayWhen": "return form && form.{{flex}}{{key}}PartyStateCode",
            "props": {
              "resultProp": "name",

              "axiosParams": {
                "url": "api/code/query/code_master",
                "method": "POST",
                "data": {
                  "subqueries": {
                    "codeType": {
                      "value": "STATE-PROVINCE"
                    },
                    "code": {
                      "value": "{{context.fixValue}}"
                    }
                  }
                }
              }
            }
          },

          {
            "label": "Widget.cityCode",
            "name": "{{flex}}{{key}}PartyCityCode",
            "component": "ReadField",
            "class": "xs-12 md-12",
            "checkDisplayWhen": "return form && form.{{flex}}{{key}}PartyCityCode",
            "props": {
              "resultProp": "name",

              "axiosParams": {
                "url": "api/code/query/code_master",
                "method": "POST",
                "data": {
                  "subqueries": {
                    "codeType": {
                      "value": "AIRPORT"
                    },
                    "code": {
                      "value": "{{context.fixValue}}"
                    }
                  }
                }
              }
            }
          },

          {
            "label": "Widget.contactAddress",
            "name": "{{flex}}{{key}}PartyAddress",
            "component": "v-text-field",
            "class": "xs-12 md-12",
            "checkDisplayWhen": "return form && form.{{flex}}{{key}}PartyAddress"
          },

          {
            "label": "Widget.contactName",
            "name": "{{flex}}{{key}}PartyContactName",
            "component": "v-text-field",
            "class": "xs-12 md-12",
            "checkDisplayWhen": "return form && form.{{flex}}{{key}}PartyContactName"
          },

          {
            "label": "Widget.contactPhone",
            "name": "{{flex}}{{key}}PartyContactPhone",
            "component": "v-text-field",
            "class": "xs-12 md-12",
            "checkDisplayWhen": "return form && form.{{flex}}{{key}}PartyContactPhone"
          },

          {
            "label": "Widget.contactEmail",
            "name": "{{flex}}{{key}}PartyContactEmail",
            "component": "v-text-field",
            "class": "xs-12 md-12",
            "checkDisplayWhen": "return form && form.{{flex}}{{key}}PartyContactEmail"
          }
        ]
      }
    }
  ]
}
