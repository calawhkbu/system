{
  "components": [{
    "is": "WidgetForm",
    "props": {
      "class": "padding-top-5",
      "fields": [{
          "label": "ShipmentPage.moduleType",
          "name": "moduleTypeCode",
          "component": "AsyncSelect",
          "class": "xs-3",
          "relation": "moduleType",
          "props": {
            "axiosParams": {
              "url": "api/code/query/code_master",
              "method": "POST",
              "data": {
                "subqueries": {
                  "codeType": {
                    "value": "MODULE"
                  }
                }
              }
            },
            "item-text": "name",
            "item-value": "code",
            "showValue": false
          }
        },
        {
          "label": "ShipmentPage.boundType",
          "name": "boundTypeCode",
          "component": "AsyncSelect",
          "class": "xs-3",
          "relation": "boundType",
          "props": {
            "axiosParams": {
              "url": "api/code/query/code_master",
              "method": "POST",
              "data": {
                "subqueries": {
                  "codeType": {
                    "value": "BOUND"
                  }
                }
              }
            },
            "item-text": "name",
            "item-value": "code"
          }
        }, {
          "label": "ShipmentPage.nominatedType",
          "name": "nominatedTypeCode",
          "component": "AsyncSelect",
          "class": "xs-2",
          "relation": "boundType",
          "props": {
            "axiosParams": {
              "url": "api/code/query/code_master",
              "method": "POST",
              "data": {
                "subqueries": {
                  "codeType": {
                    "value": "NOMINATED_TYPE"
                  }
                }
              }
            },
            "item-text": "name",
            "item-value": "code"
          }
        }, {
          "label": "ShipmentPage.service",
          "name": "serviceCode",
          "component": "AsyncSelect",
          "class": "xs-2",
          "relation": "service",
          "props": {
            "axiosParams": {
              "url": "api/code/query/code_master",
              "method": "POST",
              "data": {
                "subqueries": {
                  "codeType": {
                    "value": "SERVTYPE"
                  }
                }
              }
            },
            "item-text": "name",
            "item-value": "code",
            "showValue": false
          }
        }, {
          "label": "ShipmentPage.isDirect",
          "name": "isDirect",
          "component": "v-select",
          "class": "xs-1",
          "props": {
            "items": [{
              "value": 1,
              "text": "YES"
            }, {
              "value": 0,
              "text": "NO"
            }]
          }
        }, {
          "label": "ShipmentPage.isCoload",
          "name": "isCoload",
          "component": "v-select",
          "class": "xs-1",
          "props": {
            "items": [{
              "value": 1,
              "text": "YES"
            }, {
              "value": 0,
              "text": "NO"
            }]
          }
        }, {
          "label": "ShipmentPage.houseNo",
          "name": "houseNo",
          "component": "v-text-field",
          "class": "xs-3"
        }, {
          "label": "ShipmentPage.masterNo",
          "name": "masterNo",
          "component": "v-text-field",
          "class": "xs-3"
        }, {
          "label": "ShipmentPage.jobNo",
          "name": "jobNo",
          "component": "v-text-field",
          "class": "xs-3"
        }, {
          "label": "ShipmentPage.jobDate",
          "name": "jobDate",
          "component": "DateTimePicker",
          "class": "xs-3",
          "props": {
            "type": "date",
            "utc": true,
            "defaultFormat": "ddd MMM DD YYYY",
            "useOwnFormat": true
          }
        },
        {
          "label": "ShipmentPage.carrier",
          "name": "carrier",
          "component": "AsyncAutoSuggest",
          "class": "xs-12 md-3",
          "relation": "carrier",
          "more": [{
            "name": "carrierCode",
            "selector": "code"
          }, {
            "name": "carrierName",
            "selector": "name"
          }],
          "props": {
            "searchValueParams": {
              "method": "POST",
              "url": "api/code/query/code_master",
              "data": {
                "subqueries": {
                  "codeType": {
                    "value": "CARRIER"
                  },
                  "code": {
                    "value": "{{context.autoSuggestModel}}"
                  }
                }
              }
            },
            "searchTextParams": {
              "method": "POST",
              "url": "api/code/query/code_master",
              "data": {
                "subqueries": {
                  "codeType": {
                    "value": "CARRIER"
                  },
                  "q": {
                    "value": "{{context.search}}"
                  }
                },
                "limit": 5
              }
            },
            "item-text": "name",
            "item-value": "code"
          }
        },
        {
          "label": "ShipmentPage.vessel",
          "name": "vesselName",
          "component": "AsyncAutoSuggest",
          "class": "xs-3",
          "relation": "vesselName",
          "relationMore": [{
            "name": "vesselName",
            "selector": "name"
          }],
          "checkDisplayWhen": "return form && form.moduleTypeCode && form.moduleTypeCode === 'SEA'",
          "props": {
            "canCreate": true,
            "canCreateValue": {
              "id": null,
              "name": "{{autoSuggestModel}}"
            },
            "searchTextParams": {
              "method": "POST",
              "url": "api/code/query/code_master",
              "data": {
                "subqueries": {
                  "codeType": {
                    "value": "VESSEL"
                  },
                  "q": {
                    "value": "{{context.search}}"
                  }
                },
                "limit": 5
              }
            },
            "item-text": "name",
            "item-value": "name"
          }
        },
        {
          "label": "ShipmentPage.voyageFlightNumber",
          "name": "voyageFlightNumber",
          "component": "v-text-field",
          "class": "xs-3",
          "checkDisplayWhen": "return form && form.moduleTypeCode && (form.moduleTypeCode === 'AIR' || form.moduleTypeCode === 'SEA')"
        },
        {
          "class": "xs-3"
        },
        {
          "class": "xs-3",
          "checkDisplayWhen": "return form && form.moduleTypeCode && form.moduleTypeCode === 'AIR'"
        },
        {
          "label": "ShipmentPage.placeOfReceipt",
          "name": "placeOfReceipt",
          "component": "AsyncAutoSuggest",
          "class": "xs-12 md-3",
          "more": [{
            "name": "placeOfReceiptCode",
            "selector": "portCode"
          }, {
            "name": "placeOfReceiptCode",
            "selector": "name"
          }],
          "props": {
            "searchValueParams": {
              "method": "POST",
              "url": "api/location/query/location",
              "data": {
                "subqueries": {
                  "moduleTypeCode": {
                    "value": "{{context.form.moduleTypeCode}}"
                  },
                  "portCodeLike": {
                    "value": "{{context.autoSuggestModel}}"
                  }
                }
              }
            },
            "searchTextParams": {
              "method": "POST",
              "url": "api/location/query/location",
              "data": {
                "subqueries": {
                  "moduleTypeCode": {
                    "value": "{{context.form.moduleTypeCode}}"
                  },
                  "q": {
                    "value": "{{context.search}}"
                  }
                },
                "limit": 5
              }
            },
            "item-text": "name",
            "item-value": "portCode"
          }
        },
        {
          "label": "ShipmentPage.portOfLoading",
          "name": "portOfLoading",
          "component": "AsyncAutoSuggest",
          "class": "xs-12 md-3",
          "more": [{
            "name": "portOfLoadingCode",
            "selector": "portCode"
          }, {
            "name": "portOfLoadingName",
            "selector": "name"
          }],
          "props": {
            "searchTextParams": {
              "method": "POST",
              "url": "api/location/query/location",
              "data": {
                "subqueries": {
                  "moduleTypeCode": {
                    "value": "{{context.form.moduleTypeCode}}"
                  },
                  "q": {
                    "value": "{{context.search}}"
                  }
                },
                "limit": 5
              }
            },
            "item-text": "name",
            "item-value": "portCode"
          }
        },
        {
          "label": "ShipmentPage.portOfDischarge",
          "name": "portOfDischarge",
          "component": "AsyncAutoSuggest",
          "class": "xs-12 md-3",
          "more": [{
            "name": "portOfDischargeCode",
            "selector": "portCode"
          }, {
            "name": "portOfDischargeName",
            "selector": "name"
          }],
          "props": {
            "searchTextParams": {
              "method": "POST",
              "url": "api/location/query/location",
              "data": {
                "subqueries": {
                  "moduleTypeCode": {
                    "value": "{{context.form.moduleTypeCode}}"
                  },
                  "q": {
                    "value": "{{context.search}}"
                  }
                },
                "limit": 5
              }
            },
            "item-text": "name",
            "item-value": "portCode"
          }
        },
        {
          "label": "ShipmentPage.placeOfDelivery",
          "name": "placeOfDelivery",
          "component": "AsyncAutoSuggest",
          "class": "xs-12 md-3",
          "more": [{
            "name": "placeOfDeliveryCode",
            "selector": "portCode"
          }, {
            "name": "placeOfDeliveryName",
            "selector": "name"
          }],
          "props": {
            "searchValueParams": {
              "method": "POST",
              "url": "api/location/query/location",
              "data": {
                "subqueries": {
                  "moduleTypeCode": {
                    "value": "{{context.form.moduleTypeCode}}"
                  },
                  "portCodeLike": {
                    "value": "{{context.autoSuggestModel}}"
                  }
                }
              }
            },
            "searchTextParams": {
              "method": "POST",
              "url": "api/location/query/location",
              "data": {
                "subqueries": {
                  "moduleTypeCode": {
                    "value": "{{context.form.moduleTypeCode}}"
                  },
                  "q": {
                    "value": "{{context.search}}"
                  }
                },
                "limit": 5
              }
            },
            "item-text": "name",
            "item-value": "portCode"
          }
        },
        {
          "label": "ShipmentPage.incoTerms",
          "name": "incoTerms",
          "component": "AsyncAutoSuggest",
          "class": "xs-4",
          "more": [{
            "name": "incoTermsCode",
            "selector": "code"
          }],
          "relationMore": [{
              "name": "freightTermsCode",
              "selector": "flexData.frieghtTermsCode"
            },
            {
              "name": "otherTermsCode",
              "selector": "flexData.otherTermsCode"
            }
          ],
          "props": {
            "searchTextParams": {
              "url": "api/code/query/code_master",
              "method": "POST",
              "data": {
                "subqueries": {
                  "codeType": {
                    "value": "INCOTERMS"
                  },
                  "q": {
                    "value": "{{context.search}}"
                  }
                },
                "limit": 5
              }
            },
            "item-text": "code",
            "item-value": "code"
          }
        },
        {
          "label": "ShipmentPage.freightTerms",
          "name": "freightTermsCode",
          "component": "AsyncSelect",
          "class": "xs-1",
          "relation": "freightTerms",
          "props": {
            "axiosParams": {
              "url": "api/code/query/code_master",
              "method": "POST",
              "data": {
                "subqueries": {
                  "codeType": {
                    "value": "PAYTERMS"
                  }
                }
              }
            },
            "item-text": "name",
            "item-value": "code"
          }
        },
        {
          "label": "ShipmentPage.otherTerms",
          "name": "otherTermsCode",
          "component": "AsyncSelect",
          "class": "xs-1",
          "relation": "otherTerms",
          "props": {
            "axiosParams": {
              "url": "api/code/query/code_master",
              "method": "POST",
              "data": {
                "subqueries": {
                  "codeType": {
                    "value": "PAYTERMS"
                  }
                }
              }
            },
            "item-text": "name",
            "item-value": "code"
          }
        },
        {
          "label": "ShipmentPage.commodity",
          "name": "commodity",
          "component": "v-text-field",
          "class": "xs-6"
        },{
          "label": "ShipmentPage.picId",
          "name": "picId",
          "component": "v-text-field",
          "class": "xs-6 md-3"
        },
        {
          "label": "ShipmentPage.picEmail",
          "name": "picEmail",
          "component": "v-text-field",
          "class": "xs-6 md-3"
        }
      ]
    }
  }]
}
