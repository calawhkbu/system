                      "id": null,
                      "code": "{{autoSuggestModel}}"
                    },
                    "searchTextParams": {
                      "url": "api/code/query/code_master",
                      "method": "POST",
                      "data": {
                        "subqueries": {
                          "codeType": {
                            "value": "STATE-PROVINCE"
                          },
                          "q": {
                            "value": "{{context.search}}"
                          }
                        },
                        "limit": 5
                      }
                    },
                    "item-text": "name",
                    "item-value": "code"
