{
  "components": [{
    "is": "WidgetForm",
    "props": {
      "fields": [{
        "label": "ShipmentPage.poNo",
        "name": "shipmentPo",
        "component": "MultiField",
        "checkDisplayWhen": "return form.shipmentPo && form.shipmentPo.length > 0",
        "class": "xs-12",
        "props": {
          "itemContainerClass": "xs-12",
          "updateValue": [
            "return Array.isArray(value) ? value.map(v => v.poNo) : [value]"
          ],
          "onCheckFixValueFunc": [
            "return value.map(v => ({ poNo: v }))"
          ]
        }
      }, {
        "label": "ShipmentPage.references",
        "name": "shipmentReference",
        "component": "TableField",
        "class": "xs-12",
        "props": {
          "topLabel": true,
          "fields": [{
              "label": "ShipmentPage.refName",
              "name": "refName",
              "class": "xs-12 md-5",
              "component": "LocalAutoSuggest",
              "colGroupProps": {
                "style": {
                  "width": "25%"
                }
              },
              "props": {
                "canCreate": true,
                "funcItems": ["MBL", "MAWB", "HBL", "HAWB"]
              }
            },
            {
              "label": "ShipmentPage.refDescription",
              "name": "refDescription",
              "class": "xs-12 md-5",
              "component": "v-text-field",
              "colGroupProps": {
                "style": {
                  "width": "50%"
                }
              }
            },
            {
              "label": "ShipmentPage.docDate",
              "name": "docDate",
              "component": "DateTimePicker",
              "class": "xs-12 md-2",
              "colGroupProps": {
                "style": {
                  "width": "20%"
                }
              },
              "props": {
                "type": "date",
                "utc": true,
                "defaultFormat": "ddd MMM DD YYYY",
                "useOwnFormat": true
              }
            }
          ]
        }
      }]
    }
  }]
}