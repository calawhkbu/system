{
  "class": "grid full-height",
  "components": [{
      "is": "DynamicComponent",
      "props": {
        "class": "xs-12 grid min-height-28 align-item-center full-width padding-8",
        "components": [{
            "is": "v-btn",
            "props": {
              "small": true,
              "flat": true,
              "icon": true
            },
            "slots": [{
              "is": "Icon",
              "props": {
                "icon": "arrow_back"
              }
            }],
            "events": {
              "click.stop": [{
                "type": "router",
                "otherParams": {
                  "func": "push",
                  "path": "/{{ p[0] }}/{{ p[1] }}"
                }
              }]
            }
          },
          {
            "is": "I18nText",
            "props": {
              "i18nContext": "ShipmentPage",
              "i18nKey": "importTitle"
            }
          }, {
            "is": "v-spacer"
          }
        ]
      }
    },
    {
      "is": "DynamicComponent",
      "props": {
        "class": "xs-12 md-8 padding-left-8 padding-right-8",
        "components": [{
          "is": "WidgetImport",
          "props": {
            "tableName": "shipment",
            "importKey": "houseNo",
            "importFileTypes": ["xlsx"],
            "importAxiosParams": {
              "z": {
                "method": "POST",
                "url": "api/shipment/import"
              }
            },
            "importFormLayout": {
              "components": [{
                  "is": "DynamicComponent",
                  "props": {
                    "class": "xs-12 grid padding-bottom-2",
                    "components": [{
                      "is": "DynamicComponent",
                      "props": {
                        "class": "xs-6 padding-right-2",
                        "components": [{
                          "is": "AsyncComponent",
                          "props": {
                            "layoutName": "shipment/import/party"
                          }
                        }]
                      }
                    }, {
                      "is": "DynamicComponent",
                      "props": {
                        "class": "xs-6 padding-left-2",
                        "components": [{
                          "is": "AsyncComponent",
                          "props": {
                            "layoutName": "shipment/import/date"
                          }
                        }]
                      }
                    }]
                  }
                },
                {
                  "is": "AsyncComponent",
                  "props": {
                    "class": "xs-12 margin-bottom-2 padding-top-4",
                    "layoutName": "shipment/import/general"
                  }
                },
                {
                  "is": "AsyncComponent",
                  "props": {
                    "class": "xs-12 margin-bottom-2 padding-top-4",
                    "layoutName": "shipment/import/container"
                  }
                },
                {
                  "is": "AsyncComponent",
                  "props": {
                    "class": "xs-12 margin-bottom-2 padding-top-4",
                    "layoutName": "shipment/import/reference"
                  }
                }
              ]
            }
          }
        }]
      }
    }
  ]
}