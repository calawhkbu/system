{
  "components": [{
      "is": "DynamicComponent",
      "props": {
        "layout": "v-card",
        "class": "absolute-topleft-20 zindex-300",
        "components": [{
          "is": "v-btn",
          "props": {
            "icon": true
          },
          "events": {
            "click.stop": [{
              "type": "dispatch",
              "otherParams": {
                "name": "widget/changeMode",
                "mode": "detail-edit"
              }
            }]
          },
          "slots": [{
            "is": "Icon",
            "props": {
              "icon": "arrow_back"
            }
          }]
        }]
      }
    },
    {
      "is": "DynamicComponent",
      "props": {
        "layout": "v-card",
        "class": "absolute-topright-20 zindex-250",
        "components": [{
          "is": "ClickHidden",
          "props": {
            "desktopOpen": true
          },
          "slots": [{
            "is": "DynamicComponent",
            "props": {
              "style": {
                "height": "500px",
                "max-height": "75%",
                "overflow": "auto"
              },
              "components": [{
                  "is": "WidgetForm",
                  "props": {
                    "style": {
                      "max-width": "400px"
                    },
                    "fields": [{
                        "label": "ShipmentPage.houseNo",
                        "name": "houseNo",
                        "component": "v-text-field",
                        "class": "xs-6"
                      },
                      {
                        "label": "ShipmentPage.masterNo",
                        "name": "masterNo",
                        "component": "v-text-field",
                        "class": "xs-6"
                      },
                      {
                        "label": "ShipmentPage.carrier",
                        "name": ["carrier.name", "carrierName", "carrierCode"],
                        "component": "v-text-field",
                        "class": "xs-12"
                      },
                      {
                        "label": "ShipmentPage.portOfLoading",
                        "name": ["portOfLoading.name", "portOfLoadingName", "portOfLoadingCode"],
                        "component": "v-text-field",
                        "class": "xs-6"
                      },
                      {
                        "label": "ShipmentPage.portOfDischarge",
                        "name": ["portOfDischarge.name", "portOfDischargeName", "portOfDischargeCode"],
                        "component": "v-text-field",
                        "class": "xs-6"
                      },
                      {
                        "label": "ShipmentPage.departureDateEstimated",
                        "name": "shipmentDate.departureDateEstimated",
                        "component": "DateTimePicker",
                        "class": "xs-6",
                        "props": {
                          "type": "datetime",
                          "utc": true,
                          "defaultFormat": "ddd MMM DD YYYY",
                          "useOwnFormat": true
                        }
                      },
                      {
                        "label": "ShipmentPage.arrivalDateEstimated",
                        "name": "shipmentDate.arrivalDateEstimated",
                        "component": "DateTimePicker",
                        "class": "xs-6",
                        "props": {
                          "type": "datetime",
                          "utc": true,
                          "defaultFormat": "ddd MMM DD YYYY",
                          "useOwnFormat": true
                        }
                      },
                      {
                        "label": "ShipmentPage.departureDateActual",
                        "name": "shipmentDate.departureDateActual",
                        "component": "DateTimePicker",
                        "class": "xs-6",
                        "props": {
                          "type": "datetime",
                          "utc": true,
                          "defaultFormat": "ddd MMM DD YYYY",
                          "useOwnFormat": true
                        }
                      },
                      {
                        "label": "ShipmentPage.arrivalDateActual",
                        "name": "shipmentDate.arrivalDateActual",
                        "component": "DateTimePicker",
                        "class": "xs-6",
                        "props": {
                          "type": "datetime",
                          "utc": true,
                          "defaultFormat": "ddd MMM DD YYYY",
                          "useOwnFormat": true
                        }
                      }
                    ]
                  }
                },
                {
                  "is": "SmallTracking",
                  "props": {
                    "axiosBaseParams": [
                      "const trackingNos = [];",
                      "const data = context.$store.state.widget.data;",
                      "if (data.trackingNos) {",
                      "const { masterNo, containerNo, carrierBookingNo } = data.tracking",
                      "if (masterNo) { trackingNos.push(masterNo) }",
                      "if (containerNo && containerNo.length) { trackingNos.concat(containerNo) }",
                      "if (carrierBookingNo && carrierBookingNo.length) { trackingNos.concat(carrierBookingNo) }",
                      "}",
                      "return {",
                      "url: 'api/swivel-tracking/get',",
                      "method: 'POST',",
                      "data: { trackingNos }",
                      "};"
                    ],
                    "getModule": "moduleTypeCode",
                    "icon": "statusCode",
                    "titleKey": {
                      "key": "key",
                      "track": true
                    },
                    "subTitleKeys": [
                      "statusDescription",
                      "statusPlace",
                      {
                        "key": "statusDate",
                        "is": "Datetime",
                        "format": "YYYY-MM-DD HH:mm:ss"
                      }
                    ],
                    "iconMap": {
                      "BKCF": "tracking/bkcf",
                      "STSP": "tracking/stsp",
                      "EPRL": "tracking/eprl",
                      "GITM": "tracking/cy-cutoff",
                      "LOBD": "tracking/lobd",
                      "DLPT": "tracking/dlpt",
                      "TSLB": "tracking/lobd",
                      "TSBA": "tracking/cy-cutoff",
                      "BDAR": "tracking/dlpt",
                      "DSCH": "tracking/lobd",
                      "DECL": "tracking/sumit-so-date",
                      "PASS": "tracking/cus",
                      "TMPS": "tracking/sale-invoice-date",
                      "STCS": "tracking/stcs",
                      "RCVE": "tracking/eprl",
                      "BKD": "tracking/bkcf",
                      "RCS": "tracking/rcs",
                      "MNF": "tracking/eprl",
                      "DEP": "tracking/airplane-takeoff",
                      "TAR": "tracking/airplane-landing",
                      "TDE": "tracking/airplane-takeoff",
                      "ARR": "tracking/airplane-landing",
                      "RWB": "tracking/sale-invoice-date",
                      "RCF": "tracking/rcf",
                      "CUS": "tracking/cus",
                      "NFD": "tracking/nfd",
                      "MAN": "tracking/man",
                      "DLV": "tracking/stsp",
                      "CANF": "tracking/canf",
                      "ERR": "tracking/err",
                      "bookingReceivedByForwarder": "tracking/paperwork",
                      "oceanBill": "tracking/sumit-so-date",
                      "SEAdeparture": "tracking/ship-departure",
                      "SEAarrival": "tracking/ship-arrival",
                      "AIRdeparture": "tracking/air-departure",
                      "AIRarrival": "tracking/air-arrival",
                      "cargoReady": "tracking/cargo-ready",
                      "cyCutoff": "tracking/cy-cutoff",
                      "pickup": "tracking/pickup",
                      "delivery": "tracking/final-door-delivery",
                      "finalDoorDelivery": "tracking/final-door-delivery"
                    }
                  }
                }
              ]
            }
          }]
        }]
      }
    },
    {
      "is": "Map",
      "slots": [{
        "is": "Route",
        "props": {
          "startPoint": "widget.data.portOfLoadingCode",
          "endPoint": "widget.data.portOfDischargeCode",
          "estimatedStartTime": "widget.data.shipmentDate.departureDateEstimated",
          "actualStartTime": "widget.data.shipmentDate.departureDateActual",
          "estimatedEndTime": "widget.data.shipmentDate.arrivalDateEstimated",
          "actualEndTime": "widget.data.shipmentDate.arrivalDateActual",
          "mode": "widget.data.moduleTypeCode"
        }
      }]
    }
  ]
}