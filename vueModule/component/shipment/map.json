{
  "components": [{
      "is": "DynamicComponent",
      "props": {
        "layout": "v-card",
        "class": "absolute-topleft-20 zindex-300",
        "components": [{
          "is": "v-btn",
          "props": {
            "icon": true
          },
          "events": {
            "click.stop": [{
              "type": "dispatch",
              "otherParams": {
                "name": "widget/changeMode",
                "mode": "detail"
              }
            }]
          },
          "slots": [{
            "is": "Icon",
            "props": {
              "icon": "arrow_back"
            }
          }]
        }]
      }
    },
    {
      "is": "DynamicComponent",
      "props": {
        "layout": "v-card",
        "class": "absolute-topright-20 zindex-250",
        "components": [{
          "is": "ClickHidden",
          "props": {
            "desktopOpen": true
          },
          "slots": [{
            "is": "WidgetForm",
            "props": {
              "style": {
                "max-width": "300px"
              },
              "fields": [{
                  "label": "ShipmentPage.houseNo",
                  "name": "houseNo",
                  "component": "v-text-field",
                  "class": "xs-12"
                },
                {
                  "label": "ShipmentPage.masterNo",
                  "name": "masterNo",
                  "component": "v-text-field",
                  "class": "xs-12"
                },
                {
                  "label": "ShipmentPage.carrier",
                  "name": ["carrier.name", "carrierCode"],
                  "component": "v-text-field",
                  "class": "xs-12"
                },
                {
                  "label": "ShipmentPage.portOfLoading",
                  "name": ["pol.locationName", "portOfLoading"],
                  "component": "v-text-field",
                  "class": "xs-6"
                },
                {
                  "label": "ShipmentPage.portOfDischarge",
                  "name": ["pod.locationName", "portOfDischarge"],
                  "component": "v-text-field",
                  "class": "xs-6"
                },
                {
                  "label": "ShipmentPage.departureDateEstimated",
                  "name": "estimatedDepartureDate",
                  "component": "DateTimePicker",
                  "class": "xs-6 md-3",
                  "props": {
                    "type": "datetime",
                    "utc": true,
                    "defaultFormat": "ddd MMM DD YYYY",
                    "useOwnFormat": true
                  }
                },
                {
                  "label": "ShipmentPage.arrivalDateEstimated",
                  "name": "estimatedArrivalDate",
                  "component": "DateTimePicker",
                  "class": "xs-6 md-3",
                  "props": {
                    "type": "datetime",
                    "utc": true,
                    "defaultFormat": "ddd MMM DD YYYY",
                    "useOwnFormat": true
                  }
                }
              ]
            }
          }]
        }]
      }
    },
    {
      "is": "Map",
      "slots": [{
        "is": "Route",
        "props": {
          "startPoint": "widget.data.portOfLoading",
          "endPoint": "widget.data.portOfDischarge",
          "estimatedStartTime": "widget.data.departureDateEstimated",
          "actualStartTime": "widget.data.departureDateActual",
          "estimatedEndTime": "widget.data.arrivalDateEstimated",
          "actualEndTime": "widget.data.arrivalDateActual",
          "mode": "widget.data.moduleType"
        }
      }]
    }
  ]
}