{
  "class": "grid",
  "components": [{
    "is": "DynamicComponent",
    "props": {
      "class": "xs-12 grid min-height-28 align-item-center full-width",
      "components": [{
        "is": "v-btn",
        "props": {
          "small": true,
          "flat": true,
          "icon": true
        },
        "slots": [{
          "is": "Icon",
          "props": {
            "icon": "arrow_back"
          }
        }],
        "events": {
          "click.stop": [{
            "type": "router",
            "otherParams": {
              "func": "push",
              "path": "/trackings"
            }
          }]
        }
      }, {
        "is": "I18nText",
        "props": {
          "i18nContext": "TrackingPage",
          "i18nKey": "title",
          "i18nOptions": {
            "id": "{{context.$store.state.widget.data.name}}"
          }
        }
      }, {
        "is": "v-spacer"
      }, {
        "is": "v-btn",
        "props": {
          "small": true,
          "icon": true,
          "flat": true,
          "class": "primary white-text"
        },
        "slots": [{
          "is": "Icon",
          "props": {
            "small": true,
            "icon": "edit"
          }
        }],
        "events": {
          "click.stop": [{
            "type": "dispatch",
            "otherParams": {
              "name": "widget/changeMode",
              "mode": "edit"
            }
          }]
        }
      }]
    }
  }, {
    "is": "DynamicComponent",
    "props": {
      "layout": "v-card",
      "class": "xs-12 margin-bottom-8",
      "components": [{
        "is": "WidgetForm",
        "props": {
          "class": "xs-12",
          "fields": [{
              "label": "TrackingPage.trackingNo",
              "name": "trackingNo",
              "component": "v-text-field",
              "class": "xs-12"
            },
            {
              "name": "trackingContainers",
              "label": "TrackingPage.containers",
              "component": "GridField",
              "class": "xs-12",
              "props": {
                "overrideRowIndex": 1,
                "fields": [{
                  "label": "TrackingPage.containerNo",
                  "name": "containerNo",
                  "component": "v-text-field",
                  "class": "xs-12"
                }, {
                  "label": "TrackingPage.sealNo",
                  "name": "sealNo",
                  "component": "v-text-field",
                  "class": "xs-4 hidden"
                }, {
                  "label": "TrackingPage.container",
                  "name": "sealNo",
                  "component": "v-text-field",
                  "class": "xs-4 hidden"
                }, {
                  "label": "TrackingPage.status",
                  "name": "trackingStatus",
                  "component": "TableField",
                  "class": "xs-12",
                  "props": {
                    "fields": [{
                      "label": "TrackingPage.isEstimated",
                      "name": "isEstimated",
                      "component": "v-checkbox",
                      "colGroupProps": {
                        "style": {
                          "width": "5%"
                        }
                      }
                    }, {
                      "label": "TrackingPage.statusCode",
                      "name": "statusCode",
                      "component": "LocalAutoSuggest",
                      "colGroupProps": {
                        "style": {
                          "width": "20%"
                        }
                      },
                      "props": {
                        "canCreate": true,
                        "funcItems": [{
                          "text": "1",
                          "value": "111"
                        }]
                      }
                    }, {
                      "label": "TrackingPage.statusDate",
                      "name": "statusCode",
                      "component": "DateTimePicker",
                      "colGroupProps": {
                        "style": {
                          "width": "15%"
                        }
                      },
                      "props": {
                        "type": "datetime",
                        "utc": true,
                        "defaultFormat": "ddd MMM DD YYYY HH:mm:ss",
                        "useOwnFormat": true
                      }
                    }, {
                      "label": "TrackingPage.statusDescription",
                      "name": "statusDescription",
                      "component": "v-text-field",
                      "colGroupProps": {
                        "style": {
                          "width": "40%"
                        }
                      }
                    }, {
                      "label": "TrackingPage.statusPlace",
                      "name": "statusPlace",
                      "component": "v-text-field",
                      "colGroupProps": {
                        "style": {
                          "width": "20%"
                        }
                      }
                    }]
                  }
                }]
              }
            },
            {
              "label": "TrackingPage.status",
              "name": "trackingStatus",
              "checkDisplayWhen": "return form && form.trackingContainers && form.trackingContainers.length === 0",
              "component": "TableField",
              "class": "xs-12",
              "props": {
                "fields": [{
                  "label": "TrackingPage.isEstimated",
                  "name": "isEstimated",
                  "component": "v-checkbox",
                  "colGroupProps": {
                    "style": {
                      "width": "5%"
                    }
                  }
                }, {
                  "label": "TrackingPage.statusCode",
                  "name": "statusCode",
                  "component": "LocalAutoSuggest",
                  "colGroupProps": {
                    "style": {
                      "width": "20%"
                    }
                  },
                  "props": {
                    "canCreate": true,
                    "funcItems": [{
                      "text": "1",
                      "value": "111"
                    }]
                  }
                }, {
                  "label": "TrackingPage.statusDate",
                  "name": "statusCode",
                  "component": "DateTimePicker",
                  "colGroupProps": {
                    "style": {
                      "width": "15%"
                    }
                  },
                  "props": {
                    "type": "datetime",
                    "utc": true,
                    "defaultFormat": "ddd MMM DD YYYY HH:mm:ss",
                    "useOwnFormat": true
                  }
                }, {
                  "label": "TrackingPage.statusDescription",
                  "name": "statusDescription",
                  "component": "v-text-field",
                  "colGroupProps": {
                    "style": {
                      "width": "40%"
                    }
                  }
                }, {
                  "label": "TrackingPage.statusPlace",
                  "name": "statusPlace",
                  "component": "v-text-field",
                  "colGroupProps": {
                    "style": {
                      "width": "20%"
                    }
                  }
                }]
              }
            }
          ]
        }
      }]
    }
  }]
}