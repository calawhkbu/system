{
  "layout": "v-card",
  "components": [{
    "is": "WidgetForm",
    "props": {
      "fields": [{
        "label": "CustomerPage.party",
        "name": "parties",
        "component": "GridField",
        "class": "xs-12",
        "props": {
          "fields": [{
              "label": "CustomerPage.partyName",
              "name": "name",
              "component": "v-text-field",
              "class": "xs-12"
            }, {
              "label": "CustomerPage.partyShortName",
              "name": "shortName",
              "component": "v-text-field",
              "class": "xs-12"
            },
            {
              "label": "CustomerPage.email",
              "name": "email",
              "component": "v-text-field",
              "props": {
                "type": "email"
              },
              "class": "xs-3"
            },
            {
              "label": "CustomerPage.phone",
              "name": "phone",
              "component": "v-text-field",
              "class": "xs-3"
            },
            {
              "label": "CustomerPage.fax",
              "name": "fax",
              "component": "v-text-field",
              "class": "xs-3"
            },
            {
              "label": "CustomerPage.identity",
              "name": "identity",
              "component": "v-text-field",
              "class": "xs-3"
            },
            {
              "label": "CustomerPage.address",
              "name": "address",
              "component": "v-textarea",
              "props": {
                "auto-grow": true,
                "rows": 5
              },
              "class": "xs-12"
            },
            {
              "label": "CustomerPage.city",
              "name": "cityCode",
              "component": "AsyncAutoSuggest",
              "relation": "cityCode",
              "relationMore": [{
                "name": "cityCode",
                "selector": "locationCode"
              }],
              "props": {
                "canCreate": true,
                "canCreateValue": {
                  "id": null,
                  "code": "{{autoSuggestModel}}"
                },
                "searchTextParams": {
                  "url": "api/location/query/location",
                  "method": "POST",
                  "data": {
                    "subqueries": {
                      "q": {
                        "value": "{{context.search}}"
                      }
                    },
                    "limit": 5
                  }
                },
                "item-text": "name",
                "item-value": "locationCode"
              },
              "class": "xs-3"
            },
            {
              "label": "CustomerPage.state",
              "name": "stateCode",
              "component": "AsyncAutoSuggest",
              "relation": "stateCode",
              "relationMore": [{
                "name": "stateCode",
                "selector": "code"
              }],
              "props": {
                "canCreate": true,
                "canCreateValue": {
                  "id": null,
                  "code": "{{autoSuggestModel}}"
                },
                "searchTextParams": {
                  "url": "api/code/query/code_master",
                  "method": "POST",
                  "data": {
                    "subqueries": {
                      "codeType": {
                        "value": "STATE-PROVINCE"
                      },
                      "q": {
                        "value": "{{context.search}}"
                      }
                    },
                    "limit": 5
                  }
                },
                "item-text": "name",
                "item-value": "code"
              },
              "class": "xs-3"
            },
            {
              "label": "CustomerPage.country",
              "name": "countryCode",
              "component": "AsyncAutoSuggest",
              "relation": "countryCode",
              "relationMore": [{
                "name": "countryCode",
                "selector": "code"
              }],
              "props": {
                "canCreate": true,
                "canCreateValue": {
                  "id": null,
                  "code": "{{autoSuggestModel}}"
                },
                "searchTextParams": {
                  "url": "api/code/query/code_master",
                  "method": "POST",
                  "data": {
                    "subqueries": {
                      "codeType": {
                        "value": "COUNTRY"
                      },
                      "q": {
                        "value": "{{context.search}}"
                      }
                    },
                    "limit": 5
                  }
                },
                "item-text": "name",
                "item-value": "code"
              },
              "class": "xs-3"
            },
            {
              "label": "CustomerPage.zip",
              "name": "zip",
              "component": "v-text-field",
              "class": "xs-3"
            }
          ]
        }
      }]
    }
  }]
}