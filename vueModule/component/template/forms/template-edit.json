{
  "components": [{
      "is": "WidgetForm",
      "props": {
        "fields": [{
            "label": "TemplatePage.templateName",
            "name": "templateName",
            "component": "v-text-field",
            "class": "xs-12",
            "canEdit": "return false",
            "props": {
              "disable": true
            }
          },
          {
            "label": "TemplatePage.format",
            "name": "format",
            "component": "JsonField",
            "class": "xs-12"
          }
        ]
      }
    },
    {
      "is": "UploadDownloadButton",
      "props": {
        "uploadParams": {
          "axiosParams": {
            "method": "POST",
            "url": "/api/template/upload",
            "data": {
              "base64String": "{{base64String}}",
              "extension": "{{extension}}",
              "templateName": "{{context.$store.state.widget.data.templateName}}"
            }
          }
        },
        "downloadParams": {
          "fileName": "{{context.$store.state.widget.data.templateName}}",
          "extension": "{{context.$store.state.widget.data.extension}}",
          "axiosParams": {
            "responseType": "blob",
            "method": "POST",
            "url": "/api/template/download",
            "data": {
              "templateName": "{{context.$store.state.widget.data.templateName}}"
            }
          }
        }
      }
    }
  ]
}