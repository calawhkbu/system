{
  "layout": "v-card",
  "style": {
    "min-height": "350px"
  },
  "components": [
    {
      "is": "DynamicComponent",
      "props": {
        "components": [
          {
            "is": "v-card-title",
            "props": {
              "primary-title": true
            },
            "slots": [
              {
                "name": "default",
                "is": "I18nText",
                "props": {
                  "class": "title",
                  "i18nKey": "UserProfilePage.UserPreferences"
                }
              }
            ]
          },
          {
            "is": "Form",
            "props": {
              "form": "$store.state.config",
              "submitActions": [
                {
                  "type": "dispatch",
                  "otherParams": {
                    "name": "config/updateAll"
                  },
                  "errorMessage": "UserProfilePage.UpdateUserPreferencesFail",
                  "afterActions": [
                    {
                      "type": "notify",
                      "otherParams": {
                        "notification": "UserProfilePage.UpdateUserPreferencesSuccess"
                      }
                    }
                  ]
                }
              ],
              "fields": [
                {
                  "label": "config.locale",
                  "name": "locale",
                  "component": "AsyncSelect",
                  "class": "xs-12 md-6",
                  "props": {
                    "item-text": "value",
                    "item-value": "key",
                    "axiosParams": {
                      "url": "i18n/available-languages"
                    }
                  }
                },
                {
                  "label": "config.timezone",
                  "name": "timezone",
                  "component": "LocalAutoSuggest",
                  "class": "xs-12 md-6",
                  "props": {
                    "funcItems": "return context.$moment.tz.names()"
                  }
                },
                {
                  "label": "config.dateFormat",
                  "name": "dateFormat",
                  "component": "v-text-field",
                  "class": "xs-12 md-6",
                  "hint": "config.dateFormatHint"
                },
                {
                  "label": "config.timeFormat",
                  "name": "timeFormat",
                  "component": "v-text-field",
                  "class": "xs-12 md-6",
                  "hint": "config.timeFormatHint"
                },
                {
                  "label": "config.dateTimeFormat",
                  "name": "dateTimeFormat",
                  "component": "v-text-field",
                  "class": "xs-12 md-6",
                  "hint": "config.dateTimeFormatHint"
                },
                {
                  "label": "config.dow",
                  "name": "weekFormat.dow",
                  "component": "v-select",
                  "class": "xs-12 md-6",
                  "more": [
                    {
                      "name": "weekFormat.doy",
                      "func": "return 7 + value - 1"
                    }
                  ],
                  "props": {
                    "items": [
                      {
                        "text": "Sunday",
                        "value": 0
                      },
                      {
                        "text": "Monday",
                        "value": 1
                      },
                      {
                        "text": "Tuesday",
                        "value": 2
                      },
                      {
                        "text": "Wednesday",
                        "value": 3
                      },
                      {
                        "text": "Thursday",
                        "value": 4
                      },
                      {
                        "text": "Friday",
                        "value": 5
                      },
                      {
                        "text": "Saturday",
                        "value": 6
                      }
                    ]
                  }
                }
              ]
            },
            "slots": [
              {
                "name": "post-form",
                "is": "DynamicComponent",
                "props": {
                  "components": [
                    {
                      "is": "v-btn",
                      "props": {
                        "type": "submit",
                        "flat": true
                      },
                      "slots": [
                        {
                          "is": "i18nText",
                          "props": {
                            "i18nKey": "Save"
                          }
                        }
                      ]
                    }
                  ]
                }
              }
            ]
          }
        ]
      }
    }
  ]
}
