                  "required": true,
                  "axiosParams": {
                    "url": "api/code/query/code_master",
                    "method": "POST",
                    "data": {
                      "subqueries": {
                        "codeType": {
                          "value": "MODULE"
                        }
                      }
                    }
                  },
                  "item-text": "name",
