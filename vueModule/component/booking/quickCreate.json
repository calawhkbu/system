                    "url": "api/party/query/party_auto_suggest",
                    "searchValueParams": "{ \"subqueries\": { \"id\": {{query}}, \"partyTypes\": {\"value\": [\"shipper\"]} } }",
                    "searchTextParams": {
                      "subqueries": {
                        "q": {
                          "value": "{{query}}"
