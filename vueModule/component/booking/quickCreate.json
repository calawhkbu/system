                  "component": "AsyncSelect",
                  "class": "xs-12 md-6",
                  "relation": "incoTerms",
                  "relationMore": [{
                    "name": "freightTermsCode",
                    "selector": "data.frieghtTermsCode"
                  }, {
                    "name": "otherTermsCode",
                    "selector": "data.otherTermsCode"
                  }],
                  "props": {
                    "url": "api/code/query/code_master",
                    "item-text": "name",
                    "item-value": "code",
                    "params": {
                      "fields": [
                        "code_master.*",
                        "flex_data.data"
