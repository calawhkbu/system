{
  "class": "grid",
  "components": [{
      "is": "DynamicComponent",
      "props": {
        "class": "flex align-item-center full-width",
        "components": [{
            "is": "v-btn",
            "props": {
              "flat": true,
              "icon": true,
              "small": true
            },
            "events": {
              "click.stop": [{
                "type": "router",
                "otherParams": {
                  "func": "push",
                  "path": "/bookings"
                }
              }]
            },
            "slots": [{
              "is": "Icon",
              "props": {
                "icon": "arrow_back"
              }
            }]
          },
          {
            "is": "I18nText",
            "props": {
              "class": "headline",
              "i18nKey": "BookingPage.quickCreate",
              "swig": true
            }
          },
          {
            "is": "v-spacer"
          }, {
            "is": "v-btn",
            "props": {
              "fab": true,
              "small": true
            },
            "slots": [{
              "is": "Icon",
              "props": {
                "icon": "navigate_next",
                "small": true
              }
            }],
            "events": {
              "click.stop": [{
                "type": "dispatch",
                "otherParams": {
                  "name": "widget/changeMode",
                  "mode": "create"
                }
              }]
            }
          }
        ]
      }
    },
