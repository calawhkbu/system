          {
            "label": "BookingPage.placeOfReceipt",
            "name": "placeOfReceipt",
            "component": "AsyncAutoSuggest",
            "class": "xs-12 md-3",
            "readonlyClass": "hidden",
            "more": [{
              "name": "placeOfReceiptCode",
              "selector": "portCode"
            }],
            "props": {
              "method": "POST",
              "url": "api/location/query/location",
              "searchValueParams": {
                "subqueries": {
                  "moduleType": {
                    "value": "{{form.moduleTypeCode}}"
                  },
                  "ports": {
                    "value": "{{query}}"
                  }
                }
              },
              "searchTextParams": {
                "subqueries": {
                  "moduleType": {
                    "value": "{{form.moduleTypeCode}}"
                  },
                  "q": {
                    "value": "{{query}}"
                  }
                },
                "limit": 5
              },
              "item-text": "name",
              "item-value": "portCode"
            }
          },
          {
            "label": "BookingPage.portOfLoading",
