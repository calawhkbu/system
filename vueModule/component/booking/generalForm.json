          {
            "label": "BookingPage.moduleType",
            "name": "moduleTypeCode",
            "component": "AsyncSelect",
            "class": "xs-12 md-6",
            "props": {
              "url": "api/code",
              "item-text": "name",
              "item-value": "code",
              "params": {
                "where": {
                  "codeType": "moduleType"
                }
              }
            }
          },
          {
            "label": "BookingPage.boundType",
            "name": "boundTypeCode",
            "component": "AsyncAutoSuggest",
            "class": "xs-12 md-6",
            "props": {
              "url": "api/code",
              "searchValueParams": {
                "where": {
                  "codeType": "boundType",
                  "code": "{{query}}"
                }
              },
              "searchTextParams": {
                "where": {
                  "codeType": "boundType",
                  "$or": {
                    "code": "{{query}}",
                    "name": "{{query}}"
                  }
                },
                "limit": 5
              },
              "cache-items": true,
              "hide-no-data": true,
              "hide-details": true,
              "item-text": "name",
