            "label": "BookingPage.moduleType",
            "name": "moduleTypeCode",
            "component": "AsyncSelect",
            "class": "xs-12 md-4",
            "validator": [
              "required"
            ],
            "relation": "moduleType",
            "props": {
              "required": true,
              "url": "api/code/query/code_master",
              "item-text": "name",
              "item-value": "code",
              "params": {
                "subqueries": {
                  "codeType": {
                    "value": "MODULE"
                  }
                }
              }
            }
          },
          {
            "label": "BookingPage.boundType",
