{
  "layout": "v-card",
  "style": {
    "min-height": "75px"
  },
  "components": [
    {
      "is": "WidgetForms",
      "props": {
        "formProps": {
          "class": "xs-12 md-6"
        },
        "keyProps": {
          "key": "Party",
          "fixKey": [
            "shipper",
            "consignee",
            "forwarder",
            "agent",
            "notifyParty"
          ],
          "orderBy": {
            "shipper": 0,
            "consignee": 10,
            "forwarder": 20,
            "agent": 30,
            "notifyParty": 40
          }
        },
        "extraContainerProps": {
          "class": "xs-12 md-6"
        },
        "params": {
          "query": "{{query}}"
        },
        "fields": [
          {
            "name": "{{key}}PartyId",
            "component": "AsyncAutoSuggest",
            "class": "xs-12",
            "props": {
              "url": "api/party/query/party",
              "searchValueParams": "{ \"subqueries\": { \"id\": {{query}} } }",
              "searchTextParams": {
                "subqueries": {
                  "q": {
                    "value": "{{query}}"
                  }
                },
                "limit": 5
              },
              "cache-items": true,
              "hide-no-data": true,
              "hide-details": true,
              "item-text": "name",
              "item-value": "partyId"
            }
          },
          {
            "label": "Widget.contactName",
            "name": "{{key}}PartyContactName",
            "component": "v-text-field",
            "class": "xs-12 md-4",
            "readonlyClass": "hidden"
          },
          {
            "label": "Widget.contactEmail",
            "name": "{{key}}PartyContactEmail",
            "component": "v-text-field",
            "class": "xs-12 md-4",
            "readonlyClass": "hidden"
          },
          {
            "label": "Widget.contactPhone",
            "name": "{{key}}PartyContactPhone",
            "component": "v-text-field",
            "class": "xs-12 md-4",
            "readonlyClass": "hidden"
          },
          {
            "name": "flexData.data.{{key}}PartyContact",
            "component": "TableField",
            "class": "xs-12",
            "readonlyClass": "hidden",
            "props": {
              "overrideRowIndex": 1,
              "fields": [
                {
                  "label": "Widget.contactXName",
                  "name": "name",
                  "component": "v-text-field",
                  "class": "xs-12 md-4",
                  "_readonlyClass": "hidden"
                },
                {
                  "label": "Widget.contactXEmail",
                  "name": "email",
                  "component": "v-text-field",
                  "class": "xs-12 md-4",
                  "_readonlyClass": "hidden"
                },
                {
                  "label": "Widget.contactXPhone",
                  "name": "phone",
                  "component": "v-text-field",
                  "class": "xs-12 md-4",
                  "_readonlyClass": "hidden"
                }
              ]
            }
          },
          {
            "label": "Widget.partyEmail",
            "name": "{{key}}Email",
            "component": "v-text-field",
            "class": "xs-12 md-4",
            "readonlyClass": "hidden"
          },
          {
            "label": "Widget.partyPhone",
            "name": "{{key}}Phone",
            "component": "v-text-field",
            "class": "xs-12 md-4",
            "readonlyClass": "hidden"
          },
          {
            "label": "Widget.partyFax",
            "name": "{{key}}Fax",
            "component": "v-text-field",
            "class": "xs-12 md-4",
            "readonlyClass": "hidden"
          },
          {
            "label": "Widget.address",
            "name": "{{key}}Address",
            "component": "v-textarea",
            "props": {
              "auto-grow": true,
              "rows": 5
            },
            "class": "xs-12",
            "readonlyClass": "hidden"
          },
          {
            "label": "Widget.city",
            "name": "{{key}}CityCode",
            "component": "AsyncAutoSuggest",
            "class": "xs-12 md-6",
            "readonlyClass": "xs-4",
            "props": {
              "url": "api/code/query/code_master",
              "searchValueParams": {
                "subqueries": {
                  "codeType": {
                    "value": "location"
                  },
                  "q": {
                    "value": "{{query}}"
                  }
                }
              },
              "searchTextParams": {
                "subqueries": {
                  "codeType": {
                    "value": "location"
                  },
                  "q": {
                    "value": "{{query}}"
                  }
                },
                "limit": 5
              },
              "cache-items": true,
              "hide-no-data": true,
              "hide-details": true,
              "item-text": "name",
              "item-value": "code"
            }
          },
          {
            "label": "Widget.state",
            "name": "{{key}}StateCode",
            "component": "AsyncAutoSuggest",
            "class": "xs-12 md-6",
            "readonlyClass": "xs-4",
            "props": {
              "url": "api/code/query/code_master",
              "searchValueParams": {
                "subqueries": {
                  "codeType": {
                    "value": "state"
                  },
                  "q": {
                    "value": "{{query}}"
                  }
                }
              },
              "searchTextParams": {
                "subqueries": {
                  "codeType": {
                    "value": "state"
                  },
                  "q": {
                    "value": "{{query}}"
                  }
                },
                "limit": 5
              },
              "cache-items": true,
              "hide-no-data": true,
              "hide-details": true,
              "item-text": "name",
              "item-value": "code"
            }
          },
          {
            "label": "Widget.country",
            "name": "{{key}}CountryCode",
            "component": "AsyncAutoSuggest",
            "class": "xs-12 md-6",
            "readonlyClass": "xs-4",
            "props": {
              "url": "api/code/query/code_master",
              "searchValueParams": {
                "subqueries": {
                  "codeType": {
                    "value": "country"
                  },
                  "q": {
                    "value": "{{query}}"
                  }
                }
              },
              "searchTextParams": {
                "subqueries": {
                  "codeType": {
                    "value": "country"
                  },
                  "q": {
                    "value": "{{query}}"
                  }
                },
                "limit": 5
              },
              "cache-items": true,
              "hide-no-data": true,
              "hide-details": true,
              "item-text": "name",
              "item-value": "code"
            }
          },
          {
            "label": "Widget.zip",
            "name": "{{key}}Zip",
            "component": "v-text-field",
            "class": "xs-12 md-6",
            "readonlyClass": "hidden"
          }
        ]
      }
    }
  ]
}
