{
  "components": [
    {
      "is": "WidgetForms",
      "props": {
        "formProps": {
          "class": "xs-12"
        },
        "keyProps": {
          "key": "Date",
          "fixKey": [
            "cargoReady",
            "pickup",
            "cYCutOff",
            "departure",
            "arrival",
            "finalDoorDelivery"
          ],
          "orderBy": {
            "cargoReady": 5,
            "pickup": 10,
            "cYCutOff": 20,
            "departure": 30,
            "arrival": 40,
            "finalDoorDelivery": 50
          }
        },
        "formProps": {
          "class": "xs-12 padding-4"
        },
        "labelProps": {
          "class": "font-110"
        },
        "nonFixedLabelProps": {
          "cargoReady": {
            "class": "font-110 blue--text"
          },
          "cYCutOff": {
            "class": "font-110 teal--text"
          },
          "pickup": {
            "class": "font-110 green--text"
          },
          "departure": {
            "class": "font-110 brown--text"
          },
          "arrival": {
            "class": "font-110 indigo--text"
          },
          "finalDoorDelivery": {
            "class": "font-110 pink--text"
          }
        },
        "fields": [
          {
            "label": "Widget.estimatedDate",
            "name": "{{key}}DateEstimated",
            "component": "DateTimePicker",
            "class": "xs-12 md-5",
            "props": {
              "type": "datetime",
              "utc": true,
              "defaultFormat": "ddd MMM DD YYYY HH:mm:ss",
              "useOwnFormat": true
            }
          },
          {
            "label": "Widget.actualDate",
            "name": "{{key}}DateActual",
            "component": "DateTimePicker",
            "class": "xs-12 md-5",
            "props": {
              "type": "datetime",
              "utc": true,
              "defaultFormat": "ddd MMM DD YYYY HH:mm:ss",
              "useOwnFormat": true
            }
          },
          {
            "label": "Widget.remark",
            "name": "{{key}}Remark",
            "component": "v-text-field",
            "class": "xs-12 md-2"
          }
        ]
      }
    }
  ]
}
