{
  "components": [{
    "is": "WidgetForm",
    "class": "xs-12",
    "props": {
      "fields": [{
          "name": "shipperParty",
          "component": "AsyncAutoSuggest",
          "class": "xs-12",
          "relation": "shipperParty",
          "relationMore": [{
              "name": "shipperPartyId",
              "selector": "id"
            },
            {
              "name": "shipperPartyName",
              "selector": "name"
            },
            {
              "name": "shipperPartyPhone",
              "selector": "partyPhone"
            },
            {
              "name": "shipperPartyFax",
              "selector": "partyFax"
            },
            {
              "name": "shipperPartyEmail",
              "selector": "partyEmail"
            },
            {
              "name": "shipperPartyAddress",
              "selector": "partyAddress"
            },
            {
              "name": "shipperPartyCityCode",
              "selector": "partyCityCode"
            },
            {
              "name": "shipperPartyStateCode",
              "selector": "partyStateCode"
            },
            {
              "name": "shipperPartyCountryCode",
              "selector": "partyCountryCode"
            },
            {
              "name": "shipperPartyZip",
              "selector": "partyZip"
            },
            {
              "name": "shipperPartyContactPersonId",
              "selector": "contactPersonId"
            },
            {
              "name": "shipperPartyContactPersonEmail",
              "selector": "contactPersonEmail"
            },
            {
              "name": "shipperPartyContactPersonName",
              "selector": "contactPersonName"
            },
            {
              "name": "shipperPartyContactPersonPhone",
              "selector": "contactPersonPhone"
            }
          ],
          "props": {
            "canCreate": true,
            "canCreateValue": {
              "id": null,
              "name": "{{autoSuggestModel}}"
            },
            "searchValueParams": {
              "url": "api/party/{{context.autoSuggestModel}}"
            },
            "searchTextParams": {
              "url": "api/party/query/party_auto_suggest",
              "method": "POST",
              "data": {
                "subqueries": {
                  "q": {
                    "value": "{{context.search}}"
                  },
                  "partyTypes": {
                    "value": ["{{key}}"]
                  }
                },
                "limit": 5
              }
            },
            "item-text": "name",
            "item-value": "erpCode"
          }
        },
        {
          "label": "Widget.contactName",
          "name": "shipperPartyContactName",
          "component": "v-text-field",
          "class": "xs-12 md-4",
          "readonlyClass": "hidden"
        },
        {
          "label": "Widget.contactEmail",
          "name": "shipperPartyContactEmail",
          "component": "v-text-field",
          "class": "xs-12 md-4",
          "readonlyClass": "hidden"
        },
        {
          "label": "Widget.contactPhone",
          "name": "shipperPartyContactPhone",
          "component": "v-text-field",
          "class": "xs-12 md-4",
          "readonlyClass": "hidden"
        },
        {
          "name": "shipperPartyContacts",
          "component": "TableField",
          "class": "xs-12",
          "readonlyClass": "hidden",
          "props": {
            "overrideRowIndex": 1,
            "fields": [{
                "label": "Widget.contactXName",
                "name": "Name",
                "component": "v-text-field",
                "class": "xs-12 md-4",
                "_readonlyClass": "hidden"
              },
              {
                "label": "Widget.contactXEmail",
                "name": "Email",
                "component": "v-text-field",
                "class": "xs-12 md-4",
                "_readonlyClass": "hidden"
              },
              {
                "label": "Widget.contactXPhone",
                "name": "Phone",
                "component": "v-text-field",
                "class": "xs-12 md-4",
                "_readonlyClass": "hidden"
              }
            ]
          }
        },
        {
          "label": "Widget.address",
          "name": "shipperPartyAddress",
          "component": "v-textarea",
          "props": {
            "auto-grow": true,
            "rows": 5
          },
          "class": "xs-12",
          "readonlyClass": "hidden"
        },
        {
          "label": "Widget.city",
          "name": "shipperPartyCityCode",
          "component": "AsyncAutoSuggest",
          "class": "xs-12 md-4",
          "readonlyClass": "xs-12 md-4",
          "relation": "shipperPartyCityCode",
          "relationMore": [{
            "name": "shipperPartyCityCode",
            "selector": "locationCode"
          }],
          "props": {
            "canCreate": true,
            "canCreateValue": {
              "id": null,
              "code": "{{autoSuggestModel}}"
            },
            "searchTextParams": {
              "url": "api/location/query/location",
              "method": "POST",
              "data": {
                "subqueries": {
                  "q": {
                    "value": "{{context.search}}"
                  }
                },
                "limit": 5
              }
            },
            "item-text": "name",
            "item-value": "locationCode"
          }
        },
        {
          "label": "Widget.state",
          "name": "shipperPartyStateCode",
          "component": "AsyncAutoSuggest",
          "class": "xs-12 md-4",
          "readonlyClass": "xs-12 md-4",
          "relation": "shipperPartyStateCode",
          "relationMore": [{
            "name": "shipperPartyStateCode",
            "selector": "code"
          }],
          "props": {
            "canCreate": true,
            "canCreateValue": {
              "id": null,
              "code": "{{autoSuggestModel}}"
            },
            "searchTextParams": {
              "url": "api/code/query/code_master",
              "method": "POST",
              "data": {
                "subqueries": {
                  "codeType": {
                    "value": "STATE-PROVINCE"
                  },
                  "q": {
                    "value": "{{context.search}}"
                  }
                },
                "limit": 5
              }
            },
            "item-text": "name",
            "item-value": "code"
          }
        },
        {
          "label": "Widget.country",
          "name": "shipperPartyCountryCode",
          "component": "AsyncAutoSuggest",
          "class": "xs-12 md-4",
          "readonlyClass": "xs-12 md-4",
          "relation": "shipperPartyCountryCode",
          "relationMore": [{
            "name": "shipperPartyCountryCode",
            "selector": "code"
          }],
          "props": {
            "canCreate": true,
            "canCreateValue": {
              "id": null,
              "code": "{{autoSuggestModel}}"
            },
            "searchTextParams": {
              "url": "api/code/query/code_master",
              "method": "POST",
              "data": {
                "subqueries": {
                  "codeType": {
                    "value": "COUNTRY"
                  },
                  "q": {
                    "value": "{{context.search}}"
                  }
                },
                "limit": 5
              }
            },
            "item-text": "name",
            "item-value": "code"
          }
        },
        {
          "label": "Widget.zip",
          "name": "shipperPartyZip",
          "component": "v-text-field",
          "class": "xs-12 md-6",
          "readonlyClass": "hidden"
        }
      ]
    }
  }]
}