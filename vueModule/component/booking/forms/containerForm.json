{
  "components": [{
      "is": "WidgetForm",
      "class": "xs-12",
      "props": {
        "fields": [{
            "label": "BookingPage.poNo",
            "name": "flexData.data.poNo",
            "component": "v-text-field",
            "class": "xs-12 md-6"
          },
          {
            "label": "BookingPage.cargoValueCurrencyCode",
            "name": "cargoValueCurrencyCode",
            "component": "AsyncAutoSuggest",
            "class": "xs-12 md-2",
            "more": [{
              "name": "cargoValueCurrencyCode",
              "selector": "code"
            }],
            "props": {
              "searchTextParams": {
                "url": "api/code/query/code_master",
                "method": "POST",
                "data": {
                  "subqueries": {
                    "codeType": {
                      "value": "CURRENCY"
                    },
                    "q": {
                      "value": "{{context.search}}"
                    }
                  }
                }
              },
              "onInit": true,
              "item-text": "code",
              "item-value": "code",
              "showValue": false
            }
          },
          {
            "label": "BookingPage.cargoValue",
            "name": "cargoValue",
            "component": "v-text-field",
            "class": "xs-12 md-4",
            "props": {
              "type": "number",
              "min": 0
            }
          },
          {
            "label": "BookingPage.BookingPacking",
            "name": "bookingPopacking",
            "component": "TableField",
            "props": {
              "topLabel": true,
              "fields": [{
                  "label": "BookingPage.ctns",
                  "name": "ctns",
                  "component": "v-text-field",
                  "props": {
                    "type": "number",
                    "min": 0
                  },
                  "slots": [{
                    "name": "append",
                    "is": "I18nText",
                    "props": {
                      "i18nKey": "CTNS"
                    }
                  }]
                },
                {
                  "label": "BookingPage.pieces",
                  "name": "pieces",
                  "component": "v-text-field",
                  "props": {
                    "type": "number",
                    "min": 0
                  }
                },
                {
                  "label": "BookingPage.piecesUnit",
                  "name": "piecesUnit",
                  "component": "v-select",
                  "props": {
                    "items": [{
                        "text": "W/C",
                        "value": "W/C"
                      },
                      {
                        "text": "PCS",
                        "value": "PCS"
                      },
                      {
                        "text": "M/C",
                        "value": "M/C"
                      },
                      {
                        "text": "BAG",
                        "value": "BAG"
                      },
                      {
                        "text": "CASE",
                        "value": "CASE"
                      },
                      {
                        "text": "PLT",
                        "value": "PLT"
                      },
                      {
                        "text": "ROLL",
                        "value": "ROLL"
                      },
                      {
                        "text": "PAIR",
                        "value": "PAIR"
                      },
                      {
                        "text": "CTN",
                        "value": "CTN"
                      }
                    ]
                  }
                },
                {
                  "label": "BookingPage.volume",
                  "name": "volume",
                  "component": "v-text-field",
                  "props": {
                    "type": "number",
                    "min": 0
                  },
                  "slots": [{
                    "name": "append",
                    "is": "I18nText",
                    "props": {
                      "i18nKey": "CBM"
                    }
                  }]
                },
                {
                  "label": "BookingPage.length",
                  "name": "length",
                  "component": "v-text-field",
                  "props": {
                    "type": "number",
                    "min": 0
                  },
                  "slots": [{
                    "name": "append",
                    "is": "I18nText",
                    "props": {
                      "i18nKey": "X"
                    }
                  }]
                },
                {
                  "label": "BookingPage.width",
                  "name": "width",
                  "component": "v-text-field",
                  "props": {
                    "type": "number",
                    "min": 0
                  },
                  "slots": [{
                    "name": "append",
                    "is": "I18nText",
                    "props": {
                      "i18nKey": "X"
                    }
                  }]
                },
                {
                  "label": "BookingPage.height",
                  "name": "height",
                  "component": "v-text-field",
                  "props": {
                    "type": "number",
                    "min": 0
                  }
                },
                {
                  "label": "BookingPage.lwhUnit",
                  "name": "lwhUnit",
                  "component": "v-select",
                  "props": {
                    "items": [{
                        "text": "CM",
                        "value": "CM"
                      },
                      {
                        "text": "IN",
                        "value": "IN"
                      }
                    ]
                  }
                },
                {
                  "label": "BookingPage.weight",
                  "name": "weight",
                  "component": "v-text-field",
                  "props": {
                    "type": "number"
                  }
                },
                {
                  "label": "BookingPage.weightUnit",
                  "name": "weightUnit",
                  "component": "v-select",
                  "props": {
                    "items": [{
                        "text": "KGs",
                        "value": "KGs"
                      },
                      {
                        "text": "LBs",
                        "value": "LBs"
                      }
                    ]
                  }
                }
              ]
            }
          }
        ]
