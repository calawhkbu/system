          "label": "BookingPage.BookingContainer",
          "name": "bookingContainers",
          "component": "TableField",
          "props": {
            "topLabel": true,
            "fields": [{
                "label": "BookingPage.containerTypeCode",
                "name": "containerTypeCode",
                "component": "AsyncSelect",
                "props": {
                  "item-text": "name",
                  "item-value": "code",
                  "axiosParams": {
                    "url": "api/code/query/code_master",
                    "method": "POST",
                    "data": {
                      "subqueries": {
                        "codeType": {
                          "value": "CONTTYPE"
                        }
                      }
                    }
                  }
                }
              }, {
                "label": "BookingPage.containerNo",
                "name": "containerNo",
                "component": "v-text-field",
                "props": {
                  "color": "blue"
                }
              },
              {
                "label": "BookingPage.sealNo",
                "name": "sealNo",
                "component": "v-text-field"
              },
              {
                "label": "BookingPage.soNo",
                "name": "soNo",
                "component": "v-text-field"
              },
              {
                "label": "BookingPage.ctns",
                "name": "ctns",
                "component": "v-text-field",
                "props": {
                  "type": "number",
                  "min": 0
                },
                "slots": [{
                  "name": "append",
                  "is": "I18nText",
                  "props": {
                    "i18nKey": "CTNS"
                  }
                }]
              }
            ]
          }
        }]
      }
    }
  ]
