{
  "layout": "v-card",
  "class": "grid",
  "components": [{
      "is": "WidgetForm",
      "class": "xs-12",
      "props": {
        "fields": [{
          "label": "BookingPage.BookingPacking",
          "name": "bookingPopacking",
          "component": "TableField",
          "props": {
            "topLabel": true,
            "fields": [{
                "label": "BookingPage.ctns",
                "name": "ctns",
                "component": "v-text-field",
                "props": {
                  "type": "number",
                  "min": 0
                },
                "slots": [{
                  "name": "append",
                  "is": "I18nText",
                  "props": {
                    "i18nKey": "CTNS"
                  }
                }]
              },
              {
                "label": "BookingPage.pieces",
                "name": "pieces",
                "component": "v-text-field",
                "props": {
                  "type": "number",
                  "min": 0
                }
              },
              {
                "label": "BookingPage.piecesUnit",
                "name": "piecesUnit",
                "component": "v-select",
                "props": {
                  "items": [{
                      "text": "W/C",
                      "value": "W/C"
                    },
                    {
                      "text": "PCS",
                      "value": "PCS"
                    },
                    {
                      "text": "M/C",
                      "value": "M/C"
                    },
                    {
                      "text": "BAG",
                      "value": "BAG"
                    },
                    {
                      "text": "CASE",
                      "value": "CASE"
                    },
                    {
                      "text": "PLT",
                      "value": "PLT"
                    },
                    {
                      "text": "ROLL",
                      "value": "ROLL"
                    },
                    {
                      "text": "PAIR",
                      "value": "PAIR"
                    },
                    {
                      "text": "CTN",
                      "value": "CTN"
                    }
                  ]
                }
              }, {
                "label": "BookingPage.volume",
                "name": "volume",
                "component": "v-text-field",
                "props": {
                  "type": "number",
                  "min": 0
                },
                "slots": [{
                  "name": "append",
                  "is": "I18nText",
                  "props": {
                    "i18nKey": "CBM"
                  }
                }]
              },
              {
                "label": "BookingPage.length",
                "name": "length",
                "component": "v-text-field",
                "props": {
                  "type": "number",
                  "min": 0
                },
                "slots": [{
                  "name": "append",
                  "is": "I18nText",
                  "props": {
                    "i18nKey": "X"
                  }
                }]
              },
              {
                "label": "BookingPage.width",
                "name": "width",
                "component": "v-text-field",
                "props": {
                  "type": "number",
                  "min": 0
                },
                "slots": [{
                  "name": "append",
                  "is": "I18nText",
                  "props": {
                    "i18nKey": "X"
                  }
                }]
              },
              {
                "label": "BookingPage.height",
                "name": "height",
                "component": "v-text-field",
                "props": {
                  "type": "number",
                  "min": 0
                }
              },
              {
                "label": "BookingPage.lwhUnit",
                "name": "lwhUnit",
                "component": "v-select",
                "props": {
                  "items": [{
                      "text": "CM",
                      "value": "CM"
                    },
                    {
                      "text": "IN",
                      "value": "IN"
                    }
                  ]
                }
              },
              {
                "label": "BookingPage.weight",
                "name": "weight",
                "component": "v-text-field",
                "props": {
                  "type": "number"
                }
              },
              {
                "label": "BookingPage.weightUnit",
                "name": "weightUnit",
                "component": "v-select",
                "props": {
                  "items": [{
                      "text": "KGs",
                      "value": "KGs"
                    },
                    {
                      "text": "LBs",
                      "value": "LBs"
                    }
                  ]
                }
              }
            ]
          }
        }]
      }
    },
    {
      "is": "WidgetForm",
      "class": "xs-12",
      "props": {
        "fields": [{
          "label": "BookingPage.BookingContainer",
          "name": "bookingContainers",
          "component": "TableField",
          "props": {
            "topLabel": true,
            "fields": [{
                "label": "BookingPage.containerNo",
                "name": "containerNo",
                "component": "v-text-field",
                "props": {
                  "color": "blue"
                }
              },
              {
                "label": "BookingPage.containerTypeCode",
                "name": "containerTypeCode",
                "component": "AsyncSelect",
                "props": {
                  "url": "api/code/query/code_master",
                  "item-text": "name",
                  "item-value": "code",
                  "params": {
                    "subqueries": {
                      "codeType": {
                        "value": "CONTTYPE"
                      }
                    }
                  }
                }
              },
              {
                "label": "BookingPage.sealNo",
                "name": "sealNo",
                "component": "v-text-field"
              },
              {
                "label": "BookingPage.quantity",
                "name": "quantity",
                "component": "v-text-field"
              },
              {
                "label": "BookingPage.quantityUnit",
                "name": "quantityUnit",
                "component": "v-select",
                "props": {
                  "items": [{
                    "text": "CTNS",
                    "value": "CTNS"
                  }]
                }
              }
            ]
          }
        }]
      }
    }
  ]
