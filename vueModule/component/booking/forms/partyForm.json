{
  "components": [{
    "is": "WidgetForms",
    "props": {
      "canAdd": false,
      "canAddProps": {
        "class": "xs-12 md-6",
        "style": {
          "display": "flex",
          "align-items": "center",
          "width": "calc(100% - 4px)",
          "height": "calc(100% - 4px)",
          "margin": "auto"
        }
      },
      "addObject": {},
      "extraContainerProps": {
        "class": "xs-12 md-6"
      },
      "formProps": {
        "class": "xs-12 md-6",
        "style": {
          "padding": "4px"
        }
      },
      "labelProps": {
        "style": {
          "font-size": "1.2rem",
          "font-weight": "bold"
        }
      },
      "nonFixedLabelProps": {
        "shipper": {
          "class": "required form-label"
        },
        "consignee": {
          "class": "required form-label"
        }
      },
      "keyProps": {
        "key": "Party",
        "fixKey": [
          "shipper",
          "consignee",
          "forwarder",
          "agent",
          "notifyParty"
        ],
        "orderBy": {
          "shipper": 0,
          "consignee": 10,
          "forwarder": 20,
          "agent": 30,
          "notifyParty": 40
        }
      },
      "params": {
        "autoSuggestModel": "{{autoSuggestModel}}"
      },
      "fields": [{
          "name": "{{key}}Party",
          "component": "AsyncAutoSuggest",
          "class": "xs-12",
          "relation": "{{key}}Party",
          "relationMore": [{
              "name": "{{key}}PartyId",
              "selector": "id"
            },
            {
              "name": "{{key}}PartyName",
              "selector": "name"
            },
            {
              "name": "{{key}}PartyPhone",
              "selector": "partyPhone"
            },
            {
              "name": "{{key}}PartyFax",
              "selector": "partyFax"
            },
            {
              "name": "{{key}}PartyEmail",
              "selector": "partyEmail"
            },
            {
              "name": "{{key}}PartyAddress",
              "selector": "partyAddress"
            },
            {
              "name": "{{key}}PartyCityCode",
              "selector": "partyCityCode"
            },
            {
              "name": "{{key}}PartyStateCode",
              "selector": "partyStateCode"
            },
            {
              "name": "{{key}}PartyCountryCode",
              "selector": "partyCountryCode"
            },
            {
              "name": "{{key}}PartyZip",
              "selector": "partyZip"
            },
            {
              "name": "{{key}}PartyContactPersonId",
              "selector": "contactPersonId"
            },
            {
              "name": "{{key}}PartyContactPersonId",
              "selector": "contactPersonId"
            },
            {
              "name": "{{key}}PartyContactPersonId",
              "selector": "contactPersonId"
            },
            {
              "name": "{{key}}PartyContactPersonId",
              "selector": "contactPersonId"
            }
          ],
          "props": {
            "showValue": false,
            "canCreate": true,
            "canCreateValue": {
              "id": null,
              "name": "{{autoSuggestModel|default(null)}}"
            },
            "url": "api/party/query/party_auto_suggest",
            "searchValueParams": "{ \"subqueries\": { \"id\": {{query}}, \"partyTypes\": {\"value\": [\"{{key}}\"]} } }",
            "searchTextParams": {
              "subqueries": {
                "q": {
                  "value": "{{query}}"
                },
                "partyTypes": {
                  "value": ["{{key}}"]
                }
              },
              "limit": 5
            },
            "item-text": "name",
            "item-value": "id"
          }
        },
        {
          "label": "Widget.contactName",
          "name": "{{key}}PartyContactName",
          "component": "v-text-field",
          "class": "xs-12 md-4",
          "readonlyClass": "hidden"
        },
        {
          "label": "Widget.contactEmail",
          "name": "{{key}}PartyContactEmail",
          "component": "v-text-field",
          "class": "xs-12 md-4",
          "readonlyClass": "hidden"
        },
        {
          "label": "Widget.contactPhone",
          "name": "{{key}}PartyContactPhone",
          "component": "v-text-field",
          "class": "xs-12 md-4",
          "readonlyClass": "hidden"
        },
        {
          "name": "flexData.data.{{key}}PartyContact",
          "component": "TableField",
          "class": "xs-12",
          "readonlyClass": "hidden",
          "props": {
            "overrideRowIndex": 1,
            "fields": [{
                "label": "Widget.contactXName",
                "name": "name",
                "component": "v-text-field",
                "class": "xs-12 md-4",
                "_readonlyClass": "hidden"
              },
              {
                "label": "Widget.contactXEmail",
                "name": "email",
                "component": "v-text-field",
                "class": "xs-12 md-4",
                "_readonlyClass": "hidden"
              },
              {
                "label": "Widget.contactXPhone",
                "name": "phone",
                "component": "v-text-field",
                "class": "xs-12 md-4",
                "_readonlyClass": "hidden"
              }
            ]
          }
        },
        {
          "label": "Widget.partyEmail",
          "name": "{{key}}PartyEmail",
          "component": "v-text-field",
          "class": "xs-12 md-4",
          "readonlyClass": "hidden"
        },
        {
          "label": "Widget.partyPhone",
          "name": "{{key}}PartyPhone",
          "component": "v-text-field",
          "class": "xs-12 md-4",
          "readonlyClass": "hidden"
        },
        {
          "label": "Widget.partyFax",
          "name": "{{key}}PartyFax",
          "component": "v-text-field",
          "class": "xs-12 md-4",
          "readonlyClass": "hidden"
        },
        {
          "label": "Widget.address",
          "name": "{{key}}PartyAddress",
          "component": "v-textarea",
          "props": {
            "auto-grow": true,
            "rows": 5
          },
          "class": "xs-12",
          "readonlyClass": "hidden"
        },
        {
          "label": "Widget.city",
          "name": "{{key}}PartyCityCode",
          "component": "AsyncAutoSuggest",
          "class": "xs-12 md-6",
          "readonlyClass": "xs-4",
          "props": {
            "url": "api/code/query/code_master",
            "searchValueParams": {
              "subqueries": {
                "codeType": {
                  "value": "LOCATION"
                },
                "q": {
                  "value": "{{query}}"
                }
              }
            },
            "searchTextParams": {
              "subqueries": {
                "codeType": {
                  "value": "LOCATION"
                },
                "q": {
                  "value": "{{query}}"
                }
              },
              "limit": 5
            },



            "item-text": "name",
            "item-value": "code"
          }
        },
        {
          "label": "Widget.state",
          "name": "{{key}}PartyStateCode",
          "component": "AsyncAutoSuggest",
          "class": "xs-12 md-6",
          "readonlyClass": "xs-4",
          "props": {
            "url": "api/code/query/code_master",
            "searchValueParams": {
              "subqueries": {
                "codeType": {
                  "value": "STATE-PROVINCE"
                },
                "q": {
                  "value": "{{query}}"
                }
              }
            },
            "searchTextParams": {
              "subqueries": {
                "codeType": {
                  "value": "STATE-PROVINCE"
                },
                "q": {
                  "value": "{{query}}"
                }
              },
              "limit": 5
            },



            "item-text": "name",
            "item-value": "code"
          }
        },
        {
          "label": "Widget.country",
          "name": "{{key}}PartyCountryCode",
          "component": "AsyncAutoSuggest",
          "class": "xs-12 md-6",
          "readonlyClass": "xs-4",
          "props": {
            "url": "api/code/query/code_master",
            "searchValueParams": {
              "subqueries": {
                "codeType": {
                  "value": "COUNTRY"
                },
                "q": {
                  "value": "{{query}}"
                }
              }
            },
            "searchTextParams": {
              "subqueries": {
                "codeType": {
                  "value": "country"
                },
                "q": {
                  "value": "{{query}}"
                }
              },
              "limit": 5
            },



            "item-text": "name",
            "item-value": "code"
          }
        },
        {
          "label": "Widget.zip",
          "name": "{{key}}PartyZip",
          "component": "v-text-field",
          "class": "xs-12 md-6",
          "readonlyClass": "hidden"
        }
      ]
    }
  }]
}