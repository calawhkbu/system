                  "value": "{{form.moduleTypeCode}}"
                },
                "q": {
                  "value": "{{query}}"
                }
              },
              "limit": 5
            },
            "item-text": "name",
            "item-value": "portCode"
          }
        },
        {
          "label": "BookingPage.placeOfDelivery",
          "name": "placeOfDelivery",
          "component": "AsyncAutoSuggest",
          "class": "xs-12 md-3",
          "more": [{
            "name": "placeOfDeliveryCode",
            "selector": "portCode"
          }],
          "props": {
            "method": "POST",
            "url": "api/location/query/location",
            "searchValueParams": {
              "subqueries": {
                "moduleType": {
                  "value": "{{form.moduleTypeCode}}"
                },
