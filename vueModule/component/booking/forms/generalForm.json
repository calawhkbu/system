                "q": {
                  "value": "{{query}}"
                }
              },
              "limit": 5
            },
            "item-text": "name",
            "item-value": "portCode"
          }
        },
        {
          "label": "BookingPage.carrier",
          "name": "carrier",
          "component": "AsyncAutoSuggest",
          "class": "xs-12 md-6",
          "relation": "carrier",
          "more": [{
            "name": "carrierCode",
            "selector": "code"
          }],
          "props": {
            "url": "api/code/query/code_master",
            "searchValueParams": {
              "subqueries": {
                "codeType": {
                  "value": "CARRIER"
                },
                "code": {
                  "value": "{{query}}"
                }
              }
            },
            "searchTextParams": {
              "subqueries": {
                "codeType": {
                  "value": "CARRIER"
                },
                "q": {
                  "value": "{{query}}"
                }
              },
              "limit": 5
            },
            "item-text": "name",
            "item-value": "code"
          }
        },
        {
          "label": "BookingPage.vesselName",
          "name": "vesselName",
          "component": "AsyncAutoSuggest",
          "class": "xs-12 md-3",
          "more": [{
            "name": "vesselName",
            "selector": "name"
          }],
          "checkDisplayWhen": "return form && form.moduleTypeCode && form.moduleTypeCode === 'SEA'",
          "props": {
            "url": "api/code/query/code_master",
            "searchValueParams": {
              "subqueries": {
                "codeType": {
                  "value": "VESSEL"
                },
                "code": {
                  "value": "{{query}}"
                }
              }
            },
            "searchTextParams": {
              "subqueries": {
