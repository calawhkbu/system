{
  "layout": "v-card",
  "components": [{
    "is": "WidgetForms",
    "props": {
      "formProps": {
        "class": "xs-12"
      },
      "keyProps": {
        "key": "Date",
        "fixKey": [
          "cargoReady",
          "cYCutOff",
          "pickup",
          "departure",
          "arrival",
          "finalDoorDelivery"
        ],
        "orderBy": {
          "cargoReady": 0,
          "cYCutOff": 10,
          "pickup": 20,
          "departure": 30,
          "arrival": 40,
          "finalDoorDelivery": 50
        }
      },
      "fields": [{
          "label": "Widget.estimatedDate",
          "name": "{{key}}DateEstimated",
          "component": "DateTimePicker",
          "class": "xs-12 md-5",
          "props": {
            "type": "datetime",
            "utc": true,
            "defaultFormat": "ddd MMM DD YYYY HH:mm:ss",
            "useOwnFormat": true
          }
        },
        {
          "label": "Widget.actualDate",
          "name": "{{key}}DateActual",
          "component": "DateTimePicker",
          "class": "xs-12 md-5",
          "props": {
            "type": "datetime",
            "utc": true,
            "defaultFormat": "ddd MMM DD YYYY HH:mm:ss",
            "useOwnFormat": true
          }
        },
        {
          "label": "Widget.remark",
          "name": "{{key}}Remark",
          "component": "v-text-field",
          "class": "xs-12 md-2"
        }
      ]
    }
  }]
}