{
  "layout": "v-card",
  "components": [{
    "is": "MouseHidden",
    "props": {
      "hiddenProps": {
        "style": {
          "position": "absolute",
          "right": "0px",
          "top": "0px"
        }
      }
    },
    "slots": [{
      "name": "hidden",
      "is": "DynamicComponent",
      "props": {
        "components": [{
          "is": "v-btn",
          "props": {
            "icon": true,
            "small": true,
            "class": "zindex-3 margin-0"
          },
          "slots": [{
            "is": "Icon",
            "props": {
              "icon": "edit"
            }
          }],
          "events": {
            "click.stop": [{
              "type": "dispatch",
              "otherParams": {
                "name": "widget/changeMode",
                "mode": "edit"
              },
              "afterActions": [{
                "type": "dispatch",
                "otherParams": {
                  "name": "widget/updateComponentSetting",
                  "value": {
                    "stepper": 0
                  }
                }
              }]
            }]
          }
        }, {
          "is": "RightDrawer",
          "props": {
            "hide-overlay": true,
            "buttonComponentProps": {
              "icon": true,
              "small": true,
              "class": "zindex-3 margin-0"
            }
          },
          "slots": [{
            "name": "button",
            "is": "Icon",
            "props": {
              "icon": "open_in_new"
            }
          }, {
            "name": "default",
            "is": "DynamicComponent",
            "props": {
              "components": [{
                "is": "WidgetForm",
                "props": {
                  "fields": [{
                      "label": "BookingPage.moduleType",
                      "name": ["moduleType.name", "moduleTypeCode"],
                      "component": "v-text-field",
                      "class": "xs-12",
                      "validator": ["required"],
                      "props": {
                        "required": true
                      }
                    },
                    {
                      "label": "BookingPage.boundType",
                      "name": ["boundType.name", "boundTypeCode"],
                      "component": "v-text-field",
                      "class": "xs-12",
                      "validator": ["required"],
                      "props": {
                        "required": true
                      }
                    },
                    {
                      "label": "BookingPage.serviceCode",
                      "name": ["service.name", "serviceCode"],
                      "component": "v-text-field",
                      "class": "xs-12",
                      "validator": ["required"],
                      "props": {
                        "required": true
                      }
                    },
                    {
                      "label": "BookingPage.vas",
                      "name": "vas",
                      "component": "AsyncSelect",
                      "class": "xs-`1`",
                      "props": {
                        "loadOnReadOnly": true,
                        "multiple": true,
                        "axiosParams": {
                          "url": "api/code/query/code_master",
                          "method": "POST",
                          "data": {
                            "subqueries": {
                              "codeType": {
                                "value": "VAS"
                              }
                            }
                          }
                        },
                        "item-text": "name",
                        "item-value": "code",
                        "showValue": false,
                        "showSpacer": false,
                        "selectedProps": {
                          "class": "multiple"
                        }
                      }
                    },
                    {
                      "label": "BookingPage.placeOfReceipt",
                      "name": ["placeOfReceiptName", "placeOfReceipt.name", "placeOfReceiptCode"],
                      "component": "v-text-field",
                      "class": "xs-12"
                    },
                    {
                      "label": "BookingPage.portOfLoading",
                      "name": ["portOfLoadingName", "portOfLoading.name", "portOfLoadingCode"],
                      "component": "v-text-field",
                      "class": "xs-12",
                      "validator": ["required"],
                      "props": {
                        "required": true
                      }
                    },
                    {
                      "label": "BookingPage.portOfDischarge",
                      "name": ["portOfDischargeName", "portOfDischarge.name", "portOfDischargeCode"],
                      "component": "v-text-field",
                      "class": "xs-12",
                      "validator": ["required"],
                      "props": {
                        "required": true
                      }
                    },
                    {
                      "label": "BookingPage.placeOfDelivery",
                      "name": ["placeOfDeliveryName", "placeOfDelivery.name", "placeOfDeliveryCode"],
                      "component": "v-text-field",
                      "class": "xs-12"
                    },
                    {
                      "label": "BookingPage.carrier",
                      "name": ["carrierName", "carrier.name", "carrierCode"],
                      "component": "v-text-field",
                      "class": "xs-12"
                    },
                    {
                      "label": "BookingPage.contractNo",
                      "name": "flexData.contractNo",
                      "component": "v-text-field",
                      "class": "xs-12"
                    },
                    {
                      "label": "BookingPage.vesselName",
                      "name": "proposedVesselName",
                      "component": "v-text-field",
                      "class": "xs-12",
                      "checkDisplayWhen": "return form && form.moduleTypeCode && form.moduleTypeCode === 'SEA'"
                    },
                    {
                      "label": "BookingPage.voyageFlightNumber",
                      "name": "proposedVoyageFlightNumber",
                      "component": "v-text-field",
                      "class": "xs-12",
                      "checkDisplayWhen": "return form && form.moduleTypeCode && (form.moduleTypeCode === 'AIR' || form.moduleTypeCode === 'SEA')"
                    },
                    {
                      "label": "BookingPage.incoTermsCode",
                      "name": ["incoTerms.name", "incoTermsCode"],
                      "component": "v-text-field",
                      "class": "xs-12"
                    },
                    {
                      "label": "BookingPage.freightTermsCode",
                      "name": ["freightTerms.name", "freightTermsCode"],
                      "component": "v-text-field",
                      "class": "xs-12"
                    },
                    {
                      "label": "BookingPage.otherTerms",
                      "name": ["otherTerms.name", "otherTermsCode"],
                      "component": "v-text-field",
                      "class": "xs-12"
                    },
                    {
                      "label": "BookingPage.commodity",
                      "name": "commodity",
                      "component": "v-text-field",
                      "class": "xs-12",
                      "validator": ["required"],
                      "props": {
                        "required": true
                      }
                    },
                    {
                      "label": "BookingPage.polHScode",
                      "name": "polHScode",
                      "component": "v-text-field",
                      "class": "xs-12",
                      "more": ["podHScode"]
                    }
                  ]
                }
              }]
            }
          }]
        }]
      }
    }, {
      "is": "DynamicComponent",
      "props": {
        "components": [{
          "is": "v-card-title",
          "props": {
            "class": "bold uppercase padding-0 min-height-28 padding-leftright-4"
          },
          "slots": [{
            "is": "I18nText",
            "props": {
              "i18nContext": "BookingPage",
              "i18nKey": "GeneralForm"
            }
          }]
        }, {
          "is": "v-divider"
        }, {
          "is": "WidgetForm",
          "props": {
            "class": "padding-top-5",
            "fields": [{
                "label": "BookingPage.moduleType",
                "name": ["moduleType.name", "moduleTypeCode"],
                "component": "v-text-field",
                "class": "xs-3",
                "validator": ["required"],
                "props": {
                  "required": true
                }
              },
              {
                "label": "BookingPage.boundType",
                "name": ["boundType.name", "boundTypeCode"],
                "component": "v-text-field",
                "class": "xs-3",
                "validator": ["required"],
                "props": {
                  "required": true
                }
              },
              {
                "label": "BookingPage.serviceCode",
                "name": ["service.name", "serviceCode"],
                "component": "v-text-field",
                "class": "xs-3",
                "validator": ["required"],
                "props": {
                  "required": true
                }
              },
              {
                "label": "BookingPage.vas",
                "name": "vas",
                "checkDisplayWhen": "return form && form.vas && form.vas.length",
                "component": "AsyncSelect",
                "class": "xs-3",
                "props": {
                  "loadOnReadOnly": true,
                  "multiple": true,
                  "axiosParams": {
                    "url": "api/code/query/code_master",
                    "method": "POST",
                    "data": {
                      "subqueries": {
                        "codeType": {
                          "value": "VAS"
                        }
                      }
                    }
                  },
                  "item-text": "name",
                  "item-value": "code",
                  "showValue": false,
                  "showSpacer": false,
                  "selectedProps": {
                    "class": "multiple"
                  }
                }
              },
              {
                "label": "BookingPage.placeOfReceipt",
                "checkDisplayWhen": "return form && (form.placeOfReceiptName || (form.placeOfReceipt && form.placeOfReceipt.name) || form.placeOfReceiptCode)",
                "name": ["placeOfReceiptName", "placeOfReceipt.name", "placeOfReceiptCode"],
                "component": "v-text-field",
                "class": "xs-3"
              },
              {
                "label": "BookingPage.portOfLoading",
                "name": "portOfLoading",
                "name": ["portOfLoadingName", "portOfLoading.name", "portOfLoadingCode"],
                "component": "v-text-field",
                "class": "xs-3",
                "validator": ["required"],
                "props": {
                  "required": true
                }
              },
              {
                "label": "BookingPage.portOfDischarge",
                "name": ["portOfDischargeName", "portOfDischarge.name", "portOfDischargeCode"],
                "component": "v-text-field",
                "class": "xs-3",
                "validator": ["required"],
                "props": {
                  "required": true
                }
              },
              {
                "label": "BookingPage.placeOfDelivery",
                "checkDisplayWhen": "return form && (form.placeOfDeliveryName || (form.placeOfDelivery && form.placeOfDelivery.name) || form.placeOfDeliveryCode)",
                "name": ["placeOfDeliveryName", "placeOfDelivery.name", "placeOfDeliveryCode"],
                "component": "v-text-field",
                "class": "xs-3"
              },
              {
                "label": "BookingPage.carrier",
                "name": ["carrierName", "carrier.name", "carrierCode"],
                "component": "v-text-field",
                "checkDisplayWhen": "return form && (form.carrierName || (form.carrier && form.carrier.name) || form.carrierCode)",
                "class": "xs-3"
              },
              {
                "label": "BookingPage.contractNo",
                "name": "flexData.contractNo",
                "component": "v-text-field",
                "class": "xs-3",
                "checkDisplayWhen": "return form && form.flexData && form.flexData.contractNo"
              },
              {
                "label": "BookingPage.vesselName",
                "name": "proposedVesselName",
                "component": "v-text-field",
                "class": "xs-3",
                "checkDisplayWhen": "return form && form.moduleTypeCode && form.moduleTypeCode === 'SEA' && form.proposedVesselName"
              },
              {
                "label": "BookingPage.voyageFlightNumber",
                "name": "proposedVoyageFlightNumber",
                "component": "v-text-field",
                "class": "xs-3",
                "checkDisplayWhen": "return form && form.moduleTypeCode && (form.moduleTypeCode === 'AIR' || form.moduleTypeCode === 'SEA') && form.proposedVoyageFlightNumber"
              },
              {
                "label": "BookingPage.incoTermsCode",
                "name": "incoTermsCode",
                "component": "v-text-field",
                "class": "xs-3",
                "checkDisplayWhen": "return form && ((form.incoTerms && form.incoTerms.name) || form.incoTermsCode)"
              },
              {
                "label": "BookingPage.freightTermsCode",
                "name": "freightTermsCode",
                "component": "v-text-field",
                "class": "xs-3",
                "checkDisplayWhen": "return form && ((form.freightTerms && form.freightTerms.name) || form.freightTermsCode)"
              },
              {
                "label": "BookingPage.otherTerms",
                "name": "otherTermsCode",
                "component": "v-text-field",
                "class": "xs-3",
                "checkDisplayWhen": "return form && ((form.otherTerms && form.otherTerms.name) || form.otherTermsCode)"
              },
              {
                "label": "BookingPage.commodity",
                "name": "commodity",
                "component": "v-text-field",
                "class": "xs-3",
                "validator": ["required"],
                "props": {
                  "required": true
                }
              },
              {
                "label": "BookingPage.polHScode",
                "name": "polHScode",
                "component": "v-text-field",
                "class": "xs-3",
                "checkDisplayWhen": "return form && form.polHScode",
                "more": ["podHScode"]
              }, {
                "class": "xs-3"
              }, {
                "class": "xs-3"
              }, {
                "class": "xs-3"
              }
            ]
          }
        }]
      }
    }]
  }]
}
