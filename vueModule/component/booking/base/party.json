{
  "layout": "v-card",
  "class": "min-height-263",
  "components": [{
    "is": "MouseHidden",
    "props": {
      "hiddenProps": {
        "style": {
          "position": "absolute",
          "right": "0px",
          "top": "0px"
        }
      }
    },
    "slots": [{
      "name": "hidden",
      "is": "DynamicComponent",
      "props": {
        "components": [{
          "is": "v-btn",
          "props": {
            "icon": true,
            "small": true,
            "class": "zindex-3 margin-0"
          },
          "slots": [{
            "is": "Icon",
            "props": {
              "icon": "edit"
            }
          }],
          "events": {
            "click.stop": [{
              "type": "dispatch",
              "otherParams": {
                "name": "widget/changeMode",
                "mode": "edit"
              },
              "afterActions": [{
                "type": "dispatch",
                "otherParams": {
                  "name": "widget/updateComponentSetting",
                  "value": {
                    "stepper": 1
                  }
                }
              }]
            }]
          }
        }, {
          "is": "RightDrawer",
          "props": {
            "hide-overlay": true,
            "buttonComponentProps": {
              "icon": true,
              "small": true,
              "class": "zindex-3 margin-0"
            }
          },
          "slots": [{
            "name": "button",
            "is": "Icon",
            "props": {
              "icon": "open_in_new"
            }
          }, {
            "name": "default",
            "is": "WidgetMoreForm",
            "props": {
              "name": "bookingParty",
              "mainKey": "Party",
              "formProps": {
                "z": {
                  "class": "xs-12 padding-leftright-1"
                }
              },
              "labelProps": {
                "z": {
                  "isComponent": "v-label",
                  "i18nContext": "Widget",
                  "class": "font-90"
                },
                "shipper": {
                  "isComponent": "v-label",
                  "i18nContext": "Widget",
                  "class": "font-90 required"
                },
                "consignee": {
                  "isComponent": "v-label",
                  "i18nContext": "Widget",
                  "class": "font-90 required"
                }
              },
              "fieldProps": {
                "z": {
                  "thirdPartyCode.erp": "{{thirdPartyCode.erp}}",
                  "id": "{{id}}",
                  "name": "{{name}}",
                  "address": "{{address}}",
                  "cityCode": "{{cityCode}}",
                  "countryCode": "{{countryCode}}",
                  "zip": "{{zip}}",
                  "shortName": "{{shortName}}",
                  "fixValue": "{{fixValue}}"
                }
              },
              "fixedKeys": [
                "shipper",
                "consignee",
                "agent",
                "forwarder",
                "roAgent",
                "linerAgent",
                "controllingCustomer",
                "notifyParty"
              ],
              "fields": [{
                  "name": ["bookingParty.{{flex}}{{key}}PartyName", "bookingParty.{{flex}}{{key}}Party.name"],
                  "component": "v-text-field",
                  "class": "xs-12"
                },
                {
                  "label": "Widget.contactName",
                  "name": "bookingParty.{{flex}}{{key}}PartyContactName",
                  "component": "v-text-field",
                  "class": "xs-12",
                  "checkDisplayWhen": "return form && form.bookingParty && form.bookingParty.{{flex}}{{key}}PartyContactName"
                },
                {
                  "label": "Widget.contactEmail",
                  "name": "bookingParty.{{flex}}{{key}}PartyContactEmail",
                  "component": "v-text-field",
                  "class": "xs-10",
                  "checkDisplayWhen": "return form && form.bookingParty && form.bookingParty.{{flex}}{{key}}PartyContactEmail"
                },
                {
                  "label": "Widget.inviteEmail",
                  "name": "bookingParty.{{flex}}{{key}}PartyContactEmail",
                  "component": "button-field",
                  "readonlyClass": "xs-2",
                  "checkDisplayWhen": "return form && form.bookingParty && form.bookingParty.{{flex}}{{key}}PartyContactEmail",
                  "slots": [{
                    "is": "Icon",
                    "props": {
                      "icon": "email"
                    }
                  }],
                  "props": {
                    "flat": true,
                    "icon": true,
                    "events": {
                      "click.stop": [{
                        "type": "axios",
                        "otherParams": {
                          "axiosParams": {
                            "url": "/api/invitation/invite/invite",
                            "method": "POST",
                            "data": {
                              "entityId": "{{context.$store.state.widget.id}}",
                              "tableName": "shipment",
                              "partyId": ["{{ '`${args[0]}${args[1]}PartyId`'  | calculate(flex,key) | getFrom(context.$store.state.widget.data)}}"],
                              "partyType": ["{{key}}"],
                              "person": {
                                "userName": "{{ context.fixValue }}"
                              }
                            }
                          }
                        },
                        "afterActions": [{
                          "type": "notify",
                          "otherParams": {
                            "notification": "invite success"
                          }
                        }]
                      }]
                    }
                  }
                },
                {
                  "label": "Widget.contactPhone",
                  "name": "bookingParty.{{flex}}{{key}}PartyContactPhone",
                  "component": "v-text-field",
                  "class": "xs-12",
                  "checkDisplayWhen": "return form && form.bookingParty && form.bookingParty.{{flex}}{{key}}PartyContactPhone"
                },
                {
                  "name": "bookingParty.{{flex}}{{key}}PartyContacts",
                  "component": "TableField",
                  "class": "xs-12",
                  "props": {
                    "overrideRowIndex": 1,
                    "fields": [{
                        "label": "Widget.contactXName",
                        "name": "Name",
                        "component": "v-text-field",
                        "class": "xs-12"
                      },
                      {
                        "label": "Widget.contactXEmail",
                        "name": "Email",
                        "component": "v-text-field",
                        "class": "xs-10"
                      },
                      {
                        "label": "Widget.inviteEmail",
                        "checkDisplayWhen": "return form.Email && mode.includes('detail')",
                        "name": "Email",
                        "component": "button-field",
                        "readonlyClass": "xs-2",
                        "slots": [{
                          "is": "Icon",
                          "props": {
                            "icon": "email"
                          }
                        }],
                        "props": {
                          "flat": true,
                          "icon": true,
                          "events": {
                            "click.stop": [{
                              "type": "axios",
                              "otherParams": {
                                "axiosParams": {
                                  "url": "/api/invitation/invite/invite",
                                  "method": "POST",
                                  "data": {
                                    "entityId": "{{context.$store.state.widget.id}}",
                                    "tableName": "shipment",
                                    "partyId": ["{{ '`${args[0]}${args[1]}PartyId`'  | calculate(flex,key) | getFrom(context.$store.state.widget.data)}}"],
                                    "partyType": ["{{key}}"],
                                    "person": {
                                      "userName": "{{ context.fixValue }}"
                                    }
                                  }
                                }
                              },
                              "afterActions": [{
                                "type": "notify",
                                "otherParams": {
                                  "notification": "invite success"
                                }
                              }]
                            }]
                          }
                        }
                      },
                      {
                        "label": "Widget.contactXPhone",
                        "name": "Phone",
                        "component": "v-text-field",
                        "class": "xs-12"
                      }
                    ]
                  }
                },
                {
                  "label": "Widget.address",
                  "name": ["bookingParty.{{flex}}{{key}}PartyAddress", "bookingParty.{{flex}}{{key}}Party.address"],
                  "component": "v-textarea",
                  "props": {
                    "auto-grow": true,
                    "rows": 5
                  },
                  "class": "xs-12"
                },
                {
                  "label": "Widget.city",
                  "name": ["bookingParty.{{flex}}{{key}}PartyCityCode", "bookingParty.{{flex}}{{key}}Party.cityCode"],
                  "component": "v-text-field",
                  "class": "xs-12"
                },
                {
                  "label": "Widget.state",
                  "name": ["bookingParty.{{flex}}{{key}}PartyStateCode", "bookingParty.{{flex}}{{key}}Party.stateCode"],
                  "component": "v-text-field",
                  "class": "xs-12"
                },
                {
                  "label": "Widget.country",
                  "name": ["bookingParty.{{flex}}{{key}}PartyCountryCode", "bookingParty.{{flex}}{{key}}Party.countryCode"],
                  "component": "v-text-field",
                  "class": "xs-12"
                },
                {
                  "label": "Widget.zip",
                  "name": ["bookingParty.{{flex}}{{key}}PartyZip", "bookingParty.{{flex}}{{key}}Party.zip"],
                  "component": "v-text-field",
                  "class": "xs-12"
                }
              ]
            }
          }]
        }]
      }
    }, {
      "is": "DynamicComponent",
      "props": {
        "components": [{
            "is": "v-card-title",
            "props": {
              "class": "bold uppercase padding-0 min-height-28 padding-leftright-4"
            },
            "slots": [{
              "is": "I18nText",
              "props": {
                "i18nContext": "BookingPage",
                "i18nKey": "PartyForm"
              }
            }]
          }, {
            "is": "v-divider"
          },
          {
            "is": "WidgetMoreForm",
            "props": {
              "name": "bookingParty",
              "mainKey": "Party",
              "formProps": {
                "z": {
                  "class": "xs-12 md-6 padding-leftright-1"
                }
              },
              "extraGridProps": {
                "class": "xs-12 md-6 padding-leftright-1"
              },
              "labelProps": {
                "z": {
                  "isComponent": "v-label",
                  "i18nContext": "Widget",
                  "class": "font-90"
                },
                "shipper": {
                  "isComponent": "v-label",
                  "i18nContext": "Widget",
                  "class": "font-90 required"
                },
                "consignee": {
                  "isComponent": "v-label",
                  "i18nContext": "Widget",
                  "class": "font-90 required"
                }
              },
              "fieldProps": {
                "z": {
                  "thirdPartyCode.erp": "{{thirdPartyCode.erp}}",
                  "id": "{{id}}",
                  "name": "{{name}}",
                  "address": "{{address}}",
                  "cityCode": "{{cityCode}}",
                  "countryCode": "{{countryCode}}",
                  "zip": "{{zip}}",
                  "shortName": "{{shortName}}",
                  "fixValue": "{{fixValue}}"
                }
              },
              "fixedKeys": [
                "shipper",
                "consignee",
                "agent",
                "forwarder",
                "roAgent",
                "linerAgent",
                "controllingCustomer",
                "notifyParty"
              ],
              "checkDisplayWhen": [
                "return form && (form[`${key}Party`] || form[`${key}PartyName`])"
              ],
              "fields": [{
                "name": ["bookingParty.{{flex}}{{key}}Party.name", "bookingParty.{{flex}}{{key}}PartyName"],
                "component": "v-text-field",
                "class": "xs-12"
              }]
            }
          }
        ]
      }
    }]
  }]
}