{
  "components": [{
      "is": "DynamicComponent",
      "props": {
        "layout": "v-card",
        "class": "absolute-topleft-20 zindex-300",
        "components": [{
          "is": "v-btn",
          "props": {
            "icon": true
          },
          "events": {
            "click.stop": [{
              "type": "dispatch",
              "otherParams": {
                "name": "widget/changeMode",
                "mode": "detail"
              }
            }]
          },
          "slots": [{
            "is": "Icon",
            "props": {
              "icon": "arrow_back"
            }
          }]
        }]
      }
    },
    {
      "is": "DynamicComponent",
      "props": {
        "layout": "v-card",
        "class": "absolute-topright-20 zindex-250",
        "components": [{
          "is": "ClickHidden",
          "props": {
            "desktopOpen": true
          },
          "slots": [{
            "is": "DynamicComponent",
            "props": {
              "style": {
                "height": "500px",
                "max-height": "75%",
                "overflow": "auto"
              },
              "components": [{
                "is": "WidgetForm",
                "props": {
                  "style": {
                    "max-width": "400px"
                  },
                  "fields": []
                }
              }, {
                "is": "SmallTracking",
                "props": {
                  "axiosBaseParams": [
                    "const data = context.$store.state.widget.data;",
                    "return {",
                    "url: `api/tracking/getold/${data.masterNo}`,",
                    "method: 'get'",
                    "}"
                  ],
                  "getModule": "moduleType",
                  "icon": "statusCode",
                  "titleKey": {
                    "key": "key",
                    "track": true
                  },
                  "subTitleKeys": [
                    "statusDescription",
                    "statusPlace",
                    {
                      "key": "statusDate",
                      "is": "Datetime",
                      "format": "YYYY-MM-DD HH:mm:ss"
                    }
                  ],
                  "iconMap": {
                    "BKCF": "tracking/bkcf",
                    "STSP": "tracking/stsp",
                    "EPRL": "tracking/eprl",
                    "GITM": "tracking/cy-cutoff",
                    "LOBD": "tracking/lobd",
                    "DLPT": "tracking/dlpt",
                    "TSLB": "tracking/lobd",
                    "TSBA": "tracking/cy-cutoff",
                    "BDAR": "tracking/dlpt",
                    "DSCH": "tracking/lobd",
                    "DECL": "tracking/sumit-so-date",
                    "PASS": "tracking/cus",
                    "TMPS": "tracking/sale-invoice-date",
                    "STCS": "tracking/stcs",
                    "RCVE": "tracking/eprl",
                    "BKD": "tracking/bkcf",
                    "RCS": "tracking/rcs",
                    "MNF": "tracking/eprl",
                    "DEP": "tracking/airplane-takeoff",
                    "TAR": "tracking/airplane-landing",
                    "TDE": "tracking/airplane-takeoff",
                    "ARR": "tracking/airplane-landing",
                    "RWB": "tracking/sale-invoice-date",
                    "RCF": "tracking/rcf",
                    "CUS": "tracking/cus",
                    "NFD": "tracking/nfd",
                    "MAN": "tracking/man",
                    "DLV": "tracking/stsp",
                    "CANF": "tracking/canf",
                    "ERR": "tracking/err",
                    "bookingReceivedByForwarder": "tracking/paperwork",
                    "oceanBill": "tracking/sumit-so-date",
                    "SEAdeparture": "tracking/ship-departure",
                    "SEAarrival": "tracking/ship-arrival",
                    "AIRdeparture": "tracking/air-departure",
                    "AIRarrival": "tracking/air-arrival",
                    "cargoReady": "tracking/cargo-ready",
                    "cyCutoff": "tracking/cy-cutoff",
                    "pickup": "tracking/pickup",
                    "delivery": "tracking/final-door-delivery",
                    "finalDoorDelivery": "tracking/final-door-delivery"
                  }
                }
              }]
            }
          }]
        }]
      }
    },
    {
      "is": "Map",
      "slots": [{
        "is": "Route",
        "props": {
          "startPoint": "widget.data.portOfLoadingCode",
          "endPoint": "widget.data.portOfDischargeCode",
          "estimatedStartTime": "widget.data.departureDateEstimated",
          "actualStartTime": "widget.data.departureDateActual",
          "estimatedEndTime": "widget.data.arrivalDateEstimated",
          "actualEndTime": "widget.data.arrivalDateActual",
          "mode": "widget.data.moduleTypeCode"
        }
      }]
    }
  ]
}