{
  "components": [
    {
      "is": "v-card",
      "props": {
        "style": {
          "margin-bottom": "20px"
        }
      },
      "slots": [
        {
          "name": "default",
          "is": "DynamicComponent",
          "props": {
            "components": [
              {
                "is": "v-card-title",
                "props": {
                  "primary-title": true
                },
                "slots": [
                  {
                    "name": "default",
                    "is": "I18nText",
                    "props": {
                      "class": "title",
                      "i18nKey": "UserProfilePage.NotiicationPreferences"
                    }
                  }
                ]
              },
              {
                "is": "SimpleForm",
                "props": {
                  "form": "$store.state.auth.userProfile",
                  "submitActions": [
                    {
                      "type": "dispatch",
                      "otherParams": {
                        "name": "auth/submitUserProfile"
                      },
                      "errorMessage": "UserProfilePage.UpdateUserProfileFail",
                      "afterActions": [
                        {
                          "type": "notify",
                          "otherParams": {
                            "notification": "UserProfilePage.UpdateUserProfileSuccess"
                          }
                        }
                      ]
                    }
                  ],
                  "fields": []
                }
              }
            ]
          }
        }
      ]
    }
  ]
}
